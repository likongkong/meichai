.afoHead{
  width: 100%;
  padding-top: 30rpx;
  height: 100rpx;
  margin: 0 auto;
  border-radius: 10rpx;
  background: #ffffff;
  display: flex;
  justify-content: space-around;
}
.bdaiBoxImg{
  width:34rpx;
  height: 34rpx;
  margin:0 auto 10rpx;
  border-radius: 50%;
  border: 2rpx solid #B9B9B9;
  color: #B9B9B9;
  font-size: 24rpx;
  text-align: center;
  line-height: 34rpx;
  position: relative;
  background: #fff;
}
.sel .bdaiBoxImg{
  border: 2rpx solid #101010;
  color: #101010;
}
.bdaiBoxTxt{
  width: 100%;
  height: 40rpx;
  line-height: 40rpx;
  font-size:22rpx;
  text-align: center;
  color: #B9B9B9;
}
.sel .bdaiBoxTxt{
  color: #101010;
}
.markingLine{
  height: 4rpx;
  width: 100rpx;
  position: absolute;
  left: 60rpx;
  top: 0;
  bottom: 0;
  margin: auto;
  background:#B9B9B9;
}
.speedOfProgress{
  height: 100%;
  background: #101010;
}