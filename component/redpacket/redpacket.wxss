@import "/pages/smokebox/smokebox.wxss";

.redpackettu{
  position: relative;
}
.redpackettu text{
  position: absolute;
  top: 0;
  right: 0;
  width: 30rpx;
  height: 30rpx;
  text-align: center;
  font-size: 20rpx;
  line-height: 30rpx;
  background: #ff2742;
  border: 1rpx solid #fff;
  color: #fff;
  border-radius: 30rpx;
}

.snapshotshareclass {
  width: 300px;
  height: 240px;
  position: fixed;
  top: -9999px;
  left: 0;
}