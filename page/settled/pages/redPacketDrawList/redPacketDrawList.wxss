page{
  padding-bottom: 40rpx;
  background: #fff;
}
.green{
  color: #50B74F;
}
.black{
  color: #101010 !important;
}
.title{
  width: 100%;
  height: 68rpx;
  border-bottom: 2rpx solid #F5F5F5;
  font-size: 28rpx;
  color: #101010;
  line-height: 68rpx;
  padding: 0 32rpx;
  box-sizing: border-box;
}
.item{
  padding: 22rpx 0 34rpx 32rpx;
  box-sizing: border-box;
  border-bottom: 2rpx solid #F5F5F5;
}
.item:last-child{
  border-bottom: none;
}
.itemTitle{
  font-size: 28rpx;
  color: #101010;
}
.itemTitle text{
  font-size: 28rpx;
}
scroll-view{
  width: 100%;
  white-space:nowrap;
}
.itemPic{
  width: 200rpx;
  height: 200rpx;
  margin-top: 16rpx;
  margin-right: 20rpx;
  display:inline-block
}
.itemPic image{
  width: 200rpx;
  height: 200rpx;
  display: block;
}
