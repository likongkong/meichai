@import "/common/tabbar.wxss";
@import "/common/citysel.wxss";
@import "../timHomePageList/timHomePageList.wxss";

page{
  /* background:#f4f5f9; */
  background:#fff;
}
.omBox{
  width: 100%;
  padding: 20rpx 0 140rpx 0;
  font-size:28rpx;
}
.marginAuto{
  width: 610rpx;
  margin:0 20rpx 10rpx 20rpx;
}
.scrollViewTop{
  height: 60rpx;
  white-space: nowrap;
}
.obcrTopTopSnEve{
  height: 46rpx;
  line-height: 46rpx;
  position: relative;
  padding-right: 70rpx;
}
.obcrTopTopSn{
  color:#90D2D6;padding-bottom:16rpx;margin-bottom:16rpx;border-bottom:2rpx solid #f5f5f5;
}
.elsearch{
  height:60rpx;
  position: relative;
  width: 710rpx;
  margin:20rpx auto 20rpx;
}
.elsearchch{
  padding-left: 60rpx;
  width: 530rpx;
  height: 54rpx;
  background: #F5F5F5;
  border-radius: 54rpx;
}
.elserarchimg{
  width: 30rpx;
  height: 30rpx;
  position: absolute;
  left: 20rpx;
  top: 0;
  bottom: 0;
  margin: auto;
}
.elserbotom{
  position: absolute;
  top: 0;
  bottom: 0;
  right: 0;
  width: 90rpx;
  height: 58rpx;
  line-height: 58rpx;
  text-align: center;
  color: #5CA7FE;
  margin: auto;
}
.elinputbut{
  height: 100%;
  width: 500rpx;
  float: left;
}

.classifyItem{
  display: inline-block;
  padding: 0 20rpx;
  line-height: 80rpx;
  color: #000;
  box-sizing: border-box;
  position: relative;
}
.active{
  position: relative;
}
.active::before{
  content: '';
  position: absolute;
  bottom: 10rpx;
  left: 50%;
  width: 80%;
  height: 4rpx;
  background: #000;
  transform: translate(-50%, 0);  
}
.centerTxt{
  background: #fff;
  border-radius: 15rpx;
  width: 100%;
}
.orderBox{
  padding:25rpx 30rpx 0;
  width: 690rpx;
  border-top: 15rpx solid #f5f5f5;
}
.copyCart{
  position: absolute;
  top: 0;
  right: 0;
  bottom: 0;
  margin: auto;
  background:#F4F4F4;
  font-size: 24rpx;
  color: #000;
  height: 40rpx;
  line-height: 40rpx;
  width: 60rpx;
  text-align: center;
  border-radius:6rpx;
}
.orderBoxTop{
  width: 100%;
  height: 80rpx;
  line-height: 80rpx;
  display: flex;
  justify-content:space-between;
}
.userImgName{
  height: 80rpx;
  line-height: 80rpx;
}
.userImg{
  width: 80rpx;
  height: 80rpx;
  border-radius: 100%;
  display:inline-block;
  vertical-align: top;
}
.userName{
  display: inline-block;
  height: 80rpx;
  line-height: 80rpx;
  vertical-align: top;
  margin-left: 20rpx;
  width: 240rpx;
}
.orderBoxContent{
  width: 100%;
  height: 160rpx;
}
.orderBoxContentImg{
  width: 140rpx;
  height: 140rpx;
  float: left;
  border-radius: 15rpx;
}
.orderBoxContentRight{
  width: 500rpx;
  float: right;
  height: 140rpx;
  position: relative;
}
.obcrTop{
  display: flex;
  justify-content:space-between;
}
.obcrBottom{
  width: 100%;
  position: absolute;
  left: 0;
  bottom: 0;
}
.orderAddress{
  width: 100%;
  padding-top: 20rpx;
  overflow: hidden;
}
.orderAddressEve{
  margin-bottom: 20rpx;
  display:flex;
  justify-content: space-between;
}
.orderBottom{
  border-top: 2rpx solid #ECECEC;
  width: 100%;
  padding-top: 20rpx;
}
.orderBottomOrderNumber{
  width: 100%;
  height: 80rpx;
  text-align: right;
  position: relative;
}
.orderBottomButtonLeft{
  height: 58rpx;
  line-height: 58rpx;
  position: absolute;
  top: 0;
  left: 0;
  font-size: 24rpx;
  color: #909090;
  padding-right: 40rpx;
}
.orderBottomImgG{
  width: 30rpx;
  height: 30rpx;
  position: absolute;
  top: 0;
  right: 0;
  bottom: 0;
  margin: auto;
}
.orderBottomButton{
  width: 160rpx;
  height: 54rpx;
  line-height: 54rpx;
  text-align: center;
  color: #FE5D57;
  border-radius: 54rpx;
  display: inline-block;
  margin-left: 25rpx;
  vertical-align: top;
  position: relative;
  border: 2rpx solid #FE5D57;
}
.cmScreen{
  height: 80rpx;
  padding-left: 50rpx;
  line-height: 80rpx;
  color: #90D2D6;
  position: absolute;
  top: 0;
  right: 17rpx;
}
.cmScreen image{
  position: absolute;
  top: 0;
  bottom: 0;
  left: 0;
  margin: auto;
  height: 50rpx;
  width: 50rpx;
}
.scrollBox{
  width: 100%;
  position: relative;
  border-bottom: 2rpx solid #f5f5f5;
}
.wdmbchinfnum {
  position: absolute;
  top: 0;
  right: 0;
  height: 30rpx;
  width: 30rpx;
  border: 2rpx solid #ff2742;
  border-radius: 50%;
  z-index: 9;
  font-size: 20rpx;
  color: #ff2742;
  text-align: center;
  line-height: 30rpx;
  background: #fff;
}


