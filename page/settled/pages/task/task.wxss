page{
  background:#f4f5f9;
  padding-bottom: 200rpx;
}
.wrap{
  padding: 20rpx 26rpx;
  box-sizing: border-box;
}
.card{
  width: 100%;
  padding: 30rpx 24rpx;
  box-sizing: border-box;
  background: #fff;
  border-radius: 14rpx;
}
.subsidy{
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.subsidy text:first-child{
  font-size: 30rpx;
  color: #101010;
  font-weight: bold;
}
.subsidy text:last-child{
  display: inline-block;
  height: 56rpx;
  background: #9DD7DB;
  padding: 0 14rpx;
  box-sizing: border-box;
  line-height: 56rpx;
  font-size: 24rpx;
  color: #fff;
  border-radius: 6rpx;
}
.navmenu{
 width: 100%;
 height: 94rpx;
}
.navmenu text{
  display: inline-block;
  height: 94rpx;
  line-height: 94rpx;
  font-size: 32rpx;
  color: #7F838F;
  margin-right: 62rpx;
  font-weight: bold;
}
.navmenu text:last-child{
  margin-right: 0;
}
.active{
  color: #000000 !important;
}
.head{
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.head>view:first-child{
  width: 80%;
}
.taskWrap{
  display: flex;
  align-items: center;
}
.taskWrap text:first-child{
  font-size: 32rpx;
  font-weight: bold;
  color: #000000;
  margin-right: 16rpx;
}
.taskWrap text:last-child{
  border-left: 2rpx solid #999999;
  height: 30rpx;
  line-height: 30rpx;
  display: inline-block;
  font-size: 24rpx;
  color: #999999;
  padding-left: 8rpx;
}
.progressWrap{
  display: flex;
  align-items: center;
}
.progress-box{
  width: 85%;
  margin-top: 4rpx;
}
.progress-num{
  width: 15%;
  display: inline-block;
  font-size: 24rpx;
  color:#999999;
  padding-left: 8rpx;
  box-sizing: border-box;
}
.head>view:last-child{
  width: 20%;
  text-align: center;
}
.icon{
  width: 70rpx;
  height: auto;
}
.txt1{
  display: block;
  font-size: 24rpx;
  color: #888888;
  margin-bottom: 6rpx;
}
.txt2{
  display: block;
  font-size: 24rpx;
  color: #FD8484;
}
.taskItem{
  border-bottom: 2rpx solid #EEEEEE;
  padding: 20rpx 0;
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.taskItem:last-child{
  border-bottom: none;
}
.taskItemContent{
  width: 70%;
}
.taskItemContent text{
  font-size: 28rpx;
  display: block;
}
.taskItemContent text:first-child{
  color: #101010;
}
.taskItemContent text:last-child{
  color: #EB9204;
  margin-top: 8rpx;
}
.taskItemBtn{
  width: 30%;
  display: flex;
  justify-content: flex-end;
}
.taskItemBtn text{
  display: block;
  width: 148rpx;
  height: 52rpx;
  border-radius: 52rpx;
  font-size: 28rpx;
  line-height: 52rpx;
  text-align: center;
}
.taskItemBtn1{
  border: 2rpx solid #9DD7DB;
  color: #9DD7DB;
}
.taskItemBtn2{
  background: #9DD7DB;
  color: #ffffff;
}
.taskItemBtn3{
  background: #BFBFBF;
  color: #ffffff;
}
.taskItemBtn4{
  border: 2rpx solid #BFBFBF;
  color: #BFBFBF;
}