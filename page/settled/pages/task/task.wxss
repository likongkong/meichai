page{
  background:#f4f5f9;
  /* padding-bottom: 200rpx; */
}
.wrap{
  padding: 20rpx 26rpx 0 26rpx;
  box-sizing: border-box;
}
.taskexplain{
  position: fixed;
  top: 412rpx;
  right:0;
  background: #9DD7DB;
  z-index: 15;
  border-top-left-radius: 70rpx;
  border-bottom-left-radius: 70rpx;
  padding: 4rpx 6rpx 4rpx 24rpx;
}
.taskexplain text{
  display: block;
  color: #FFFFFF;
  font-size: 24rpx;
  text-align: center;
}
.header{
  width: 100%;
  /* position: fixed;
  left: 0; */
  padding: 20rpx 26rpx 0 26rpx;
  box-sizing: border-box;
  background:#f4f5f9;
  /* z-index: 9; */
}
.content{
  margin-top: 0;
}
.card{
  width: 100%;
  padding: 30rpx 24rpx 30rpx;
  box-sizing: border-box;
  background: #fff;
  border-radius: 14rpx;
}
.content .card{
  padding: 14rpx 24rpx 30rpx;
  box-sizing: border-box;
  margin-bottom: 30rpx;
}
.content .card:last-child{
  margin-bottom: 0;
}
.subsidy{
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.subsidy text:first-child{
  font-size: 30rpx;
  color: #101010;
  font-weight: bold;
}
.subsidy text:last-child{
  display: inline-block;
  height: 56rpx;
  background: #9DD7DB;
  padding: 0 14rpx;
  box-sizing: border-box;
  line-height: 56rpx;
  font-size: 24rpx;
  color: #fff;
  border-radius: 6rpx;
}
.navmenu{
 width: 100%;
 height: 94rpx;
}
.navmenu text{
  display: inline-block;
  height: 94rpx;
  line-height: 94rpx;
  font-size: 32rpx;
  color: #7F838F;
  margin-right: 62rpx;
  font-weight: bold;
}
.navmenu text:last-child{
  margin-right: 0;
}
.active{
  color: #000000 !important;
}
.head{
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.head>view:first-child{
  width: 80%;
}
.taskWrap{
  display: flex;
  align-items: center;
}
.taskWrap .tasktitle{
  font-size: 32rpx;
  font-weight: bold;
  color: #000000;
  margin-right: 16rpx;
}
.taskWrap .taskendTime{
  border-left: 2rpx solid #999999;
  height: 30rpx;
  line-height: 30rpx;
  display: inline-block;
  font-size: 24rpx;
  color: #999999;
  padding-left: 8rpx;
}
.progressWrap{
  display: flex;
  align-items: center;
}
.progress-box{
  width: 85%;
  margin-top: 4rpx;
}
.progress-num{
  width: 15%;
  display: inline-block;
  font-size: 24rpx;
  color:#999999;
  padding-left: 8rpx;
  box-sizing: border-box;
}
.head>view:last-child{
  width: 20%;
  text-align: center;
  position: relative;
}
.awardBox{
  width: 100%;
  height: 100%;
  position: absolute;
  top: 0;
  left: 0;
  z-index: 5;
}
.icon{
  width: 70rpx;
  height: auto;
}
.txt1{
  display: block;
  font-size: 24rpx;
  color: #888888;
  margin-bottom: 6rpx;
}
.txt2{
  display: block;
  font-size: 24rpx;
  color: #FD8484;
}
.taskItem{
  border-bottom: 2rpx solid #EEEEEE;
  padding: 20rpx 0;
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.taskItem:last-child{
  border-bottom: none;
}
.taskItemContent{
  width: 70%;
}
.taskItemContent text{
  font-size: 28rpx;
  display: block;
}
.taskItemContent text:first-child{
  color: #101010;
}
.taskItemContent text:last-child{
  color: #EB9204;
  margin-top: 8rpx;
}
.taskItemBtn{
  width: 30%;
  display: flex;
  justify-content: flex-end;
}
.taskItemBtn text{
  display: block;
  width: 148rpx;
  height: 52rpx;
  border-radius: 52rpx;
  font-size: 28rpx;
  line-height: 52rpx;
  text-align: center;
}
.taskItemBtn1{
  border: 2rpx solid #9DD7DB;
  color: #9DD7DB;
}
.taskItemBtn2{
  background: #9DD7DB;
  color: #ffffff;
}
.taskItemBtn3{
  background: #BFBFBF;
  color: #ffffff;
}
.taskItemBtn4{
  border: 2rpx solid #BFBFBF;
  color: #BFBFBF;
}

/* 提现金额 */
.withdrawalAmount{
  width: 100%;
  padding: 30rpx 24rpx 30rpx;
  box-sizing: border-box;
  background: linear-gradient(to right, #FF627E, #FE9D6A); 
  border-radius: 14rpx;
}
.withdrawalAmountTop{
  width: 100%;
  border-bottom: 2rpx solid #EAEAEA;
  position: relative;
}
.wAToptxt{
  padding:0 0 20rpx 0;
  font-size: 28rpx;
  color:#fff;
}
.goWA{
  width: 220rpx;
  height: 80rpx;
  line-height: 80rpx;
  text-align: center;
  font-size:24rpx;
  color: #FF7875;
  position: absolute;
  top: 0;
  bottom: 0;
  right: 0;
  margin: auto;
  background: #fff;
  border-radius: 12rpx;
  z-index: 1;
}
.withdrawalAmountBottom{
  padding: 20rpx 0 0 0;
  display: flex;
  justify-content: space-between;
  font-size: 28rpx;
  color: #fff;
}
.withdrawalABImg{
  padding-right: 33rpx;
  position: relative;
}
.WithdrawalRecordI{
  width: 30rpx;
  height: 30rpx;
  position: absolute;
  top: 0;
  right: 0;
  bottom: 0;
  margin: auto;
}
/* 弹框 */
.orderMangAddress{
  width: 660rpx;
  background: #fff;
  border-radius: 15rpx;
}
.batchExportOrder{
  width: 600rpx;
  margin: 40rpx auto 40rpx;
  font-size: 28rpx;
  line-height: 50rpx;
  display: block;
}
.oAMaBConfirm{
  width: 200rpx;
  height: 64rpx;
  line-height: 64rpx;
  text-align: center;
  border-radius: 10rpx;
  border:2rpx solid #90D2D6;
  color: #90D2D6;
}
.oAMaBCancel{
  width: 380rpx;
  height: 68rpx;
  line-height: 68rpx;
  text-align: center;
  border-radius: 10rpx;
  background: #90D2D6;
  color: #fff;
}
.orderMangTipCenterEve{
  width: 100%;
  display: flex;
  justify-content:space-between;
  padding-bottom: 30rpx;
  font-size: 28rpx;
}