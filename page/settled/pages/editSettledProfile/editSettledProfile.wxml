<t_f title='{{c_title}}' arrow='{{c_arrow}}' backcolor='{{c_backcolor}}'></t_f>
<view style='width:100%;height:{{statusBarHeightMc}}px;'></view>

<view class="applyForOccupancy">
    <block wx:if="{{num == 4}}">
        <!-- 专区数据 -->
        <block wx:if="{{from=='zhuanqu'}}">
            <settledForm 
                id="settledForm"
                statusBarHeightMc="{{statusBarHeightMc}}"
                list="{{IPData}}"
                bind:bindchange="bindchange"
                num="{{num}}"
                isEdit="true"
                brandInfo="{{brandInfo}}">
            </settledForm>
        </block>
        <block wx:else>
            <settledForm 
                id="settledForm"
                statusBarHeightMc="{{statusBarHeightMc}}"
                list="{{settledType==0?enterpriseData:personData}}"
                bind:bindchange="bindchange"
                num="{{num}}"
                isEdit="true"
                brandInfo="{{brandInfo}}">
            </settledForm>
        </block>
        <block>
            <view class="agreement" catchtap='showrule'>提交即代表默认同意<text>《商家入驻协议》</text></view>
            <view class="footerWrap">
                <view data-num="1" catchtap="navigateBack">返回</view>
                <view catchtap="submitAudit">提交</view>
            </view>
        </block>
    </block>
</view>