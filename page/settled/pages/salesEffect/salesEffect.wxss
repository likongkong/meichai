@import "/common/tabbar.wxss";

page{
  background:#f4f5f9;
}
.seBox{
  width: 100%;
  padding: 20rpx 0 20rpx 0;
}
.marginAuto{
  width: 710rpx;
  margin:0 auto 20rpx;
}
.elsearch{
  height:60rpx;
  position: relative;
}
.elsearchch{
  padding-left: 60rpx;
  width: 530rpx;
  height: 54rpx;
  background: #fff;
  border-radius: 20rpx;
  border: 2rpx solid #BBBBBB;
}
.elserarchimg{
  width: 30rpx;
  height: 30rpx;
  position: absolute;
  left: 20rpx;
  top: 0;
  bottom: 0;
  margin: auto;
}
.elinputbut{
  height: 100%;
  width: 500rpx;
  float: left;
  font-size: 24rpx;
}
.elserbotom{
  position: absolute;
  top: 0;
  bottom: 0;
  right: 0;
  width: 90rpx;
  height: 58rpx;
  line-height: 58rpx;
  text-align: center;
  border-radius: 15rpx;
  font-size: 24rpx;
  color: #fff;
  background: #90D2D6;
  margin: auto;
}

.seTop{
  width: 100%;
  padding: 30rpx 0 20rpx 0;
  position: relative;
}
.seTopBox{
  width: 720rpx;
  margin:0 auto 20rpx;
  background:#fff;
  border-radius: 16rpx;
}
