
@import "/common/tabbar.wxss";
page{
  background:#f4f5f9;
  padding-bottom: 140rpx;
}

.seckill{
  width: 100%;
}
.seckillImgBox{
  width: 100%;
  max-height: 750rpx;
  overflow: hidden;
}
.seckillImgBox image{
  display: block;
  width: 100%;
  height: auto;
}
.sCountDown{
  width: 100%;
  height: 60rpx;
  line-height: 60rpx;
  background:rgba(0, 0, 0, .5);
  font-size:28rpx;
  text-align: center;
  color: #fff;
}
.sCountstock{
  width: 100%;
  height: 60rpx;
  line-height: 60rpx;
  color:#999495;
  font-size: 24rpx;
}
.sCountstock text{
  margin-left: 40rpx;
}
.seckillName{
  color: #101010;
  font-size: 28rpx;
  text-align: center;
  padding:0 20rpx 20rpx;
}
.sSaleTime{
  font-size: 24rpx;
  color:#178FF8;
  text-align:center;
}
.sSalePrice{
  color: #FF2742;
  font-size: 28rpx;
  text-align: center;
  padding-top: 20rpx;
}
.briefIntroduction{
  font-size: 28rpx;
  padding: 20rpx;
}
.exhibdeimgBox{
  padding: 20rpx 0;
  height: 100rpx;
}
.exhibdeimgNewNew{
  width: 100rpx;
  height: 100rpx;
  float: left;
  border-radius: 6rpx;
  box-shadow: 2px 2px 5rpx #ccc;  
}
.exhibdeimgBRight{
  height: 100rpx;
  width: 590rpx;
  float: left;
  margin-left: 20rpx;
  position: relative;
}
.exhibdeimgBRTop{
  width: 100%;
  padding-top: 10rpx;
  font-size: 24rpx;
  vertical-align: top;
}
.exhibdeimgBRTopName{
  max-width: 300rpx;
  display: inline-block;
  font-size: 24rpx;
  height:36rpx ;
  line-height: 36rpx;
  vertical-align: top;
}
.questionMark{
  display:inline-block;
  height: 36rpx;
  margin:0 0 0 15rpx;
  vertical-align: top;
}
.emlhright{
  width:90rpx;
  height: 36rpx;
  text-align: center;
  line-height: 36rpx;
  color: #fff;
  display: inline-block;
  font-size: 24rpx;
  border-radius: 8rpx;
  background:#34A9ED;
  margin-left: 15rpx;
  vertical-align: top;
}
.emlhrightNo{
  width:90rpx;
  height: 36rpx;
  text-align: center;
  line-height: 36rpx;
  color: #34A9ED;
  display: inline-block;
  font-size: 24rpx;
  border-radius: 8rpx;
  border:2rpx solid #34A9ED;
  margin-left: 15rpx;
}

.exhibdeimgBRRipos{
  width: 136rpx;
  height: 100rpx;
  position: absolute;
  top: 0;
  right: 0;
}
.exhibdeimgBRRipos image{
  width: 60rpx;
  height: 60rpx;
  display: block;
  margin: auto;
}
.exhibdeimgBRRipos view{
  width: 100%;
  height: 40rpx;
  text-align: center;
  font-size: 24rpx;
  line-height: 40rpx;
}
.briefIntroduction{
  display: block;
  width:670rpx;font-size:26rpx;color:#333;margin:15rpx auto 0;
  background:#fff;
  border-radius: 10rpx;
  padding:20rpx;
}
.bDappIcomBox{
  width:710rpx;
  padding-top: 15rpx;
  height: 100rpx;
  margin:0 auto 40rpx;
  border-radius: 10rpx;
  background:#fff;
  display: flex;
  justify-content:space-around;
}
.bdaiBoxImg{
  width:55rpx;
  height: 55rpx;
  display: block;
  margin:auto;
  border-radius: 50%;
}
.bdaiBoxTxt{
  width: 100%;
  height: 40rpx;
  line-height: 40rpx;
  font-size:22rpx;
  text-align: center;
}



.addfrindcomimg {
  width: 100%;
  height: 100%;
  display: block;
}
.addfrindcommon {
  width: 500rpx;
  height: 700rpx;
  position: absolute;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  margin: auto;
  border-radius: 15rpx;
}
.addfrindcomsave {
  width: 300rpx;
  height: 80rpx;
  text-align: center;
  line-height: 80rpx;
  background: #FB595F;
  font-size: 26rpx;
  color: #fff;
  position: absolute;
  left: 0;
  right: 0;
  bottom: -120rpx;
  margin: auto;
  border-radius: 15rpx;
}

.detailimg image {
  display: block;
  width: 100% !important;
  background:#fff
}

.seckillBottom{
  width: 100%;
  height: 130rpx;
  position: fixed;
  left: 0;
  bottom: 0;
  z-index: 9;
  background: #fff;
}
.seckillBottomBox{
  width: 100%;
}
.sbottomLeft{
  width: 110rpx;
  height: 100rpx;
  float: left;
}

.emlplefimgshare{
  width: 40rpx;
  height: 40rpx;
  margin:10rpx auto 0;
  position: relative;
  display: block;
}
.sbottomLeftNum{
  width: 100%;
  height: 40rpx;
  line-height: 40rpx;
  text-align: center;
  font-size: 24rpx;
}

.sbottomRight{
  height: 100rpx;
  float: right;
  width: 410rpx;
  position: relative;
}
.sbottomRightButton{
  height: 70rpx;
  line-height: 70rpx;
  width: 380rpx;
  border-radius: 60rpx;
  text-align: center;
  position: absolute;
  top: 0;
  left: 0;
  bottom: 0;
  margin: auto;
  z-index: 1;
  font-size: 28rpx;
  color: #fff;
}