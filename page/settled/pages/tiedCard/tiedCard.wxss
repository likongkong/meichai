page{
  background:#f4f5f9;
}
.applyForOccupancy{
  padding-bottom: 150rpx; 
}
.card{
  width: 100%;
  background: #ffffff;
  margin: 20rpx auto 0 auto;
}
.footerWrap{
  padding: 20rpx 35rpx 40rpx 35rpx;
  box-sizing: border-box;
  display: flex;
  justify-content: space-between;
  position:fixed;
  bottom: 0;
  left: 0;
  width: 100%;
  z-index: 100;
}
.footerWrap view{
  height: 70rpx;
  line-height: 70rpx;
  text-align: center;
  font-size: 24rpx;
  border-radius: 10rpx;
}
.footerWrap view:first-child{
  width: 25%;
  border: 2rpx solid #90D2D6;
  background: #fff;
  color: #90D2D6;
}
.footerWrap view:last-child{
  width: 71%;
  height: 74rpx;
  background: #90D2D6;
  color: #ffffff;
}