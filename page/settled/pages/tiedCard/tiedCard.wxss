page{
  background:#f4f5f9;
}
.applyForOccupancy{
  padding-bottom: 260rpx; 
}
.card{
  width: 730rpx;
  background: #ffffff;
  border-radius: 20rpx;
  margin: 20rpx auto 0 auto;
}
.footerWrap{
  background:#f4f5f9;
  padding: 20rpx 35rpx 40rpx 35rpx;
  box-sizing: border-box;
  display: flex;
  justify-content: space-between;
  position:fixed;
  bottom: 0;
  left: 0;
  width: 100%;
  z-index: 100;
}
.footerWrap view{
  height: 80rpx;
  line-height: 80rpx;
  text-align: center;
  font-size: 28rpx;
  border-radius: 16rpx;
}
/* .footerWrap view:first-child{
  width: 20%;
  border: 2rpx solid #90D2D6;
  color: #90D2D6;
} */
.footerWrap view{
  width: 100%;
  height: 84rpx;
  background: #90D2D6; 
  color: #ffffff;
}
.illustrate{
  width: 100%;
  background: #ffffff;
  padding: 20rpx;
  box-sizing: border-box;
  margin: 20rpx 0;
}
.illustrate text{
  display: block;
  font-size: 28rpx;
  color: #989898;
}
.illustrate .title{
  margin-bottom: 10rpx;
}
.resetWrap{
  width: 100%;
  text-align: center;    
  color: #959595;
  font-size: 26rpx;
  /* margin-top: 30rpx; */
  /* position: fixed;
  bottom: 152rpx;
  left: 0; */
  background:#f4f5f9;
  padding: 20rpx 0 0 0;
}
.resetWrap view{
  display: inline-block;
  background-color: #AE7676;
  color: #fff;
  border-radius: 8rpx;
  padding: 0 12rpx;
}
.agreementWrap{
  width: 100%;
  text-align: center;    
  color: #959595;
  margin-top: 30rpx;
  position: fixed;
  bottom: 140rpx;
  left: 0;
  background:#f4f5f9;
  padding: 20rpx 0 0 0;
  font-size: 28rpx;
  color: #7B7B7B;
  display: flex;
  align-items: center;
  justify-content: center;
}
.agreementRadio{
  transform:scale(0.7);
}
.agreementWrap text{
  color: #90D2D6;
}
.mask{
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  background: rgba(0,0,0,.4);
  z-index: 100;
  display: flex;
  justify-content: center;
  align-items: center;
}
.maskBox{
  width: 680rpx;
  height: auto;
  background: #fff;
  border-radius: 8rpx;
}
.unlinkWrap{
  padding: 72rpx 48rpx 60rpx 42rpx;
  box-sizing: border-box;
}
.unlinkWrap .title{
  color: #101010;
  font-size: 28rpx;
  display: block;
  margin-bottom: 56rpx;
}
.footerWrap1{
  display: flex;
  justify-content: space-between;
  width: 100%;
  background-color: #ffffff;
}
.footerWrap1 view{
  height: 72rpx;
  line-height: 72rpx;
  text-align: center;
  font-size: 24rpx;
  border-radius: 12rpx;
}
.footerWrap1 view:first-child{
  width: 30%;
  border: 2rpx solid #90D2D6;
  color: #90D2D6;
}
.footerWrap1 view:last-child{
  width: 63%;
  height: 76rpx;
  background: #90D2D6;
  color: #ffffff;
}