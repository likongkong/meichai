
@import "/common/tabbar.wxss";
page{
  background:#f4f5f9;
}
.atlas{
  width: 100%;
}
.atlasswiper{
  width: 100%;
  height: 750rpx;
}
.atlasswiperimage{
  width: 100%;
  height: 100%;
  display: block;
}
.atlasIwant{
  width: 100%;
  padding: 20rpx 0 0 0;
  text-align: center;
}
.atlasIwantEve{
  display: inline-block;
  width: 330rpx;
}
.atlasIwantETop{
  width: 100%;
  height: 70rpx;
  line-height: 70rpx;
  font-size: 24rpx;
  text-align: center;
  background:#fff;
  border-radius: 12rpx;
}
.atlasIwantEb{
  width: 240rpx;
  height: 70rpx;
  line-height: 70rpx;
  font-size: 24rpx;
  text-align: right;
  position: relative;
  margin:auto;
}
.emlplefimgshare{
  width: 36rpx;
  height: 36rpx;
  margin-right: 15rpx;
  display: inline-block;
  vertical-align:middle;
}
.atlasIwantEbImg{
  width: 40rpx;
  height: 40rpx;
  position: absolute;
  top: 0;
  bottom: 0;
  margin: auto;
  border-radius: 50%;
}
.atlasCenter{
  padding: 20rpx;
  background:#fff;
}
.atlasCName{
  width: 100%;
  font-size: 28rpx;
  font-weight: bold;
  padding: 30rpx 0;
}
.atlasCNChild{
  width: 100%;
  font-size: 24rpx;
  padding-bottom: 30rpx;
}

.atlasCNChildEve{
  background:#F5F5F5;
  padding: 20rpx;
  border-radius: 16rpx;
  margin-bottom: 20rpx;
}
.atlasCNChildETop{
  margin-bottom: 20rpx;
  display:flex;
  justify-content: space-between;
  font-size: 24rpx;
}
.startSubscription{
  width: 172rpx;
  height: 56rpx;
  line-height: 56rpx;
  border-radius: 10rpx;
  color: rgba(255, 255, 255, 100);
  font-size: 14px;
  text-align: center;  
}

.emlist{
  width:100%;
  background: #fff;
  margin-bottom:15rpx; 
}
.emlhead{
  padding: 20rpx;
  height: 70rpx;
  position: relative;
}
.emlhleft{
  width:500rpx;
  height: 100%;
  float: left; 
}
.emlhlimg{
  width: 70rpx;
  height: 70rpx;
  float: left;
  border-radius: 50%;
}
.emlhltxt{
  width: 395rpx;
  height: 70rpx;
  float: left;
  margin-left:15rpx; 
}
.emlhlttop{
  width: 100%;
  height: 38rpx;
  line-height: 36rpx;
  font-size: 24rpx;
}
.emlhltbottom{
  width: 100%;
  height: 30rpx;
  font-size: 22rpx;  
  color: #999;
}
.emlhright{
  width:90rpx;
  height: 36rpx;
  text-align: center;
  line-height: 36rpx;
  color: #ccc;
  position: absolute;
  top: 0;
  bottom: 0;
  right: 20rpx;
  margin: auto; 
  font-size: 24rpx;
  border-radius: 4rpx;
  background:#34A9ED;
  color: #fff;
}
.boxAtlas{
  border: 2rpx solid #34A9ED;
  overflow: hidden;
  margin: 10rpx;
  font-size: 20rpx;
  line-height: 30rpx;
  border-radius: 6rpx;
  padding: 10rpx;
}
.boxAtlasLeft{
  float: left;
  height: 30rpx;
  padding: 0 10rpx;
  color: #fff;
  background:#FF890B;
  border-radius: 6rpx;
}
.boxAtlasName{
  width: 140rpx;
  height: 30rpx;
  float: left;
  margin-left: 10rpx;
}
.boxAtlasRight{
  float: right;
  height: 30rpx;
  color: #34A9ED;
  text-decoration:underline;
}
.wxIcon{
  width: 60rpx;
  height: 60rpx;
  float: left;
  margin-right: 50rpx;
  border-radius: 50%;
  position: relative;
}
.dyDetheat{
  height: 60rpx;
  line-height: 60rpx;
  float: right;
  font-size:24rpx;
  padding-left: 50rpx;
  position: relative;
}
.emlplefimgimg{
  width: 40rpx;
  height: 40rpx;
  position: absolute;
  top: 0;
  bottom: 0;
  left: 0;
  margin: auto;
}

