}

.WaybillNoDeletedBox{
  padding-right: 60rpx;
  position: relative;
}
.WaybillNoDeleted{
  width: 40rpx;
  height: 40rpx;
  border-radius: 50%;
  position: absolute;
  top: 0;
  right: 0;
  bottom: 0;
  margin: auto;
}
.orderAddressEveWrap{
  background: #F2F2F2;
  padding: 10rpx;
  box-sizing: border-box;
  border-radius: 10rpx;
}
.orderAddressEveWrap view:last-child{
  margin-bottom: 0;
}
.shouhuoAddress{
  display: flex;
  justify-content:flex-start;
  align-items:center;
}
.shouhuoAddress view:first-child{
