}
.shouhuoAddress view:last-child{
  background: #90D2D6;
  color: #fff;
  border-radius: 6rpx;
  font-size: 20rpx;
  padding: 2rpx 6rpx;
  box-sizing: border-box;
  margin-left:8rpx;
}

