  height: 50rpx;
  display: inline-block;
}

.batchExportOrder{
  width: 600rpx;
  margin: 0 auto 60rpx;
  font-size: 28rpx;
  line-height: 50rpx;
  display: block;
}

.orderBoxTopNew{
  color:#9DD7DB;text-align: center;font-size: 24rpx;
  width: 100%;
  height: 80rpx;
  line-height: 80rpx;
}
.cmScreen{
  height: 80rpx;
  padding-left: 50rpx;
  line-height: 80rpx;
  color: #90D2D6;
  position: absolute;
  top: 0;
  right: 20rpx;
  z-index: 2;
}
.cmScreen image{
  position: absolute;
