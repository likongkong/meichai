  vertical-align: top;
}
.leftEveheight{
  height: 50rpx;
  line-height: 50rpx;
}
.addressMoImg{
  width: 30rpx;
  height: 30rpx;
  display: inline-block;
  vertical-align: top;
  margin: 10rpx 0 0 10rpx;
}
.addressModifytTa{
  color:#AAAAAA;
  text-align: right;
  width: 360rpx;
  height: 110rpx;  
  display: inline-block;
}

.oAMaBConfirm{
  width: 200rpx;
