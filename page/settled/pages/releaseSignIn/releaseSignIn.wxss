page{
  background:#f4f5f9;
  padding-bottom: 200rpx;
}
.signInIllustrateWrap{
  background: #ffffff;
  border-radius: 20rpx;
  margin-top: 24rpx;
  padding: 12rpx 12rpx 35rpx 12rpx;
  box-sizing: border-box;
}
.signInIllustrateWrap .title{
  width: 100%;
  height: 80rpx;
  display: block;
  line-height: 80rpx;
  padding: 0 14rpx;
  box-sizing: border-box;
  border-bottom: 2rpx solid #F5F5F5;
  font-size: 28rpx;
  color: #101010;
  margin-bottom: 20rpx;
}
.signInNumWrap{
  width: 90%;
  height: 110rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
  margin: auto;
  position: relative;
}
.line{
  width: 92%;
  height: 2rpx;
  background: #000000;
  position: absolute;
  top: 50%;
  left: 50%;
  margin-top: -1rpx;
  margin-left: -46%;
}
.signInNumItem{
  height: 100%;
  /* position: absolute;
  z-index: 2; */
  display: flex;
  flex-direction:column;
  justify-content: space-between;
}
.signInNumItem view{
  width: 18rpx;
  height: 18rpx;
  border-radius: 20rpx;
  border: 2rpx solid #000000;
  background: #fff;
  margin: auto;
  position: relative;
}

.signInNumItem text:first-child{
  color: #A3A2A7;
}
.signInNumItem text{
  display: block;
  font-size: 20rpx;
  color: #101010;
  text-align: center;
}
.tipWrap{
  padding: 0 26rpx;
  box-sizing: border-box;
  margin-top: 30rpx;
}
.tip{
  color: #999495;
  font-size: 28rpx;
}
.footerWrap{
  padding: 20rpx 35rpx 40rpx 35rpx;
  box-sizing: border-box;
  display: flex;
  justify-content: space-between;
  position:fixed;
  bottom: 0;
  left: 0;
  width: 100%;
  background-color: #f4f5f9;
  z-index: 10;
}
.footerWrap view{
  height: 76rpx;
  line-height: 76rpx;
  text-align: center;
  font-size: 24rpx;
  border-radius: 10rpx;
}
.footerWrap view:first-child{
  width: 26%;
  border: 2rpx solid #90D2D6;
  color: #90D2D6;
  background: #ffffff;
}
.footerWrap1 view:nth-child(2n){
  width: 26%;
  border: 2rpx solid #90D2D6;
  color: #90D2D6;
  background: #ffffff;
}
.footerWrap view:last-child{
  width: 70%;
  height: 80rpx;
  background: #90D2D6;
  color: #ffffff;
}
.footerWrap1 view:last-child{
  width: 40%;
  height: 80rpx;
  background: #90D2D6;
  color: #ffffff;
}