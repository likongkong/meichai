
@import "/common/tabbar.wxss";
page{
  background:#f4f5f9;
  padding-bottom: 130rpx;
}
.atlas{
  width: 100%;
}
.atlasswiper{
  width: 100%;
  height: 750rpx;
}
.atlasswiperimage{
  width: 100%;
  height: 100%;
  display: block;
}
.atlasIwant{
  width: 100%;
  padding: 20rpx 0 20rpx 0;
  overflow: hidden;
}
.atlasIwantEve{
  width: 330rpx;
  float: left;
}
.atlasIwantETop{
  width: 100%;
  height: 70rpx;
  line-height: 70rpx;
  font-size: 24rpx;
  text-align: center;
  background:#fff;
  border-radius: 12rpx;
}
.atlasIwantEb{
  width: 240rpx;
  height: 40rpx;
  line-height: 40rpx;
  font-size: 24rpx;
  text-align: right;
  position: relative;
  margin:20rpx auto 0;
}
.emlplefimgshare{
  width: 36rpx;
  height: 36rpx;
  margin-right: 15rpx;
  display: inline-block;
  vertical-align:middle;
}
.atlasIwantEbImg{
  width: 40rpx;
  height: 40rpx;
  position: absolute;
  top: 0;
  bottom: 0;
  margin: auto;
  border-radius: 50%;
}
.atlasCenter{
  padding: 20rpx;
  background:#fff;
}
.atlasCName{
  width: 100%;
  font-size: 28rpx;
  font-weight: bold;
  padding: 30rpx 0;
}
.atlasCNChild{
  width: 100%;
  font-size: 24rpx;
  padding-bottom: 30rpx;
}

.atlasCNChildEve{
  background:#F5F5F5;
  padding: 20rpx;
  border-radius: 16rpx;
  margin-bottom: 20rpx;
}
.atlasCNChildETop{
  margin-bottom: 20rpx;
  display:flex;
  justify-content: space-between;
  font-size: 24rpx;
}
.startSubscription{
  width: 172rpx;
  height: 56rpx;
  line-height: 56rpx;
  border-radius: 10rpx;
  color: rgba(255, 255, 255, 100);
  font-size: 14px;
  text-align: center;  
}



.editButtom{
  width: 100%;
  padding: 15rpx 0;
  position: fixed;
  left: 0;
  bottom: 30rpx;
  z-index: 10;
  background:#fff;
  font-size:24rpx;
  text-align: center;
}
.editButtom image{
  height: 50rpx;
  width: 50rpx;
  margin: auto;
  display: block;
  margin-bottom: 10rpx;
}
