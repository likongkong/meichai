.applyForOccupancy{
  padding-bottom: 50rpx; 
}
.size26{
  font-size: 26rpx;
  text-align: center;
  margin: 10rpx 0;
}

.aFoCenter{
  width: 690rpx;
  margin: auto;
  background:#fff;
  border-radius: 15rpx;
  padding-bottom: 40rpx;
}
.failCause{
  width: 100%;
  height: 90rpx;
  background: #E3E3E3;
  color: #FF2742;
  line-height: 90rpx;
  font-size: 24rpx;
  padding-left: 20rpx;
  box-sizing: border-box;
}
.submit{
  width: 90%;
  height: 90rpx;
  line-height: 90rpx;
  background: #90D2D6;
  border-radius: 14rpx;
  color: #fff;
  font-size: 26rpx;
  margin-top: 60rpx;
}
.submit::after{
  border: none;
}

.footerFailWrap{
  padding: 0 50rpx;
  box-sizing: border-box;
  margin-top: 134rpx;
}
.footerFailWrap view{
  height: 80rpx;
  line-height: 80rpx;
  text-align: center;
  font-size: 24rpx;
  border-radius: 16rpx;
}
.footerFailWrap view{
  width: 100%;
  height: 84rpx;
  background: #90D2D6;
  color: #ffffff;
}
.footerFailWrap text{
  text-decoration:underline;
  color: #8BCFFD;
  display: block;
  text-align: center;
  padding: 10rpx 0;
  font-size: 24rpx;
}




.sampleGraphMask{
  width: 100%;
  height: 100%;
  background: rgba(0,0,0,.4);
  position: fixed;
  top: 0;
  left: 0;
  z-index: 100;
  display: flex;
  justify-content: center;
  align-items: center;
}
.sampleGraphBox{
  width: 79%;
  border-radius: 14rpx;
  background-color: #fff;
}
.sampleGraphBox text{
  display: block;
  text-align: center;
  color: #101010;
  font-size: 28rpx;
  margin: 16rpx 0 20rpx 0;
}
.sampleGraphBox image{
  width: 450rpx;
  height: auto;
  display: block;
  margin: 0 auto;
}
.sampleGraphBox button{
  width: 140rpx;
  height: 50rpx;
  line-height: 50rpx;
  background-color:#90D2D6;
  font-size: 26rpx;
  color: #fff;
  margin: 30rpx auto 20rpx; 
}
.sampleGraphBox button::after{
  border:none;
}