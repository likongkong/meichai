.applyForOccupancy{
  padding-bottom: 50rpx; 
}
.size26{
  font-size: 26rpx;
  text-align: center;
  margin: 10rpx 0;
}

.aFoCenter{
  width: 690rpx;
  margin: auto;
  background:#fff;
  border-radius: 15rpx;
  padding-bottom: 40rpx;
}
.failCause{
  width: 100%;
  height: 90rpx;
  background: #E3E3E3;
  color: #FF2742;
  line-height: 90rpx;
  font-size: 24rpx;
  padding-left: 20rpx;
  box-sizing: border-box;
}
.submit{
  width: 90%;
  height: 90rpx;
  line-height: 90rpx;
  background: #90D2D6;
  border-radius: 14rpx;
  color: #fff;
  font-size: 26rpx;
  margin-top: 60rpx;
}
.submit::after{
  border: none;
}

.footerFailWrap{
  padding: 0 50rpx;
  box-sizing: border-box;
  margin-top: 134rpx;
}
.footerFailWrap view{
  height: 80rpx;
  line-height: 80rpx;
  text-align: center;
  font-size: 24rpx;
  border-radius: 16rpx;
}
.footerFailWrap view{
  width: 100%;
  height: 84rpx;
  background: #90D2D6;
  color: #ffffff;
}
.footerFailWrap text{
  text-decoration:underline;
  color: #8BCFFD;
  display: block;
  text-align: center;
  padding: 10rpx 0;
  font-size: 24rpx;
}

