@import "/common/tabbar.wxss";
@import "/common/citysel.wxss";
page{
  background:#f4f5f9;
  padding-bottom: 150rpx;
}
.afwbox{
  width: 100%;
  padding: 25rpx 0;
}
.afwboxEve{
  padding: 0 25rpx;
  background: #fff;
}
.afwboxEveChilder{
  width: 100%;
  height: 80rpx;
  line-height: 80rpx;
  font-size: 28rpx;
  display: flex;
  justify-content:space-between;
}
.inputMoneyBox{
  width: 100%;
  height: 100rpx;
}
.inputMoneyBoxLeft{
  height: 80rpx;
  line-height: 80rpx;
  font-size: 40rpx;
  float: left;
}
.inputMoneyInput{
  margin-left: 10rpx;
  width: 300rpx;
  color:#90D2D6;
}
.InvoiceInformation{
  padding: 25rpx;
  font-size: 28rpx;
}
.copyCart{
  position: absolute;
  top: 0;
  right: 0;
  bottom: 0;
  margin: auto;
  background:#F4F4F4;
  font-size: 24rpx;
  color: #000;
  height: 40rpx;
  line-height: 40rpx;
  width: 60rpx;
  text-align: center;
  border-radius:6rpx;
}
.copyInformation{
  padding-right: 70rpx;
  color:#848484;
  position: relative;
}
.uploadMoneyBoxP{
  width: 100%;
  padding: 15rpx 0 0 0;

}
.uploadMoneyBox{
  display: inline-block;
  height: 60rpx;
  line-height: 60rpx;
  font-size: 28rpx;
  margin-right: 36rpx;
  padding-left: 50rpx;
  position: relative;
}
.uploadMoneyBoxImg{
  width: 40rpx;
  height: 40rpx;
  position: absolute;
  top: 0;
  bottom: 0;
  left: 0;
  margin: auto;
}
.uploadBottom{
  height: 50rpx;
  line-height: 50rpx;
  width: 200rpx;
  text-align: center;
  font-size: 28rpx;
  color: #68BBC4;
  border-radius: 10rpx;
  border: 2rpx solid #68BBC4;
  display: inline-block;
  margin-right: 20rpx;
}
.uploadBottomFil{
  height: 50rpx;
  line-height: 50rpx;
  font-size: 28rpx;
  color: #BABABA;
  display: inline-block;
  margin-right: 36rpx;
}
.afwbottomFix{
  width: 100%;
  position: fixed;
  left: 0;
  bottom: 0;
  z-index: 10;
}
.afwbottomFixTop{
  padding-bottom: 15rpx;
  text-align: center;
  font-size: 24rpx;
  color:#AEAEAE ;
}

.afwbottomFixBot{
  width: 500rpx;
  height: 70rpx;
  line-height: 70rpx;
  text-align: center;
  font-size: 28rpx;
  margin:0 auto 30rpx;
  color: #fff;
  background: #90D2D6;
  border-radius: 10rpx;
}








