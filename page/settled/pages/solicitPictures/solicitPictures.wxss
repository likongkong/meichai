@import "/common/tabbar.wxss";
@import "/common/citysel.wxss";
page{
  background:#f4f5f9;
  padding-bottom: 200rpx;
}

.spdetail{
  padding: 25rpx;
  border-bottom: 2rpx solid #ccc;
  background: #fff;
}
.spdetName{
  font-size: 28rpx;
  margin-bottom: 25rpx;
}
.spdetailImg{
  display: block;
  height: auto;
  width: 100%;
  border-radius: 16rpx;
}
.spdetailCenter{
  width: 100%;
  height: 120rpx;
}
.spdetailCenterImg{
  width: 120rpx;
  height: 120rpx;
  float: left;
  border-radius: 10rpx;
}
.spdetailCenterRight{
  height: 120rpx;
  line-height: 120rpx;
  margin-left: 30rpx;
  font-size: 24rpx;
  float: left;
  width: 500rpx;
}
.spdetailExplain{
  display: block;
  background: #ccc;
  border-radius: 5px;
  font-size: 28rpx;
  padding: 25rpx;
  line-height: 50rpx;
}
.dldlConfirmPublication{
  position: fixed;
  bottom:70rpx;
  left: 0;
  right: 0;
  margin: 0 auto;
  z-index: 10;
  width: 560rpx;
  height: 80rpx;
  line-height: 80rpx;
  text-align: center;
  color: #fff;
  font-size: 28rpx;
  background: #5CA7FE;
  border-radius: 80rpx;
}