<t_f title='{{c_title}}' arrow='{{c_arrow}}' backcolor='{{c_backcolor}}'></t_f>
<view style='width:100%;height:{{statusBarHeightMc}}px;'></view>
<view class="wrap">
  <text class="h1">{{data.title}}</text>
  <text class="h2">{{data.subtitle}}</text>
  <image class="litpic" src="{{data.litpic}}" mode="widthFix"></image>
  <view class="image_text">
    <rich-text nodes="{{data.image_text}}"></rich-text>
  </view>
  <text class="author">{{data.brandName}}</text>
  <text class="messageTime">{{data.publish_date}}</text>
</view>
