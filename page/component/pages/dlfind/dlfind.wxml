<hidewindow id='hide'></hidewindow>
<t_f title='{{c_title}}' arrow='{{c_arrow}}' backcolor='{{c_backcolor}}'></t_f>
<view style='width:100%;height:{{statusBarHeightMc}}px;'></view>

<view class='dlfbox' style="{{dlhintiftr?'position:fixed;top:0;left:0;right:0;bottom:0;':''}}">
  <view class='dlfhead'>

    <view style="background:#fff;">
            <block wx:if='{{visitHistory && visitHistory.length != 0}}'>
                <view class='dlfindToptitle'>最近浏览</view>
                <view style="width:100%;min-height:80rpx;background:#fff;">
                    <view style="position:relative;height:150rpx;">
                        <scroll-view scroll-x="true" style="width:710rpx;white-space: nowrap;height:150rpx;margin:auto;">
                              <block wx:for='{{visitHistory}}' wx:key='key'>
                                  <view class="ocamensweve" catchtap="jumpsouchtem" data-id='{{item.brandId}}' wx:if='{{item.brandId!=0}}'>
                                      <image class="ocamensweveimg" mode='aspectFill' style="{{item.brandId==brand_id?'border:2rpx solid #f19207;':'border:2rpx solid #fff;'}}" src="{{item.brandLogo}}"></image>
                                      <image class='dlfchrtopimgipNew' wx:if='{{item.isNewBrand}}' mode="aspectFit" src='https://cdn.51chaidan.com/images/brandSettledIcon/newIcon.png'></image>
                                      <image class='dlfchrtopimgip' wx:if='{{item.isAuth}}' mode="aspectFit" src='https://cdn.51chaidan.com/images/brandSettledIcon/approveIcon.png'></image>
                                      <view class="ocamenswevename specialellipsis" style="{{item.brandId==brand_id?'color:#f19207;':''}}">{{item.brandName}}</view>
                                  </view>                      
                              </block>
                        </scroll-view>
                    </view>   
                </view>
            </block>

            <block wx:if='{{recommendIps && recommendIps.length != 0}}'>
                <view class='dlfindToptitle'>推荐IP</view>
                <view style="width:100%;min-height:80rpx;background:#fff;margin-bottom:15rpx;">
                    <view style="position:relative;height:150rpx;width:710rpx;margin:auto;">
                        <view class="ocamensweve" catchtap="brandWholeFun" style="float:left;">
                            <image class="ocamensweveimg" src="https://cdn.51chaidan.com/images/toyCabinet/allLog.jpg"></image>
                            <view class="ocamenswevename specialellipsis">全部</view>
                        </view>
                        <scroll-view scroll-x="true" style="width:590rpx;white-space: nowrap;height:150rpx;float:left;">
                              <block wx:for='{{recommendIps}}' wx:key='key'>
                                  <view class="ocamensweve" catchtap="jumpsouchtem" data-id='{{item.brandId}}'>
                                      <image class="ocamensweveimg" mode='aspectFill' style="{{item.brandId==brand_id?'border:2rpx solid #f19207;':'border:2rpx solid #fff;'}}" src="{{item.brandLogo}}"></image>
                                      <!-- wx:if='{{item.is_authentication==1}}' -->
                                      <image class='dlfchrtopimgipNew' wx:if='{{item.isNewBrand}}' mode="aspectFit" src='https://cdn.51chaidan.com/images/brandSettledIcon/newIcon.png'></image>
                                      <image class='dlfchrtopimgip' wx:if='{{item.isAuth}}' mode="aspectFit" src='https://cdn.51chaidan.com/images/brandSettledIcon/approveIcon.png'></image>
                                      <view class="ocamenswevename specialellipsis" style="{{item.brandId==brand_id?'color:#f19207;':''}}">{{item.brandName}}</view>
                                  </view>                      
                              </block>
                        </scroll-view>
                    </view>
                </view>
            </block>
            

    </view>

    <view class="offeringCalendar" wx:if='{{salesCalendar && salesCalendar.length != 0}}'>
        <swiper class="offeringCswiper" vertical="true" autoplay="true" circular="true" interval="2500">
          <block wx:for="{{salesCalendar}}" wx:key='key'>
            <swiper-item catchtouchmove='catchTouchMove' class="swiper-item">
              <view class="dateOffering" catchtap="jumpoffering" data-id="{{item.itemId}}" data-type='1'>
                  <view class="ocswiper">
                      <image class="ocswiperImg" src='{{item.itemImg}}'></image>
                  </view>
                  <view class="occoneter">
                      <view class="ocNameTop multilineellipsis">{{item.itemName}}</view>
                      <view class="ocNameBottom specialellipsis">{{item.salesTimeName}}</view>
                  </view>   
              </view>
            </swiper-item>
          </block>
        </swiper>
        <view class="occoneterText" catchtap="jumpoffering" data-id="{{item.id}}" data-type='9037'><text>发售\n日历</text></view>
    </view>

    <!-- tab标签切换 -->
    <view id="bars" class='dlfheadtab {{isFixed?"tab-bar":""}}' style="margin-bottom:20rpx;top:{{isFixed?statusBarHeightMc:0}}px;">
      <scroll-view class="marginAuto scrollViewTop" scroll-x="true" scroll-with-animation="true" scroll-left='{{scrollleft}}' style="height:70rpx;">
        <view wx:for="{{payStatus}}" wx:key='key' id="ele{{item.num}}" class="classifyItem {{centerIndex == item.num?'active':''}}" data-index="{{item.num}}" catchtap="classifyChange">{{item.name}}</view>
      </scroll-view>         
      <image class="wOriTab" catchtap="wOriTab" mode="aspectFit" src='{{wOri==1?"https://cdn.51chaidan.com/images/brandSettledIcon/class2.png":"https://cdn.51chaidan.com/images/brandSettledIcon/class1.png"}}'></image>
    </view>

    <!-- 信息流 -->
    <block wx:if='{{wOri==1}}'>
        <view class="emlist" wx:for='{{communityList}}' wx:key='key'>
            <view class="emlhead">
                <view class="emlhleft" data-id='{{item.brandInfo.brand_id}}' catchtap="jumpexhdetail">
                    <image class='emlhlimg' src="{{item.brandInfo.brand_logo}}" mode='aspectFill'></image>

                    <image class='dlfchrtopimgipList' wx:if='{{item.brandInfo.isAuth}}' mode="aspectFit" src='https://cdn.51chaidan.com/images/brandSettledIcon/approveIcon.png'></image>

                    <view class="emlhltxt">
                          <view class="emlhlttop specialellipsis" style="font-size:26rpx;">{{item.brandInfo.brand_name}}</view>
                          <view class="emlhltbottom specialellipsis" style="font-size:24rpx;">{{item.add_time}}
                            <!-- <text class="stateBg" style="background:#ABC7F7;" wx:if='{{item.type==3}}'>动态</text>
                            <text class="stateBg" style="background:#F5B1AE;" wx:elif='{{item.type==1}}'>秒杀</text>
                            <text class="stateBg" style="background:#A9E6BA;" wx:elif='{{item.type==4}}'>签到</text>
                            <text class="stateBg" style="background:#9BE2F8;" wx:elif='{{item.type==2}}'>抽选</text> -->

                            <image class="stateBgNewImg" src="https://cdn.51chaidan.com/images/brandSettledIcon/dynamic.jpg" wx:if='{{item.type==3}}' mode="aspectFit"></image>
                            <block wx:elif='{{item.type==1}}'>
                                <!-- 限时 -->
                                <image wx:if='{{item.sellType == 1}}' class="stateBgNewImg" src="https://cdn.51chaidan.com/images/brandSettledIcon/limitSeckill.jpg" mode="aspectFit"></image>
                                <!-- 售卖 -->
                                <image wx:elif='{{item.sellType == 0}}' class="stateBgNewImg" src="https://cdn.51chaidan.com/images/brandSettledIcon/normalSeckill.jpg" mode="aspectFit"></image>
                            </block>
                            
                            <!-- <image class="stateBgNewImg" src="https://cdn.51chaidan.com/images/brandSettledIcon/" wx:elif='{{item.type==4}}' mode="aspectFill"></image> -->
                            <image class="stateBgNewImg" src="https://cdn.51chaidan.com/images/brandSettledIcon/selection.jpg" wx:elif='{{item.type==2}}' mode="aspectFit"></image>
                            <image class="stateBgNewImg" src="https://cdn.51chaidan.com/images/brandSettledIcon/showBox.jpg" wx:elif='{{item.type==5}}' mode="aspectFit"></image>

                          </view>
                    </view>
                </view>
                <view class="{{!item.is_attention?'emlhright':'emlhrightNo'}}" data-type='0' data-id='{{item.brandInfo.brand_id}}' data-istype='{{item.is_attention}}' data-ind='{{index}}' catchtap='followfun'>
										<block wx:if='{{!item.is_attention}}'>关注</block>
										<block wx:else>已关注</block>                  
                </view>
            </view>

            <view class="multilineellipsisFive" style="margin:20rpx;font-size:26rpx;" wx:if='{{item.title && item.type!=1 && item.type!=2 && item.type!=5}}' catchtap="jumpoffering" data-id="{{item.id}}" data-selltype='{{item.specialWay}}'  data-istype='{{item.type}}'><text>{{item.title}}</text></view>

            <view class="emlcontent" catchtap="jumpoffering" data-id="{{item.id}}" data-istype='{{item.type}}' data-selltype='{{item.specialWay}}'>
                <!-- 动态固定大小 -->
                <block wx:if='{{(item.type==3 || item.type==1) && item.imgArr.length==1}}'>
                    <view class="dynamicBox">
                        <image class="emlcontentimg" style="width:100%;height:100%;" src="{{item.imgArr[0]}}" mode="aspectFill"></image>
                    </view>
                </block>
                <block wx:else> 
                  <!-- 非动态单个图片 -->
                   <block wx:if='{{item.imgArr && item.imgArr.length ==1}}'>
                       <image class="emlcontentimg" style="height:399rpx;" src="{{item.imgArr[0]}}" mode="aspectFill"></image>
                       <!-- 未开始 -->
                       <image wx:if='{{item.status==1}}' class="statusImgBox" src="https://cdn.51chaidan.com/images/brandSettledIcon/NotStarted.png" mode="aspectFit"></image>
                       <!-- 已结束 -->
                       <image wx:elif='{{item.status==3}}' class="statusImgBox" src="https://cdn.51chaidan.com/images/brandSettledIcon/finished.png" mode="aspectFit"></image>
                       <!-- 进行中 -->
                       <image wx:elif='{{item.status==2}}' class="statusImgBox" src="https://cdn.51chaidan.com/images/brandSettledIcon/underway.png" mode="aspectFit"></image>
                   </block>
                   <!-- 非动态多个图片 -->
                   <block wx:else>
                        <view  class="dynamicBoxEve" wx:for='{{item.imgArr}}' wx:key='key'  wx:for-item='w' style="{{index%3==2?'margin-right:0;':''}}">
                            <image class="emlcontentimg" style="width:100%;height:100%;" src="{{w}}" mode="aspectFill"></image>
                        </view>
                   </block>
                </block>
                
                <view class="signInNowCl" style="background:#ECCA1E;" wx:if='{{item.type==4}}'>立即签到</view>
            </view>
            
            <view class="multilineellipsisFive" style="margin:20rpx;font-size:26rpx;" wx:if='{{item.title && (item.type==1 || item.type==2 || item.type==5)}}' catchtap="jumpoffering" data-id="{{item.id}}" data-selltype='{{item.specialWay}}' data-istype='{{item.type}}'><text>{{item.title}}</text></view>


            <view class="barndStatus" style="font-size:26rpx;" wx:if='{{item.temporary}}'>
                  <view class='dlfhtitlenew' style="color:#178FF8;font-size:26rpx;height: 40rpx;line-height: 40rpx;">{{item.temporary}}</view>
            </view>

            <view class="barndStatus" style="font-size:26rpx;" wx:elif='{{item.type==1 || item.type==2 || item.type==5}}'>
                <!-- 秒杀 -->
                <block wx:if='{{item.type==1}}'> 
                    <view class='dlfhtitlenew' style="color:#FF2742;font-size:26rpx;height: 40rpx;line-height: 40rpx;">￥<text style="font-size:36rpx;">{{item.price}}</text><text style="color:#000;" wx:if='{{item.integralNumber}}'> +积分{{item.integralNumber}}</text></view>
                    <view class='dlfhtitlenew' style="color:#178FF8;font-size:26rpx;height: 40rpx;line-height: 40rpx;" wx:if='{{item.status==1}}'>
                      <block wx:if='{{item.specialWay > 0}}'>直播售卖</block>
                      <block wx:else>{{item.start_time}}发售</block>
                    </view>
                    <view class='dlfhtitlenew' style="color:#999495;font-size:26rpx;height: 40rpx;line-height: 40rpx;" wx:elif='{{item.status==2 && item.sellNumber>0}}'>已售：{{item.sellNumber}}体</view>
                    <view class='dlfhtitlenew' style="color:#999495;font-size:26rpx;height: 40rpx;line-height: 40rpx;" wx:elif='{{item.status==3}}'>已结束</view>
                </block>
                <!-- 抽选 -->
                <block wx:elif='{{item.type==2}}'>
                    <view class='dlfhtitlenew' style="color:#FF2742;font-size:26rpx;height: 40rpx;line-height: 40rpx;">￥<text style="font-size:36rpx;">{{item.price}}</text><text style="color:#000;">抽选价</text></view>
                    <view class='dlfhtitlenew' style="color:#178FF8;font-size:26rpx;height: 40rpx;line-height: 40rpx;" wx:if='{{item.status==1}}'>
                      <block wx:if='{{item.specialWay > 0}}'>直播售卖</block>
                      <block wx:else>{{item.start_time}}抽选</block>
                    </view>
                    <view class='dlfhtitlenew' style="color:#4CC148;font-size:26rpx;height: 40rpx;line-height: 40rpx;" wx:elif='{{item.status==2&&item.sellNumber>0}}'>{{item.sellNumber}}人参加</view>
                    <view class='dlfhtitlenew' style="color:#999495;font-size:26rpx;height: 40rpx;line-height: 40rpx;" wx:elif='{{item.status==3}}'>已结束</view>
                </block>
                <!-- 抽盒 -->
                <block wx:elif='{{item.type==5}}'>
                    <view class='dlfhtitlenew' style="color:#FF2742;font-size:26rpx;height: 40rpx;line-height: 40rpx;">￥<text style="font-size:36rpx;">{{item.price}}</text></view>
                    <view class='dlfhtitlenew' style="color:#178FF8;font-size:26rpx;height: 40rpx;line-height: 40rpx;" wx:if='{{item.status==1}}'>
                      <block wx:if='{{item.specialWay > 0}}'>直播售卖</block>
                      <block wx:else>{{item.start_time}}开始</block>
                    </view>
                    <view class='dlfhtitlenew' style="color:#999495;font-size:26rpx;height: 40rpx;line-height: 40rpx;" wx:elif='{{item.status==2}}'>在线抽盒</view>
                    <view class='dlfhtitlenew' style="color:#999495;font-size:26rpx;height: 40rpx;line-height: 40rpx;" wx:elif='{{item.status==3}}'>已结束</view>
                </block>
                
            </view>

            <view class="boxAtlas" catchtap="jumpoffering" data-id="{{item.illustratedInfo.id}}" data-type='9035' style="width:500rpx;margin:20rpx;font-size:26rpx;" wx:if='{{item.illustratedInfo}}'>
                <!-- <view class="boxAtlasLeft">图鉴</view> -->
                <image class="boxAtlasLeftNimg" src="https://cdn.51chaidan.com/images/brandSettledIcon/illustrated.jpg"  mode="aspectFit"></image>
                <view class="boxAtlasName specialellipsis" style="width:288rpx;">{{item.illustratedInfo.title}}</view>
                <view class="boxAtlasRight">点击查看</view>
            </view>
            <!-- bottom -->
            <view style="min-height:40rpx;padding:20rpx;position: relative;">
                <view style="width:100%;min-height:40rpx;">
                      <view class="emlhltbottom" style="font-size:24rpx;height:40rpx;line-height:40rpx;">


                        <text style="float:right;margin-right:10rpx;">{{item.like_number>0?item.like_number:''}}</text>
                        <image style="margin:0 15rpx;" class="emlplefimgshare" src="{{item.is_like?'https://www.51chaidan.com/images/brand/red_heart.png':'https://www.51chaidan.com/images/brand/black_heart.png'}}"    data-type='{{item.type}}' data-ind='{{index}}' catchtap='followfun' data-id='{{item.id}}'></image>
                        
                        
                        <text class="emlplefttxt" catchtap="jumpoffering" data-id="{{item.id}}" data-selltype='{{item.specialWay}}' data-istype='{{item.type}}' style="float:right;" wx:if='{{item.commentNumber}}' >{{item.commentNumber?item.commentNumber:'0'}}</text>
                        <image class="emlplefimgshare" catchtap="jumpoffering" data-selltype='{{item.specialWay}}' data-id="{{item.id}}" data-istype='{{item.type}}' src="https://cdn.51chaidan.com/images/brandSettledIcon/comment.png"></image>


                      </view>
                </view>
            </view>


        </view> 
        <!--  暂无数据  -->
        <view class='noDataCom' wx:if='{{communityList.length==0&&nodataiftr}}'>
            <image src='https://cdn.51chaidan.com/images/brandInfoIcon/no-data.png' class='noDataImg'></image>
        </view>        
        
        <view style="width:100%;height:110rpx;"></view>

    </block>
    <!-- 瀑布流 -->
    <block wx:else>

      <view style='width:100%;overflow:hidden;'>
        <!-- 左边 -->
        <view class='dlfevecom' style='float:left;'>
          <view class='dlfeveleft' wx:for='{{communityList}}' wx:key='key' wx:if='{{index%2==0}}'>
            <view style='position:relative;margin-bottom:10rpx;'>
                <view style='width:360rpx;margin:auto;position:relative;height:340rpx;overflow: hidden;'  catchtap="jumpoffering" data-id="{{item.id}}" data-istype='{{item.type}}' data-selltype='{{item.specialWay}}'>
                  <!-- <view class='dlfchrbright' wx:if='{{item.type==3}}'>动态</view>
                  <view class='dlfchrbright' wx:elif='{{item.type==1}}' style="background:#F5B1AE;">秒杀</view>
                  <view class='dlfchrbright' wx:elif='{{item.type==2}}' style="background:#9BE2F8;">抽选</view>
                  <view class='dlfchrbright' wx:elif='{{item.type==4}}' style="background:#A9E6BA;">签到</view> -->

                  <image class="dlfchrbrightNimg" src="https://cdn.51chaidan.com/images/brandSettledIcon/dynamic.jpg" wx:if='{{item.type==3}}' mode="aspectFit"></image>
                  <block wx:elif='{{item.type==1}}'>
                      <!-- 限时 -->
                      <image wx:if='{{item.sellType == 1}}' class="dlfchrbrightNimg" src="https://cdn.51chaidan.com/images/brandSettledIcon/limitSeckill.jpg" mode="aspectFit"></image>
                      <!-- 售卖 -->
                      <image wx:elif='{{item.sellType == 0}}' class="dlfchrbrightNimg" src="https://cdn.51chaidan.com/images/brandSettledIcon/normalSeckill.jpg" mode="aspectFit"></image>
                  </block>

                  <!-- <image class="dlfchrbrightNimg" src="https://cdn.51chaidan.com/images/brandSettledIcon/" wx:elif='{{item.type==4}}' mode="aspectFill"></image> -->
                  <image class="dlfchrbrightNimg" src="https://cdn.51chaidan.com/images/brandSettledIcon/selection.jpg" wx:elif='{{item.type==2}}' mode="aspectFit"></image>
                  <image class="dlfchrbrightNimg" src="https://cdn.51chaidan.com/images/brandSettledIcon/showBox.jpg" wx:elif='{{item.type==5}}' mode="aspectFit"></image>
                  
                  <image data-ind='{{index}}' style='width:360rpx;height:340rpx;margin:auto;display:block;border-radius:10rpx 10rpx 0 0;' mode='aspectFit' src='{{item.imgArr[0]}}?imageMogr2/thumbnail/720x'></image>
                </view>
            </view>
            <view class='dlfhtitlenew' wx:if='{{item.type==4}}'>签到赢积分</view>
            <view class='dlfhtitlenew'>{{item.title}}</view>

            <view class='dlfhtitlenew' wx:if='{{item.temporary}}' style="color:#178FF8;">{{item.temporary}}</view>
            <!-- 动态 -->
            <block wx:elif='{{item.type==3}}'></block>
            <!-- 秒杀 -->
            <block wx:elif='{{item.type==1}}'>
                <block wx:if='{{item.status==1}}'>
                    <view class='dlfhtitlenew' style="color:#178FF8;">
                      <block wx:if='{{item.specialWay > 0}}'>直播售卖</block>
                      <block wx:else>{{item.start_time}}开始</block>
                    </view>
                </block>
                <block wx:if='{{item.status==2 ||item.status==3}}'>
                    <view class='dlfhtitlenew' style="overflow:hidden;">
                        <block wx:if='{{item.status==2}}'>
                            <view style="float:left;color:#4CC148;">进行中</view>
                        </block>
                        <block wx:if='{{item.status==3}}'>
                            <view style="float:left;color:#999495;">已结束</view>
                        </block>
                        <view style="float:right;color:#999495;" wx:if='{{item.sellNumber>0}}'>已售：{{item.sellNumber}}体</view>
                    </view>
                </block>
                <view class='dlfhtitlenew' style="color:#FF2742;">￥{{item.price}}<text style="color:#000;" wx:if='{{item.integralNumber}}'> +积分{{item.integralNumber}}</text></view>
            </block>
            <!-- 抽选 -->
            <block wx:elif='{{item.type==2}}'>
                <block wx:if='{{item.status==1}}'>
                    <view class='dlfhtitlenew' style="color:#178FF8;">
                      <block wx:if='{{item.specialWay > 0}}'>直播售卖</block>
                      <block wx:else>{{item.start_time}}开始</block>
                    </view>
                </block>
                <block wx:if='{{item.status==2 ||item.status==3}}'>
                    <view class='dlfhtitlenew' style="overflow:hidden;">
                        <block wx:if='{{item.status==2}}'>
                            <view style="float:left;color:#4CC148;">已发售</view>
                        </block>
                        <block wx:if='{{item.status==3}}'>
                            <view style="float:left;color:#999495;">已结束</view>
                        </block>
                        <view style="float:right;color:#999495;" wx:if='{{item.sellNumber>0}}'>{{item.sellNumber}}人参加</view>
                    </view>
                </block>
                <view class='dlfhtitlenew' style="color:#FF2742;">￥{{item.price}}<text style="color:#000;" wx:if='{{item.integralNumber}}'> +积分{{item.integralNumber}}</text></view>
            </block>
            <!-- 签到 -->
            <block wx:elif='{{item.type==4}}'></block>

            <!-- 抽盒 -->
            <block wx:elif='{{item.type==5}}'>
                <block wx:if='{{item.status==1}}'>
                    <view class='dlfhtitlenew' style="color:#178FF8;">
                      <block wx:if='{{item.specialWay > 0}}'>直播售卖</block>
                      <block wx:else>{{item.start_time}}开始</block>
                    </view>
                    <view class='dlfhtitlenew' style="color:#FF2742;">￥{{item.price}}</view>
                </block>
                <block wx:if='{{item.status==2 ||item.status==3}}'>
                    <view class='dlfhtitlenew' style="overflow:hidden;">
                        <block wx:if='{{item.status==2}}'>
                            <view style="float:left;color:#999495;">在线抽盒</view>
                        </block>
                        <block wx:if='{{item.status==3}}'>
                            <view style="float:left;color:#999495;">已结束</view>
                        </block>
                    </view>
                </block>
            </block>


            <view class="boxAtlas" wx:if='{{item.illustratedInfo}}' catchtap="jumpoffering" data-id="{{item.illustratedInfo.id}}" data-type='9035' data-selltype='{{item.specialWay}}'>
                <!-- <view class="boxAtlasLeft">图鉴</view> -->
                <image class="boxAtlasLeftNimg" src="https://cdn.51chaidan.com/images/brandSettledIcon/illustrated.jpg"  mode="aspectFit"></image>
                <view class="boxAtlasName specialellipsis">{{item.illustratedInfo.title}}</view>
                <view class="boxAtlasRight">点击查看</view>
            </view>
            <view class='dlfhpbox'>
              <view style='width:40rpx;height:40rpx;border-radius:50%;float:left;margin:8rpx 8rpx 0 8rpx;position: relative;'  data-id='{{item.brandInfo.brand_id}}' catchtap="jumpexhdetail">
                <image src='{{item.brandInfo.brand_logo}}' style="width:100%;height:100%;border-radius:50%;"></image>
                <view wx:if="{{item.isVip}}" class='vipBG'><image src='https://cdn.51chaidan.com/images/calendar/VipCase.png' style='position: relative;'></image></view>

                <image class='dlfchrtopimgipListp' wx:if='{{item.brandInfo.isAuth}}' mode="aspectFit" src='https://cdn.51chaidan.com/images/brandSettledIcon/approveIcon.png'></image>
              </view>
