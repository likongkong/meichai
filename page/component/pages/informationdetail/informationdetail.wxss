page{
  background:#f4f5f9;
}
.infodebox{
  height: 166rpx;
  padding: 0 20rpx;
  border-bottom: 1rpx solid #e5e5e5;
  background: #fff;
}
.infodeleft{
  width: 166rpx;
  height: 166rpx;
  float: left;
  position: relative;
}
.infoderight{
  width: 526rpx;
  height: 166rpx;
  float: right;
}
.infodelimg{
  position: absolute;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  margin: auto;
  height: 60rpx;
  width: 60rpx;
}
.infodelimgs{
  position: absolute;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  margin: auto;
  height: 100rpx;
  width: 100rpx;
}
.infodertime{
  width: 100%;
  height: 40rpx;
  line-height: 40rpx;
  text-align: right;
  font-size: 28rpx;
  color: #666;
}
.wdcrtxt{
  width: 100%;
  height: 70rpx;
  font-size: 26rpx;
  color: #666;
}
/*  双行  */
.multilineellipsis{
    display: -webkit-box;
    word-break: break-all;
    text-overflow: ellipsis;
    overflow: hidden;
    -webkit-box-orient: vertical;
    -webkit-line-clamp:2;   
}
.inifderimg{
  width: 12rpx;
  height: 20rpx;
  display: inline-block;
  margin-left:10rpx;
}







