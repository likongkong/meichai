/* 弹框 */
.searchtip{
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  z-index: 999;
}
.searchtipbg{
  width: 100%;
  height: 100%;
  background: #000;
  opacity: 0.6;
}
.searchtipcon{
  width: 500rpx;
  height: 600rpx;
  background: #fff;
  position: absolute;
  top: 0;
  left: 0;
  bottom: 0;
  right: 0;
  margin: auto; 
  overflow: scroll;
}
.searchtipcontxt{
  display: block;
  padding: 20rpx;
  font-size: 26rpx;
}









