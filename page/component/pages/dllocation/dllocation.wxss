}
.sonsoright{
  height: 100rpx;
  line-height: 100rpx;
  color: #f93f45;
  font-size: 24rpx;
  text-decoration:underline; 
  float: right;
  margin-right: 25rpx;
}
/*   单行  */
.specialellipsis{
  overflow:hidden;
