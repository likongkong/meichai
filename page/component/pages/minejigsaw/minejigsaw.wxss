/* pages/minejigsaw/minejigsaw.wxss */
@import "../../common/mPublic.wxss";

page{
  background:#F0EFF5;
}

.topbarstyle {
  width: 100%;
  height: 100rpx;
  position: fixed;
  top: 0;
  left: 0;
  z-index: 5;
}

view.color {
  border-bottom:1px solid #fff;
}

.barstyle {
  width: 70%;
  height: 80%;
  float: left;
  color: #fff;
  line-height: 80rpx;
  text-align: center;
  margin-left:15%;
}

navigator {
  width: 38rpx;
  height: 36rpx;
  float: left;
  margin-top: 32rpx;
  background: #ff6968;
  margin-left: 35rpx;
}

navigator:after {
  content: '';
  display: block;
  width: 25rpx;
  height: 25rpx;
  border-right: 4rpx solid #fff;
  border-top: 4rpx solid #fff;
  -webkit-transform: rotate(225deg);
  /*箭头方向可以自由切换角度*/
  transform: rotate(225deg);
}