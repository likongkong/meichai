@import "../../../../pages/activitysharinglist/activitysharinglist.wxss";
@import "../../../../common/tabbar.wxss";
page {
  background: #f5f6fa;
}
.limlistbox{
  width: 100%;
}
.limitlbut{
  width: 100%;
  overflow: hidden;
  padding-bottom: 16rpx;
  
}
.limieve{
  width: 344rpx;
  float: left;
  margin-right: 20rpx;
  background: #fff;
  border-radius: 10rpx;
  margin-bottom: 20rpx;
  position: relative;
}
.limieveimg{
  width: 100%;
  height: 183.4rpx;
  display: block;
  border-radius: 10rpx 10rpx 0 0;
}
.limievetxtname{
  color:#000;height:70rpx;width:93%;margin:0 auto;font-size: 24rpx;font-weight: bold;
}
.limievetxttime{
  width: 93%;
  height: 50rpx;
  font-size: 22rpx;
  color: #ccc;
  margin:0 auto;
}

.labelstyle {
  width: 155rpx;
  line-height: 30rpx;
  opacity: 0.9;
  position: absolute;
  top: 10rpx;
  right: 10rpx;
  color: #fff;
  border-radius: 7rpx;
  font-size:25rpx;
  text-align:center;
}
.limstorlogo{
  width: 30rpx;
  height: 30rpx;
  display: inline-block;
  border-radius: 50%;
  margin: 7rpx 5rpx 0 5rpx;
  vertical-align: top;
}




















