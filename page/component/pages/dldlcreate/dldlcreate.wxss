/*  双行  */
.multilineellipsis{
  display: -webkit-box;
  word-break: break-all;
  text-overflow: ellipsis;
  overflow: hidden;
  -webkit-box-orient: vertical;
  -webkit-line-clamp:2;   
}
.orderconttop{
  width: 100%;
  height:75rpx;
  position: relative;
}
.orderconttopimg{
  width:28rpx;
  height:28rpx;
