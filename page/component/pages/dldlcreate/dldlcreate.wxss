}

.creatPrize{
  position: absolute;
  top: 0;
  left: 0;
  height: 30rpx;
  line-height: 30rpx;
  font-size: 20rpx;
  color: #fff;
  padding: 0 10rpx;
  background: #ff3624;
}

/*   单行  */
.specialellipsis{
  overflow:hidden;
  text-overflow:ellipsis;
  white-space:nowrap;  
}

.dldlpoistioclick{
  width: 100%;
