page{
  background: #fff;
  width:100%;
  
}

.fragment{
  height:40rpx;
  position: absolute;
  bottom:15rpx;
  z-index: 3;
}
.fragmentbg{
  width:100%;
  height: 100%;
  background: #000;
  opacity: 0.6;
}
.fragmenttxt{
  width: 100%;
  height:40rpx;
  text-align: center;
  font-size: 20rpx;
  color: #ecc65c;
  position: absolute;
  top: 0;
  left: 0;
  vertical-align: top;
}
.fragmentimg{
  width: 16rpx;
  height: 30rpx;
  display: inline-block;
  vertical-align: top;
  margin:6rpx 5rpx 0 0;
}
.fragmentfont{
  height: 40rpx;
  display: inline-block;
  line-height: 40rpx;
}
.jumpAreward{
  position: absolute;
  right: 0;
  bottom: 0;
  height:60rpx;line-height:60rpx;
  font-size:24rpx;
  color:#1890ff;
}




