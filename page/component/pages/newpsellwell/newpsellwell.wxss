@import "../../../../pages/index/index.wxss";
@import "../../../../pages/dismantlingbox/dismantlingbox.wxss";
page{
  background: #f4f5f9;
} 
.box {
  background:#f4f5f9;
  padding-top:0;
  padding-bottom:10rpx;
}
/*  首页按钮  */
.frontpagebutton{position: fixed;right: 20rpx;bottom: 226rpx;z-index: 5;width: 80rpx;height: 80rpx;font-size: 28rpx;background: #000;opacity: 0.5;text-align: center;color: #fff;border-radius: 50%;display: flex;align-items: center;justify-content: center;}
/*  双行  */
.multilineellipsis{
  display: -webkit-box;
  word-break: break-all;
  text-overflow: ellipsis;
  overflow: hidden;
  -webkit-box-orient: vertical;
  -webkit-line-clamp:2;   
}
.disintegral{
  width: 100%;
  height: 50rpx;
  line-height: 50rpx;
  text-align: center;
  font-size: 24rpx;
  color: #fff;
}
.disiconcgol{
  width: 40rpx;
  height: 40rpx;
  display: inline-block;
  vertical-align:top;
  position: relative;
  top: 5rpx;
}
.integralposi{
  width: 100%;
  height: 100rpx;
  background: #fff;
  position: fixed;
  left: 0;
  bottom: 0;
  z-index: 8;
}
.integralposileft{
  height: 100rpx;
  line-height: 100rpx;
  float: left;
  color: #000;
  font-size: 26rpx;
  padding: 0 0 0 16rpx;
}
.integralposiright{
    position:absolute;
    top:0;
    bottom: 0;
    right:28rpx;
    margin: auto 0;
    height:60rpx;
    font-size:28rpx;
    color:#ff2742;
    background:#fff;
    text-align:center;
    line-height:60rpx;
    border:1rpx solid #ff2742;
    border-radius:10rpx;
    padding:0 20rpx;
}
.ranKingList{
  width: 40rpx;
  height: 40rpx;
  position: absolute;
  left: 0;
  top: 13rpx;
  z-index: 3;
}
.ranKingImg{
  width: 100%;
  height: 100%;
  display: block;
}
.ranKingNum{
  width: 40rpx;
  height: 40rpx;
  line-height:35rpx;
  text-align: center;
  font-size: 24rpx;
  color: #fff;
  position: absolute;
  left: 0;
  top: 0;
  z-index: 3;
} 
.blessingBagWrap{
  text-align: center;
  padding-bottom: 50rpx;
}
.blessingBag{
  margin-bottom: 18rpx;
  display: inline-block;
  background: transparent;
  margin: 0 12rpx;
}
.blessingBag image{
  width: 340rpx;
  border-radius: 20rpx;
  display: block;
  margin-bottom: 25rpx;
}
.mobilePhoneShell{
  padding-top: 30rpx;
}
.mobilePhoneShellEve{
  width: 700rpx;
  margin: 0 auto 30rpx;
  border-radius: 20rpx;
  background: #fff;
  padding-bottom: 10rpx;
}
.mobilePhoneShellEve image{
  width: 100%;
  display: block;
  border-radius: 20rpx 20rpx 0 0;
}
.mpsName{
  font-size: 24rpx;
  padding: 10rpx 20rpx;
}
.mpsNameeve{
  padding: 4rpx 20rpx;
  font-size:24rpx;
  border-radius: 20rpx;
  display: inline-block;
}