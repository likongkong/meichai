.rofcobox{
  width: 100%;
}
.rofcohead{
  width: 100%;
  height: 300rpx;
  background: #ff2742;
}
.rofcheadtxt{
  width: 100%;
  height: 120rpx;
  line-height: 120rpx;
  text-align: center;
  color: #fff;
  font-size: 26rpx;
  position: relative;
}
.rofcheadnum{
  width: 100%;
  height: 70rpx;
  color: #fff;
  font-size: 46rpx;
  text-align: center;
  
}
.rofccontent{
  width: 100%;
}
.rofcceve{
  margin:0 30rpx;
  height: 130rpx;
  border-bottom:1rpx solid #a9a9a9; 
  position: relative;
}
.rofccevehead{
  margin: 0 50rpx;
  height: 80rpx;
  line-height: 80rpx;
  color: #000;
  font-size: 30rpx;
  position: relative;
}
.rofccevecirco{
  width: 24rpx;
  height: 24rpx;
  background: #000;
  border-radius: 50%;
  position: absolute;
  top: 0;
  bottom: 0;
  left: -40rpx;
  margin: auto 0;
}
.rofccevedata{
  margin: 0 50rpx;
  height: 50rpx;
  font-size: 26rpx;
  color: #999999;
}
.rofcceveright{
  height: 120rpx;
  font-size: 26rpx;
  color: #f85441;
  line-height: 120rpx;
  position: absolute;
  right: 0;
  top: 0;
}








