@import "../../../../pages/index/index.wxss";
.box{
  padding-top:90rpx; 
}
.sousuo{
  height: 60rpx;
  width: 94%;
  padding: 14rpx 3%;
  position: fixed;
  left: 0;
  background: #f4f5f9; 
  z-index: 99;
}
.indheadinpw{
  width: 77%;
  padding-left: 3%;
  height: 60rpx;
  background: #fff;
  color: #666;
  border-radius: 10rpx;
  float: left;
  font-size: 26rpx;
}
.clicksousuo{
  height: 60rpx;
  width: 16%;
  text-align: center;
  line-height: 60rpx;
  font-size: 26rpx;
  color: #333;
  float: right;
}
.hotseavoc{
  width: 100%;
  background: #fff;
  margin-top: 14rpx;
}

.nodata{
  width: 100%;
  background: #f1f1f1;
  padding: 50rpx 0;
}
.suosuoimgno{
  width: 120rpx;
  height: 160rpx;
  display: block;
  margin:40rpx auto 0;
}
.nodatasuous{
  width: 100%;
  height: 40rpx;
  text-align: center;
  line-height: 40rpx;
  font-size: 24rpx;
  color: #999;
}
/* 弹框 */
.searchtip{
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  z-index: 999;
}
.searchtipbg{
  width: 100%;
  height: 100%;
  background: #000;
  opacity: 0.6;
}
.searchtipcon{
  width: 500rpx;
  height: 600rpx;
  background: #fff;
  position: absolute;
  top: 0;
  left: 0;
  bottom: 0;
  right: 0;
  margin: auto; 
  overflow: scroll;
}
.searchtipcontxt{
  display: block;
  padding: 20rpx;
  font-size: 26rpx;
}


.iconSoldOut{
  width:116rpx;
  height:65rpx;
  position: absolute;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  margin: auto;
  z-index: 4;
}

.bclefticon{
  width:100%;
  height: 100%;
  position: absolute;
  top: 0;
  left: 0;
  background: #fff;
  opacity: 0.6;
  z-index: 2;
}




