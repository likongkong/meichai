<canvas canvas-id='myordercanimgser' class='myordercanimgsernew'></canvas>
<!-- <loading hidden="{{headhidden}}" >
    加载中...
</loading>   -->
<t_f title='{{c_title}}' arrow='{{c_arrow}}' backcolor='{{c_backcolor}}'></t_f>
<view style='width:100%;height:{{statusBarHeightMc}}px;'></view>

<canvas canvas-id='canimgserceshi' style='width:375px;height:788px;position:fixed;left:0;top:-999999rpx;'></canvas>

<view class='box'
    style="{{upserimgbox||upserimgboxact?'position:fixed;top:0;left:0;right:0;bottom:0;':''}}padding-bottom:110rpx;">


    <!-- 电子 -->
    <block wx:if='{{comdata.isToyshowDrawGoods}}'>

        <view class="ordeQRcode" wx:if='{{comdata.status == 2}}'>
            <view class="ordeQRImgBox">
                <image src="{{comdata.qrcode}}" class="ordeQRImgBoxImg"></image>
                <view wx:if='{{comdata.isItOverdue}}' class='bgctipref'>已过期</view>
            </view>
            <view class="ordeQRTitN" style="text-align:center;border-top:0;">需凭本人身份证线下扫码领取<text
                    wx:if='{{comdata.drawDate}}'>2021.1.{{comdata.drawDate}}</text></view>
        </view>

        <view wx:if='{{comdata.status==5}}' class='odchtead'>
            <view style='float:left;font-size:24rpx;'>订单编号:{{comdata.order_sn}}</view>
            <view class='wholeconnumrrbox'>
                <view class='wholeconnumrr'>
                    已领取
                </view>
            </view>
        </view>

        <view class='orderdetcon'>
            <view class='odchteadcom' data-gid='{{comdata.gid}}' data-order_type='{{comdata.order_type}}'
                data-activity_id='{{comdata.activity_id}}'
                style="{{comdata.status==5?'border-top: 1rpx solid #e5e5e5;':'border-top:0;'}}"
                bindtap='addressmanagement'>
                <view class='odchcomleft'>
                    <view class='odchlefimg'>
                        <image src='{{comdata.gcover}}' bindload="imageLoadad"
                            style='height: 100%;width:{{comdata.width||140}}rpx;margin:0 auto;display:{{comdata.width?"block":"none"}};'
                            mode='aspectFill'></image>
                    </view>
                </view>
                <view class='odchcomright' style='position:relative;' wx:if='{{comdata.order_type!=7}}'>
                    <view class='odchrhead multilineellipsis'>{{comdata.gname}}</view>
                    <view class='wholeconnum specialellipsis' wx:if='{{comdata.prize}}' style='padding-top:10rpx;'>
                        奖品:{{comdata.prize}}</view>
                    <view class='wholeconnum specialellipsis'>
                        <view class='wholeconnumdisilin'>
                            <block wx:if='{{comdata.order_type==1&&comdata.suitName&&comdata.suitNumber}}'>
                                {{comdata.suitName}}x{{comdata.suitNumber}}
                            </block>
                            <block wx:else>数量:x{{comdata.gnumber}}<text wx:if='{{comdata.roleName}}'
                                    style='color:#ff2840;margin-left:15rpx;'>款式:{{comdata.roleName}}</text></block>
                        </view>
                        <view class='wholeconnumdisilin' wx:if='{{comdata.size}}'>尺寸:{{comdata.size}}</view>
                        <view class='wholeconnumdisilin' wx:if='{{comdata.color}}'>颜色:{{comdata.color}}</view>
                    </view>
                    <view class='wholeconnum specialellipsis'
                        wx:if='{{comdata.customer_note&&(comdata.status==1||comdata.status==2||comdata.status==12||comdata.status==13||comdata.status==3||comdata.status==4)}}'>
                        备注:{{comdata.customer_note}}</view>
                    <view class='officialreply'
                        wx:if='{{comdata.customer_reply&&(comdata.status==1||comdata.status==2||comdata.status==12||comdata.status==13||comdata.status==4||comdata.status==16)}}'>
                        回复:{{comdata.customer_reply}}</view>
                    <view class='taxrpice'>
                        <view class='taxpricleft' wx:if='{{!comdata.roleName}}'>
                            <block wx:if='{{comdata.order_type==1&&comdata.goods_price==0}}'>组合赠品</block>
                            <block wx:elif='{{comdata.order_type==3}}'>拆币兑换</block>
                            <block wx:elif='{{comdata.order_type==4}}'>邀请奖励</block>
                            <block wx:elif='{{comdata.order_type==2||comdata.order_type==9}}'>免费</block>
                            <block wx:elif='{{comdata.order_type==33&&comdata.amount==0}}'>免费</block>
                            <block wx:elif='{{comdata.amount==0}}'>免费</block>
                            <block
                                wx:elif='{{comdata.order_type==1&&comdata.suitPrice&&comdata.suitName&&comdata.suitNumber}}'>
                                ￥{{comdata.suitPrice}}</block>
                            <block wx:else>￥{{comdata.goods_price}}</block>
                        </view>
                    </view>

                </view>

            </view>
            <view class='totalmon'>

                <view class='totalmonnum' wx:if='{{comdata.order_type==1}}'>
                    合计:￥{{comdata.amount}}
                </view>

                <view class='daifukuan' style='text-align:left;'
                    wx:if='{{comdata.order_type==6&&(comdata.status==18||comdata.status==0)}}'>
                    结束时间 : {{comdata.gift_time}}
                </view>

            </view>

            <view style="padding:0 0 20rpx 0;background:#fff;">
                <view class="ordeQRTitle" style="border-top: 1rpx solid #e5e5e5;">
                    <view class="ordeQRTitN"><text style="color:#666;">配送方式：</text>线下展会凭本人身份证领取</view>
                </view>
            </view>

            <view style="padding:0 0 20rpx 0;background:#fff;">
                <view class="ordeQRTitle">
                    <view class="ordeQRTitN"><text
                            style="color:#666;display:inline-block;vertical-align:top;width:120rpx;">参展人：</text>{{comdata.consignee}}
                        {{comdata.mobile}}</view>
                    <view class="ordeQRTitN"><text
                            style="color:#666;display:inline-block;vertical-align:top;width:120rpx;"></text>身份证
                        {{comdata.idcard}}</view>
                </view>
            </view>


        </view>




    </block>
    <!-- 普通 -->
    <block wx:else>

        <view wx:if='{{comdata.order_type!=6}}'>
            <view class='rdheadback' wx:if='{{comdata.status==0&&comdata.order_type!=7}}'>
                <block wx:if='{{comdata.order_type==24&&(comdata.isBalances==1||comdata.isBalances==2)}}'>已付定金</block>
                <block wx:else>等待付款</block>
                <image class='rdheadimg' src='../../../../pages/images/icon_wallet2.png'></image>
            </view>
            <view class='rdheadback' wx:if='{{comdata.status==0&&comdata.order_type==7}}'>
                进行中
                <image class='rdheadimg' src='../../../../pages/images/icon_wallet2.png'></image>
            </view>
            <view class='rdhead' wx:if='{{comdata.status==2}}'>
                支付成功
                <image class='rdheadimg' src='../../../../pages/images/icon_clock2.png'></image>
            </view>
            <view class='rdhead' wx:if='{{comdata.status==3}}'>
                等待发货
                <image class='rdheadimg' src='../../../../pages/images/icon_package2.png'></image>
            </view>

            <view class='rdhead' wx:if='{{comdata.status==4}}'>
                等待收货
                <image class='rdheadimg' src='../../../../pages/images/icon_car4.png'></image>
            </view>
            <view class='rdhead' wx:if='{{comdata.status==5}}'>
                交易成功
                <image class='rdheadimg' src='../../../../pages/images/icon_complete.png'></image>
            </view>
            <view class='rdheadback' wx:if='{{comdata.status==7}}'>
                已退款
                <image class='rdheadimg' src='../../../../pages/images/icon_failed.png'></image>
            </view>
            <view class='rdheadback' wx:if='{{comdata.status==8}}'>
                <block wx:if='{{comdata.isBalances==3}}'>奖品已过期</block>
                <block wx:elif='{{comdata.order_type!=7}}'>取消未付款</block>
                <block wx:else>已取消订单</block>
                <image class='rdheadimg' src='../../../../pages/images/icon_failed.png'></image>
            </view>
        </view>

        <!-- {{comdata.order_type!=7&&comdata.status!=16&&comdata.status!=1&&comdata.status!=8&&comdata.status!=11}}   -->
        <block wx:if="{{comdata.consignee != 0 && comdata.consignee != ''}}">
            <view
                wx:if='{{comdata.order_type!=7&&(comdata.status!= 0||comdata.status!=8)&&(comdata.order_type!=2&&(comdata.status!=1||comdata.status!=16||comdata.status!=11))}}'
                class='boxch'>
                <view class='ordetaaddleft'>
                    <image class='orderaddimg' src='../../../../pages/images/icon_address2.png'></image>
                </view>
                <view class='ordetaaddright'>
                    <view class='ordaddhead'>
                        <view class='odhleft'>收货人:{{comdata.consignee}}</view>
                        <view class='odhright'>{{comdata.mobile}}</view>
                    </view>
                    <view class='ordaddcon'>收货地址:<block
                            wx:if='{{comdata.province!=0 || comdata.city!=0 || comdata.district!=0 || comdata.address!=0}}'>
                            {{comdata.province}} {{comdata.city}} {{comdata.district}} {{comdata.address}}</block>
                        <block wx:else>暂未填写</block>
                    </view>

                </view>
            </view>
        </block>
 
        <view class='addressmodify' wx:if='{{comdata.canChangedAddress&&comdata.status!=20}}'>
            <block wx:if='{{comdata.province!=0 || comdata.city!=0 || comdata.district!=0 || comdata.address!=0}}'>注:每个订单只可修改一次收货地址<view class='modifaddclick' catchtap='modaddfun'>修改地址</view></block>
            <block wx:else>注:每个订单只可添加一次收货地址<view class='addmodifaddclick' catchtap='modaddfun'>添加地址</view></block>
        </view>

        <view wx:if='{{comdata.order_type==7&&comdata.status==2}}' class='boxch'>
            <view class='ordetaaddleft'>
                <image class='orderaddimg' src='../../../../pages/images/icon_address2.png'></image>
            </view>
            <view class='ordetaaddright'>
                <view class='ordaddhead'>
                    <view class='odhleft'>收货人:{{comdata.consignee}}</view>
                    <view class='odhright'>{{comdata.mobile}}</view>
                </view>
                <view class='ordaddcon'>收货地址:{{comdata.province}} {{comdata.city}} {{comdata.district}}
                    {{comdata.address}}
                </view>
            </view>
        </view>

        <view class='odchtead' wx:if='{{comdata.order_type!=6 && comdata.auditRefund != 2 && comdata.auditRefund != 3}}'>
            <view class='odchteadleft'>{{comdata.retailer}}</view>
            <view class='wholeimgdel' data-oid='{{comdata.cart_id}}' bindtap='delwholeheadrimg'
                wx:if='{{comdata.status==8||comdata.status==5||comdata.status==10||comdata.status==11}}'>
                <image src='../../../../pages/images/icon_delete.png' class='wholeheadrimg'></image>
            </view>
            <view class='wholeconnumrrbox'>
                <view class='wholeconnumrr' style='margin-right: 13rpx;'
                    wx:if='{{(comdata.status!=0 && comdata.status!=1 && comdata.status!=10 &&comdata.status!=7&& comdata.status!=11&&comdata.status!=8&&comdata.status!=16&&comdata.status!=17) && comdata.order_type==2}}'>
                    免单成功</view>
                <view class='wholeconnumrr' wx:if='{{comdata.status==0 && comdata.order_type!=7}}'>
                    <block wx:if='{{comdata.order_type==24}}'>
                        <block wx:if='{{comdata.status==0&&(comdata.isBalances==1||comdata.isBalances==2)}}'>已付定金
                        </block>
                        <block wx:elif='{{comdata.status==8&&comdata.isBalances==3}}'>奖品过期已取消</block>
                    </block>
                    <block wx:else>未付款</block>
                </view>
                <view class='wholeconnumrr' style='color:#259B24;'
                    wx:if='{{comdata.status==0 && comdata.order_type==7}}'>
                    结束时间:{{comdata.overtime}}
                </view>
                <view class='wholeconnumrr' wx:if='{{comdata.status==1&&comdata.order_type!=2}}'>
                    已付款
                </view>
                <view class='wholeconnumrr' wx:if='{{comdata.status==1&&comdata.order_type==2}}'>
                    <block wx:if='{{comdata.isAuditPic==1}}'>
                        <block wx:if='{{comdata.auditPic==0}}'>报名未完成,等待上传截图</block>
                        <block wx:if='{{comdata.auditPic==1}}'>上传完成,等待审核</block>
                        <block wx:if='{{comdata.auditPic==2}}'><text style='color:#42bd56;'>审核成功,等待开奖</text></block>
                        <block wx:if='{{comdata.auditPic==3}}'>审核失败,重新上传</block>
                    </block>
                    <block wx:else>报名成功 等待开奖</block>
                </view>
                <view class='wholeconnumrr' wx:if='{{comdata.status==2&&comdata.order_type==1}}'>
                    支付成功
                    <!-- <block wx:if='{{comdata.order_type==2}}'><text style='color:#42bd56;'>恭喜您中奖,等待收货</text></block>
                              <block wx:else>拆单成功</block> -->
                </view>
                <view class='wholeconnumrr' style='color:#259B24;'
                    wx:if='{{comdata.status==2 && comdata.order_type==7}}'>
                    活动结束，等待发货
                </view>
                <view class='wholeconnumrr' wx:if='{{comdata.stockup==0&&comdata.status==3}}'>
                    等待发货
                </view>
                <view class='wholeconnumrr' wx:if='{{comdata.status==4}}'>
                    等待收货
                </view>
                <view class='wholeconnumrr' wx:if='{{comdata.status==5}}'>
                    已完成
                </view>
                <view class='wholeconnumrr' wx:if='{{comdata.status==6}}'>
                    售后
                </view>
                <view class='wholeconnumrr' wx:if='{{comdata.status==7}}'>
                    已退款
                </view>
                <view class='wholeconnumrr' wx:if='{{comdata.status==8 && comdata.order_type!=7}}'>
                    取消未付款
                </view>
                <view class='wholeconnumrr' style='color:#7D7D7D;'
                    wx:if='{{comdata.status==8 && comdata.order_type==7}}'>
                    活动结束，订单关闭
                </view>
                <view class='wholeconnumrr' wx:if='{{comdata.status==10}}'>
                    报名失败
                </view>
                <view class='wholeconnumrr' wx:if='{{comdata.status==11}}'>
                    免单失败
                </view>
                <view class='wholeconnumrr' wx:if='{{comdata.status==12}}'>
                    正在备货
                </view>
                <view class='wholeconnumrr' wx:if='{{comdata.status==13}}'>
                    等待发货
                </view>
                <view class='wholeconnumrr' wx:if='{{comdata.status==14}}'>
                    下单成功
                </view>
                <view class='wholeconnumrr' wx:if='{{comdata.status==15}}'>
                    下单失败
                </view>
                <view class='wholeconnumrr'
                    wx:if='{{comdata.status==16 && comdata.order_type!=7 && comdata.order_type!=10&& comdata.order_type!=16}}'>
                    <block wx:if='{{comdata.order_type==9}}'>购买商品领奖</block>
                    <block wx:elif='{{comdata.showOrder==0}}'>已中奖，等待晒单</block>
                    <block wx:elif='{{comdata.showOrder==1}}'>已上传，待审核</block>
                    <block wx:elif='{{comdata.is_receive==2}}'>已领奖，等待发货</block>
                    <block wx:elif='{{comdata.showOrder==2}}'>晒单成功，等待领奖</block>
                    <block wx:elif='{{comdata.showOrder==3}}'>晒单失败，重新上传</block>
                    <block wx:elif='{{comdata.showOrder==4}}'>上传晒单截图</block>
                </view>
                <view class='wholeconnumrr'
                    wx:if='{{comdata.status==16 && comdata.order_type==10&& comdata.order_type!=16}}'>
                    <block wx:if='{{comdata.showOrder==1}}'>已上传，待审核</block>
                    <block wx:elif='{{comdata.showOrder==2}}'>晒单成功，等待领奖</block>
                    <block wx:elif='{{comdata.showOrder==3}}'>晒单失败，重新上传</block>
                    <block wx:else='{{comdata.showOrder==3}}'>晒单后领奖</block>
                </view>
                <view class='wholeconnumrr' style='color:#FF9800;'
                    wx:if='{{comdata.status==16 && comdata.order_type==7}}'>
                    活动结束，等待领取
                </view>
                <view class='wholeconnumrr' wx:if='{{comdata.status==17&& comdata.order_type!=7}}'>
                    奖品过期已取消
                </view>
                <view class='wholeconnumrr' style='color:#7D7D7D;'
                    wx:if='{{comdata.status==17 && comdata.order_type==7}}'>
                    活动结束，订单关闭
                </view>
                <view class='wholeconnumrr' wx:if='{{comdata.stockup==1&&comdata.status==3}}'>
                    备货中
                </view>
                <!-- 已回收 -->
                <view class='wholeconnumrr' style="color:#666;" wx:if='{{comdata.status==20}}'>
                    已回收
                </view>
            </view>

        </view>

        <!-- 为了不把原来逻辑弄乱 ，所以砍价单写 -->
        <view wx:if='{{comdata.order_type==6}}' class='odchtead'>
            <view style='float:left;font-size:24rpx;'>订单编号:{{comdata.order_sn}}</view>
            <view class='wholeconnumrrbox'>
                <view class='wholeconnumrr' wx:if='{{comdata.status==18}}'>
                    砍价中
                </view>
                <view class='wholeconnumrr' wx:if='{{comdata.status==0}}'>
                    砍价成功
                </view>
                <view class='wholeconnumrr' wx:if='{{comdata.status==2}}'>
                    已领取
                </view>
                <view class='wholeconnumrr' wx:if='{{comdata.status==3}}'>
                    待发货
                </view>
                <view class='wholeconnumrr' wx:if='{{comdata.status==4}}'>
                    待收货
                </view>
                <view class='wholeconnumrr' wx:if='{{comdata.status==5}}'>
                    已完成
                </view>
                <view class='wholeconnumrr' wx:if='{{comdata.status==17}}'>
                    砍价失败
                </view>
                <view class='wholeconnumrr' wx:if='{{comdata.status==12}}'>
                    采购中
                </view>
            </view>
        </view>


        <block wx:if='{{!comdata.isToyCabinet}}'>
            <!--  商品状态  -->
            <view class='logisticsstate'
                wx:if='{{comdata.order_type!=6&&(comdata.store_id==0&&comdata.status!=0&&comdata.status!=10&&comdata.status!=11&&comdata.status!=17&&comdata.status!=16&&comdata.status!=1&&comdata.status!=8) && comdata.auditRefund != 2 && comdata.auditRefund != 3}}'>
                <!--  status 4,5,6 全亮  -->
                <view class='iftrlogisticsstate' wx:if='{{comdata.status==4||comdata.status==5||comdata.status==6}}'>
                    <view class='logticstache'>
                        <view class='logticstachetxt' wx:if='{{comdata.order_type==2}}'>免单成功</view>
                        <view class='logticstachetxt' wx:else>支付成功</view>
                        <view class='logticstachetxtimg'>
                            <image src='../../../../pages/images/statemyorder1.png' class='statemyorder composition'>
                            </image>
                            <image src='../../../../pages/images/statemybottom1.png' class='statemybottom composition'>
                            </image>
                        </view>
                    </view>
                    <view class='logticstache'>
                        <view class='logticstachetxt'>正在备货</view>
                        <view class='logticstachetxtimg'>
                            <image src='../../../../pages/images/statemyorder1.png' class='statemyorder composition'>
                            </image>
                            <image src='../../../../pages/images/statemybottom1.png' class='statemybottom composition'>
                            </image>
                        </view>
                    </view>
                    <view class='logticstache'>
                        <view class='logticstachetxt'>等待发货</view>
                        <view class='logticstachetxtimg'>
                            <image src='../../../../pages/images/statemyorder1.png' class='statemyorder composition'>
                            </image>
                            <image src='../../../../pages/images/statemybottom1.png' class='statemybottom composition'>
                            </image>
                        </view>
                    </view>
                    <view class='logticstache'>
                        <view class='logticstachetxt'>商品发货</view>
                        <view class='logticstachetxtimg'>
                            <image src='../../../../pages/images/statemyorder1.png' class='statemyorder composition'>
                            </image>
                            <image src='../../../../pages/images/statemybottom1.png' class='statemybottom composition'>
                            </image>
                        </view>
                    </view>
                </view>
                <!--  status 1,7,8,15,16,17 全暗  -->
                <view class='iftrlogisticsstate'
                    wx:elif='{{comdata.store_id==0&&comdata.status==1||comdata.status==7||comdata.status==8||comdata.status==15||comdata.status==16||comdata.status==17}}'>
                    <view class='logticstache'>
                        <view class='logticstachetxt' wx:if='{{comdata.order_type==2}}'>免单成功</view>
                        <view class='logticstachetxt' wx:else>支付成功</view>
                        <view class='logticstachetxtimg'>
                            <image src='../../../../pages/images/statemyorder0.png' class='statemyorder composition'>
                            </image>
                            <image src='../../../../pages/images/statemybottom0.png' class='statemybottom composition'>
                            </image>
                        </view>
                    </view>
                    <view class='logticstache'>
                        <view class='logticstachetxt'>正在备货</view>
                        <view class='logticstachetxtimg'>
                            <image src='../../../../pages/images/statemyorder0.png' class='statemyorder composition'>
                            </image>
                            <image src='../../../../pages/images/statemybottom0.png' class='statemybottom composition'>
                            </image>
                        </view>
                    </view>
                    <view class='logticstache'>
                        <view class='logticstachetxt'>等待发货</view>
                        <view class='logticstachetxtimg'>
                            <image src='../../../../pages/images/statemyorder0.png' class='statemyorder composition'>
                            </image>
                            <image src='../../../../pages/images/statemybottom0.png' class='statemybottom composition'>
                            </image>
                        </view>
                    </view>
                    <view class='logticstache'>
                        <view class='logticstachetxt'>商品发货</view>
                        <view class='logticstachetxtimg'>
                            <image src='../../../../pages/images/statemyorder0.png' class='statemyorder composition'>
                            </image>
                            <image src='../../../../pages/images/statemybottom0.png' class='statemybottom composition'>
                            </image>
                        </view>
                    </view>
                </view>
                <!--  status 2,14 亮1一个  -->
                <view class='iftrlogisticsstate'
                    wx:elif='{{comdata.store_id==0&&comdata.status==2||comdata.status==14}}'>
                    <view class='logticstache'>
                        <view class='logticstachetxt' wx:if='{{comdata.order_type==2&&comdata.status==2}}'>免单成功</view>
                        <view class='logticstachetxt' wx:elif='{{comdata.status==14}}'>下单成功</view>
                        <view class='logticstachetxt' wx:else>支付成功</view>
                        <view class='logticstachetxtimg'>
                            <image src='../../../../pages/images/statemyorder1.png' class='statemyorder composition'>
                            </image>
                            <image src='../../../../pages/images/statemybottom1.png' class='statemybottom composition'>
                            </image>
                        </view>
                    </view>
