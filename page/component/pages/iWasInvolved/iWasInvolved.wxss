@import "../../../../pages/signin/signin.wxss";
@import "../../../../common/newcoupon.wxss";
@import "../../../../common/tabbar.wxss";

page{
  background:#f4f5f9;
  padding-bottom: 110rpx;
}
.iwashead{
  width: 100%;
  height:240rpx;
  background: #fff;
}
.iwashtop{
  padding: 0 20rpx;
  height: 60rpx;
  line-height: 60rpx;
  font-size: 24rpx;
}
.iwasscroll{
  min-height: 80rpx;
  width:100%;
  white-space: nowrap;
}
.iwasscrollEve{
  height:200rpx;
  display: inline-block;
  position: relative;
  vertical-align: top;
  margin-left: 20rpx;
  
}
.iwassEveImg{
  height: 120rpx;
  display: block;
}
.iwassEveTxt{
  width: 100%;
  height: 60rpx;
}
.iwassEveNum{
  height: 60rpx;
  font-size: 24rpx;
  line-height: 60rpx;
  float: left;
  margin-left:5rpx;
}
.brandTicImg{
  width: 40rpx;
  height: 40rpx;
  float: right;
  margin: 10rpx 5rpx 0 0;
}
.iwasTitle{
  height: 80rpx;
  line-height: 80rpx;
  padding:0 20rpx;
  font-size: 26rpx;
}
.taskEve{
  padding: 0 20rpx;
  background: #fff;
  height: 100rpx;
  margin-bottom: 10rpx;
  position: relative;
}
.taskEveLeft{
  float: left;
}
.telTitle{
  font-size: 24rpx;
  color: #000;
  padding:15rpx 0 10rpx 0;
}
.telTitleNum{
  font-size: 24rpx;
  color: #666;
}

.clickButton{
  position: absolute;
  right: 20rpx;
  bottom: 0;
  top: 0;
  margin:auto;
  z-index: 1;
  border-radius: 10rpx;
  height:46rpx;
  text-align:center;
  line-height:46rpx;
  color: #fff;
  font-size: 24rpx;
  width: 130rpx;
  background: #E94F57;
}


.iwasButon{
  position: fixed;
  left: 0;
  bottom: 0;
  z-index: 11;
  width: 100%;
  height: 110rpx;
  background: #fff;
}
.iwasButonTxt{
  padding:0 20rpx;
  height: 110rpx;
  line-height: 110rpx;
  font-size: 24rpx;
}
.iwastip{
  width: 100%;
  position: absolute;
  left: 0;
  bottom: 0;
  background: #fff;
}
.iwastipscroll{
  width: 100%;
  height: 100%;
}

