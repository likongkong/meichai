				<view class="exhibboxconeve" style="margin-left:20rpx;" catchtap="jumpexhdetail" data-brandid='{{item.brandId}}' data-id='{{item.id}}'>
					<image class="exhibboxeveimg" mode='aspectFit' src="{{item.images}}"></image>
