							<block wx:if='{{item.status==1}}'>{{item.start}}报名</block>
							<view wx:elif='{{item.status==2&&item.isSign}}' style="color:#6aca6b;">已报名</view>
							<block wx:elif='{{item.status==2}}'>{{item.stop}}开奖</block>
							<block wx:elif='{{item.status==3}}'>已开奖</block>
						</view>
						<view wx:elif='{{pagetype == 11 || pagetype == 12}}' style="height:40rpx;line-height:40rpx;font-size:22rpx;color:#333;float:right;">
