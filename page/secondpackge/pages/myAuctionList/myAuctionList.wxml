<t_f title='{{c_title}}' arrow='{{c_arrow}}' backcolor='{{c_backcolor}}'></t_f>
<view style='width:100%;height:{{statusBarHeightMc}}px;'></view>


<view class="wrap">
  <view class="tabbarWrap">
    <block wx:for="{{tabbarAll}}" wx:key='key' data-index="{{index}}">
      <view class="{{index == currentNum?'active':''}}" data-ind="{{index}}" catchtap="changetabbar">{{item}}</view>
    </block>
  </view>
  <view class="contentWrap"> 
    <view class="card">
      <view class="cardLeft">
        <image src="https://cdn.51chaidan.com/images/toyShow3/brandGoodsRotation/20210109/20a758966d18f609bf1154f869a20168.png" mode="widthFix"></image>
      </view>
      <view class="cardRight">
        <text class="card_title">美拆x大美术馆 和你在一起系列盲盒-隐藏款宇宙男孩</text>
        <view class="card_label"> 
          <block wx:if="{{true}}">
            <view class="lableItem red">限次出价</view>
          </block>
          <block wx:else>
            <view class="lableItem blue">限次出价</view>
          </block>
        </view>
        <view class="card_user">
          <view class="card_user_left">
            <block wx:if="{{true}}">
              <image src="https://thirdwx.qlogo.cn/mmopen/vi_32/8nHwuWOmqwVwf9XD2VBiapX59YKnoOd6GgmZnDCj3Or0JzN5C0tiaK3lKH5wYiaEnWApGBdZX0CFbuWM1Cc0PcVkQ/132" mode="widthFix"></image>
              <text style="color:#A7A7A8;">用户昵称</text>
            </block>
            <block wx:else>
              <text style="color:#FF2742;margin-left:0;">当前￥1280</text>
            </block>
          </view>
          <view class="card_user_right">
            <block wx:if="{{true}}">
              <text style="color:#FF2742;">出价￥288</text>
            </block>
            <block wx:else>
              <text style="color:#A7A7A8;">112次出价</text>
            </block>
          </view>
        </view>
        <view class="card_sponsor">
          <view class="card_sponsor_left">
            <image src="https://thirdwx.qlogo.cn/mmopen/vi_32/8nHwuWOmqwVwf9XD2VBiapX59YKnoOd6GgmZnDCj3Or0JzN5C0tiaK3lKH5wYiaEnWApGBdZX0CFbuWM1Cc0PcVkQ/132" mode="widthFix"></image>
            <text>美拆发起</text>
          </view>
          <view class="card_sponsor_right">
            剩5分23秒
          </view>
        </view>
      </view>
    </view>
  </view>
</view>



<tga_c wx:if='{{tgabox}}' bind:run='clicktga' bind:tgnone='clicktganone' bind:userinfo='userInfoHandler'></tga_c>
<view wx:if='{{!signinlayer}}' style='position: fixed;top:{{statusBarHeightMc}}px;left: 0;height:{{windowHeight-53}}px;width: 100%;z-index: 30;'  catchtap='pullupsignin'></view>



