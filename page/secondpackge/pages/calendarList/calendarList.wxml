
<t_f title='{{c_title}}' arrow='{{c_arrow}}' backcolor='{{c_backcolor}}' is_share='{{is_share}}'></t_f>
<view style='width:100%;height:{{statusBarHeightMc}}px;'></view>

<view class="swiperWrap">
  <swiper>
    <swiper-item class="swiperItem">
      <view>
        <image class="swiperItemBanner" src="http://www.51chaidan.com/images/spread/yiFanShang/1595406724.jpg" mode="widthFix"></image>
        <view class="swiperItemBrandWrap">
          <view class="swiperItemBrandInfo">
            <image class="swiperItemBrandLogo" src="https://www.51chaidan.com/images/default/headphoto/32781.jpg" mode="widthFix"></image>
            <text class="swiperItemBrandName">没拆</text>
          </view>
          <view class="swiperItemBrandVote">5000票</view>
        </view>
      </view>
    </swiper-item>
  </swiper>
</view>
