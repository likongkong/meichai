@import "../../../../pages/signin/signin.wxss";
@import "../../../../common/newcoupon.wxss";
@import "../../../../common/tabbar.wxss";

page{
  background:#f4f5f9;
  padding-bottom: 230rpx;
}

.swiperWrap{
  width: 100%;
}
.swiperWrap swiper{
  height: 409.3rpx;
}
.swiperItem{
  width: 100%;
}
.swiperItem>view{
  width: 710rpx;
  margin: 10rpx 20rpx;
  position: relative;
  border-radius: 8rpx;
  overflow: hidden;
}
.swiperItemBanner{
  width: 100%;
  height: 399.3rpx;
  display: block;
} 
.swiperItemBrandWrap{
  width: 100%;
  height: 60rpx;
  position: absolute;
  left: 0;
  bottom: 0;
  background-color: rgba(0,0,0,.5);
  border-radius: 0 0 8rpx 8rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 0 15rpx;
  box-sizing: border-box;
}
.swiperItemBrandInfo{
  display: flex;
  align-items: center;
}
.swiperItemBrandLogo{
  width: 50rpx;
  height: 50rpx;
  border-radius: 8rpx;
  display: block;
}
.swiperItemBrandName{
  font-size: 28rpx;
  color: #fff;
}
.swiperItemBrandVote{
  font-size: 28rpx;
  color: #fff;
}

.content{
  padding: 20rpx 20rpx;
  box-sizing: border-box;
}

.elsearch{
  width: 100%;
  height:90rpx;
  background: #f5f6fa;
}
.elsearchch{
  width: 100%;
  height:100%;
  background: #fff;
  border-radius: 200rpx;
}
.elserarchimg{
  width: 50rpx;
  height: 50rpx;
  float: left;
  margin: 20rpx;
}
.elserbotom{
  width: 90rpx;
  height: 50rpx;
  line-height: 50rpx;
  text-align: center;
  border-radius: 40rpx;
  font-size: 24rpx;
  color: #fff;
  background: #50a3fd;
  float: right;
  margin: 20rpx;
}
.elinputbut{
  height: 100%;
  width: 480rpx;
  float: left;
  font-size: 24rpx;
}


.tabNav{
  width: 100%;
  position: relative;
}
.tabNav .active{
  color: #E94F57;
}
.tabNav text{
  padding: 25rpx 20rpx;
  font-size: 28rpx;
  color: #000;
  display: inline-block;
}
.tabNavssIcon{
  position: absolute;
  top: 0;
  right: 0;
  width: 100rpx;
  height: 100%;
  display: flex;
  justify-content: flex-end;
  align-items: center;
  font-size: 24rpx;
}
.tabNavssIcon image{
  width: 45rpx;
  height: auto;
}
/* 全部 */
.allCalendar{
  width: 100%;
}
.calendarItem{
  width: 345rpx;
  display: inline-block;
  position: relative;
  margin-right: 20rpx;
  margin-bottom: 30rpx;
  background: #fff;
  border-radius: 8rpx;
  overflow: hidden;
}
.calendarItem:nth-child(2n){
  margin-right: 0;
}
.singleCalendarRank{
  position: absolute;
  top: 15rpx;
  left: 15rpx;
  width: 60rpx;
  height: 60rpx;
  background: rgba(0,0,0,.5);
  text-align: center;
  line-height: 60rpx;
  font-size: 28rpx;
  font-weight: bold;
  color: #fff;
  border-radius: 6rpx;  
}
.calendarItemBanner{
  width: 100%;
  height: 194rpx;
  border-radius: 8rpx 8rpx 0 0;
  display: block;
}
.calendarItemInfo{
  width: 100%;
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 0 10rpx;
  box-sizing: border-box;
}
.calendarItemInfo>view{
  display: flex;
  align-items: center;
  padding: 15rpx 0;
}
.calendarItemInfo>view image{
  width: 35rpx;
  height: auto;
  margin-right: 6rpx;
}
.calendarItemInfo>view text{
  font-size: 24rpx;
  color: #000;
  display: inline-block;
}
.calendarItemBrand{
  background: rgba(0,0,0,.3);
  position: absolute;
  bottom: 0;
  left: 0;
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 4rpx 10rpx 4rpx 4rpx;
  /* border-top-left-radius: 8rpx;
  border-top-right-radius: 8rpx;
  border-bottom-right-radius: 8rpx; */
  width: 100%;
  box-sizing: border-box;
}
.brandLogoBgFFF{
  background-color: #fff;
  margin-right: 8rpx;
  border-radius: 8rpx;
}
.calendarItemBrand image{
  width: 40rpx;
  height: 40rpx;
  display: block;
  border-radius: 8rpx;
}
.calendarItemBrand text{
  width: 100%;
  font-size: 24rpx;
  color: #fff;
  display: inline-block;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
/* 排行榜 */
.rankingItem{
  width: 100%;
}
.rankNum{
  position: absolute;
  top: 15rpx;
  left: 15rpx;
  width: 60rpx;
  height: 60rpx;
  background: rgba(0,0,0,.5);
  text-align: center;
  line-height: 60rpx;
  font-size: 28rpx;
  font-weight: bold;
  color: #fff;
