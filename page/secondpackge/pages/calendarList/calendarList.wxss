}
.rankingOthersL{
  width: 120rpx;
  display: flex;
  justify-content: flex-start;
  align-items: center;
}
.rankingOthersL image{
  width: 40rpx;
  height: auto;
}
.rankingOthersL text{
  font-size: 28rpx;
}
.rankingOthersR{
  width: 580rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 20rpx 0;
}
.bottomLine{
  position: absolute;
  left: 0;
  right: 0;
  bottom: 0;
  height: 2rpx;
  background:#FBFBFB;
  width: 600rpx;
  margin: auto;
}
.rankingOthersRLogo{
  width: 60rpx;
  height: 60rpx;
  border-radius:8rpx;
  display: inline-block;
  background: #fff;
  vertical-align: top;
}
.rankingOthersRName{
  font-size: 24rpx;
  margin-left: 15rpx;
  max-width: 320rpx;
}
.rankingOthersRNum{
  display: flex;
  align-items: center;
}
.rankingOthersRNum image{
  width: 40rpx;
  height: auto;
  margin-right: 20rpx;
}
.rankingOthersRNum text{
  font-size: 24rpx;
}

.btn{
  width: 450rpx;
  height: 70rpx;
  border-radius: 70rpx;
