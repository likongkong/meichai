}
.rankingBrandName{
  font-size: 28rpx;
  color: #fff;
  overflow: hidden;
  text-overflow:ellipsis;
  white-space: nowrap;
}
.rankingBrandVote{
  font-size: 28rpx;
  color: #fff;
}
.rankingTwoThree{
  display: flex;
  justify-content: space-between;
