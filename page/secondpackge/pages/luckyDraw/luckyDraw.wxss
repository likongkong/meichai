@import "../../../../pages/signin/signin.wxss";
@import "../../../../common/newcoupon.wxss";
@import "../../../../common/tabbar.wxss";
@import "../../../../pages/activitydetailspage/activitydetailspage.wxss";


.textDecoration{
  text-decoration:line-through;
}
.timetextstyle{
  font-size: 26rpx;
  color: #fff;
}
.ticketsWrap{
  width: 100%;
  display: flex;
  align-items: center;
  justify-content: center;
  padding: 24rpx 0;
}
.ticketsItem{
  width: 216rpx;
  background: #fff;
  border-radius: 12rpx;
  margin: 0 10rpx;
  padding: 40rpx 0; 
}
.ticketsItem text{
  display: block;
  width: 100%;
  text-align: center;
}
.takepartWrap{
  padding: 0 0 10rpx 0;
  background: #fff;
  width: 100%;
}

.tabWrap{
  width: 100%;
  background: #fff;
  margin-top: 24rpx;
}
.tabBtnWrap {
  display: flex;
  justify-content: center;
  align-items: center;
}

.tabBtnWrap view {
  font-size: 30rpx;
  color: #101010;
  border:2rpx solid  #8A8A8A;
  width:180rpx;
  height: 50rpx;
  line-height: 50rpx;
  text-align: center;
  font-size: 24rpx;
}

.active1 {
  color: #fff !important;
  background: #8A8A8A;
}


.wxParse-img{
  width: 100%;
  display: block;
}


.progress{
  width: 100%;
  height: 20rpx;
  border-radius: 20rpx;
  background: #fff;
  margin: 0 auto;
}
.progressBox{
  display: block;
  height: 100%;
  background: #E94F57;
  position: relative;
  border-radius: 20rpx;
}
.progressBox>view{
  height: 20rpx;
  display: inline-block;
  line-height: 20rpx;
  font-size: 18rpx;
  border: 2rpx solid #E94F57;
  position: absolute;
  top: -40rpx;
  right: -36rpx;
  background: #fff;
  color: #E94F57;
  border-radius: 20rpx;
}
.progressBox>view text{
  display: block;
  position: relative;
  width: 100%;
  height: 100%;
  padding: 0 10rpx;
}
.progressBox>view view{
  width: 100%;
  height: 100%;
  position: relative;
  top: 0;
  left: 0;
  z-index: -1;
}
.progressBox>view view image{
  width: 40%;
  height: 20rpx;
  position: absolute;
  top:-4rpx;
  left: 50%;
  margin-left: -20%;
  display: block;
}
.clickButtonEve{
  height: 70rpx;
  width: 280rpx;
  border-radius: 70rpx;
  font-size: 24rpx;
  color: #fff;
  
}
.displayClearTextImg{
  width: 35rpx;
  height: 35rpx;
  position: absolute;
  top: 20rpx;
  right: -50rpx;
  z-index: 1;
}
