@import "../../../../pages/signin/signin.wxss";
@import "../../../../common/newcoupon.wxss";
@import "../../../../common/tabbar.wxss";

page{
  background:#f4f5f9;
  padding-bottom: 230rpx;
}

.vipTop{
  background: #fff;
  width: 100%;
  padding:15rpx 0 30rpx 0;
}
.vipTopDisplay{
  width: 660rpx;
  /* height: 320rpx; */
  border-radius: 20rpx;
  /* background:#E6CC8F; */
  background-image: linear-gradient(to bottom right, #FFFFDE , #FFBE3E);
  margin: auto;
  padding: 50rpx 40rpx 40rpx 40rpx;
  box-sizing: border-box;
}
.vipTopDisplayT{
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.vipUserWrap{
  display: flex;
  align-items: center;
}
.vipAvatar{
  width: 70rpx;
  height: auto;
  border-radius: 70rpx;
  margin-right: 15rpx;
}
.vipUserWrap text{
  display: block;
}
.vipUserWrap .name{
  max-width: 300rpx;
  font-size: 24rpx;
  margin-bottom: 8rpx;
  color: #C38925;
}
.vipUserWrap .vipType{
  font-size: 22rpx;
  color: #C38925;
}
.openVip{
  height: 50rpx;
  line-height: 50rpx;
  padding: 0 30rpx;
  display: inline-block;
  background: #fff;
  font-size: 28rpx;
  color: #C38925;
  border-radius: 50rpx;
}
.vipTopDisplayB{
  display: flex;
  justify-content: space-between;
  /* align-items: center; */
  margin-top: 60rpx;
}
.vipTopDisplayB text{
  display: block;
  font-size: 24rpx;
}
.vipTopDisplayB .giftTxt{
  color: #000000;
  margin-bottom: 10rpx;
  color: #C38925;
}
.vipTopDisplayB .gift{
  color: #B95B06;
}
.vipPrivilegeWrap{
  /* padding: 0 50rpx; */
}
.vipPrivilegeTab{
  height: 160rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
  background-color: #fff;
  padding: 0 50rpx;
  box-sizing: border-box;
  border-bottom: 4rpx solid #f4f5f9;
}
.vipPrivilegeAward{
  /* box-shadow: 0px 4rpx 12rpx 0px rgba(0, 0, 0, 0.4);
  border: 2rpx solid rgba(255, 255, 255, 100); */
  padding: 30rpx 75rpx 60rpx 75rpx;
  border-radius: 10rpx;
  background-color: #fff;
  box-sizing: border-box;
  margin-bottom: 20rpx;
  margin-top: 10rpx;
}
.vipPrivilegeAwardGroup{
  display: flex;
  justify-content: center;
  align-items: center;
  margin: 60rpx 0;
}
.vipPrivilegeAwardGroup>view{
  display: flex;
  justify-content: space-between;
  align-items: center;
  margin: 0 30rpx;
}
.vipPrivilegeAwardGroup view image{
  width: 70rpx;
  height: auto;
  margin-right: 25rpx;
  /* border-radius: 60rpx; */
}
.vipPrivilegeAwardGroup view text{
  font-size: 24rpx;
  color: #101010;
  display: block;
  margin: 6rpx 0;
}
.vipPrivilegeAward .title{
  font-size: 28rpx;
  color: #000;
  display: block;
  text-align: center;
}
.vipPrivilegeAwardImg{
  width: 280rpx;
  height: auto;
  border-radius: 10rpx;
  margin: 15rpx auto 35rpx auto; 
  display: block;
}
.vipPrivilegeAwardExplain{
  font-size: 24rpx;
  display: block;
  padding: 0 20rpx;
  color: #666666;
  margin-bottom: 20rpx;
}
.vipPrivilegeAwardExplain:last-child{
  margin-bottom: 0;
}
.vipEveTitle{
  color: #101010;
  font-size: 30rpx;
  padding:40rpx 50rpx 40rpx 50rpx;
}
