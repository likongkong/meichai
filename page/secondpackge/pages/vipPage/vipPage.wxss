.btn{
  width: 450rpx;
  height: 70rpx;
  border-radius: 70rpx;
  background-color: #E94F57;
  color: #fff;
  font-size: 30rpx;
  text-align: center;
  line-height: 70rpx;
  position: fixed;
  bottom: 130rpx;
  left: 50%;
  margin-left: -225rpx;
}


.bgstyle {
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  bottom: 0;
  right: 0;
  background: #000;
  opacity: 0.6;
}
