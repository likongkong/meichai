.watermark:after{
  content: '';
  height: 35rpx;
  width: 100rpx;
  position: absolute;
  background: url("https://cdn.51chaidan.com/images/mc.png") no-repeat;
  background-size: 100% 100%;
  top: 15rpx;
  left: 0;
  right: 0;
  margin: auto;
}
.boxShadowEve{
  box-shadow: 1px 2px 6px #999999;
}

.dsbfcontent::-webkit-scrollbar{
  width: 0;
  height: 0;
  color: transparent;
  }
.bbsheadcon::-webkit-scrollbar{
  width: 0;
  height: 0;
  color: transparent;
}
::-webkit-scrollbar {
  display: none;
}
.dsbfcontent::-webkit-scrollbar {
  display: none;
}
.bbsheadcon::-webkit-scrollbar {
  display: none;
}

.clickButton{
  margin:auto;
  border-radius: 70rpx;
  height:70rpx;
  text-align:center;
  line-height:70rpx;
  color: #fff;
  font-size: 24rpx;
  width: 600rpx;
  background: #fb595f;
}
.tgaboxtgawchbochlibutboxtwo{
  width: 100%;
  height: 100%;
  position: absolute; 
  top: 0;
  left: 0;
  z-index: 2;
  opacity: 0;
}
.wshNew{
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  z-index: 9999;  
  background: rgba(0,0,0,.5);
  display: flex;
  align-items: center;
  justify-content: center;
  flex-direction: row;
}
/*   单行  */
.specialellipsis{
  overflow:hidden;
  text-overflow:ellipsis;
  white-space:nowrap;  
}
/* 暂无更多数据 */
.noDataCom{
  height: 600rpx;
  width: 100%;
  position: relative;
}
.noDataImg{
  width: 280rpx;
  height: 280rpx;
  position: absolute;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  margin: auto; 
}
.shareButton{
  width:100%;height:100%;position:absolute;top:0;left:0;opacity:0;z-index: 1;
}
/*  双行  */
.multilineellipsis{
  display: -webkit-box;
  word-break: break-all;
  text-overflow: ellipsis;
  overflow: hidden;
  -webkit-box-orient: vertical;
  -webkit-line-clamp:2;   
}