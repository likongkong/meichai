  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 

.vipBG{
  width: 100%;
  height: 100%;
  position: absolute !important;
  top: 0;
  left: 0;
  border-radius: 100%;
  transform: scale(1.5);
}
.vipBG image{
  width: 100%;
  height: 100%;
}
