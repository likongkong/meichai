/**citysel.wxss**/
.citypicker{
  position: fixed;
  bottom: 0;
  left: 0;
  width:100%;
  height:300px;
  background: #fff;
  z-index: 999;
}
.citybody {
  width: 100%;
  height: 100%;
}
.cityheader {
  position: absolute;
  top:0px;
  width: 100%;
  z-index: 5;
}
.city-cancel {
  float: left;
  margin: 20rpx;
  color: #828282;
}
.city-true {
  float: right;
  margin: 20rpx;
  color: #36c225;
}
.section .picker {
  background-color: #fff;
  border-bottom: 2px #cccccc solid;
  border-top: 2px #d9d9d9 solid;
  padding: 20rpx;
}
.citybackg{
  background: #000;
  opacity: 0.3;
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  z-index: 18;
}