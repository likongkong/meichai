.topfbox{
  width: 100%;
  position: fixed;
  top: 0;
  left: 0;
  z-index: 101;
}
.topftitletxt{
  width: 100%;
  position: absolute;
  left: 6rpx;
  bottom: 14rpx;
  height: 60rpx;
  line-height: 60rpx;
  text-align: center;
  font-size: 33rpx;
  color: #fff;
}
.left-icon {
  position: absolute;
  bottom: 7px;
  width: 150rpx;
  height: 56rpx;
  left: 20rpx;
  z-index: 3;
  border: 1rpx solid #999898;
  border-radius: 50rpx;
  overflow: hidden;
}
.topnavigator {
  position: absolute;
  top: 0;
  bottom: 0;
  width: 25rpx;
  height: 25rpx;
  float: left;
  margin: auto 0;
  /* background: #ff6968; */
  padding:10rpx 10rpx 10rpx 34rpx;
  z-index: 1;
}
.topnavigator:after {
  content: '';
  display: block;
  width: 20rpx;
  height: 20rpx;
  border-right: 4rpx solid #fff;
  border-top: 4rpx solid #fff;
  -webkit-transform: rotate(225deg);
  /*箭头方向可以自由切换角度*/
  transform: rotate(225deg);
}
.topcontentline{
  width: 2rpx;
  height: 36rpx;
  background: #ccc;
  position: absolute;
  top: 0;
  left: 71rpx;
  bottom: 0;
  margin: auto 0;
  z-index: 1;
}
.toprightimg{
  position: absolute;
  top: 0;
  bottom: 0;
  right: 0;
  padding:8rpx 22rpx 8rpx 8rpx;
  margin: auto 0;
  width: 36rpx;
  height: 36rpx;
  z-index: 1;
}
.topleftbackground{
  width: 100%;
  height: 100%;
  background: #000000;
  opacity: 0.1;
  border-radius: 50rpx;
}
.tabshopimg{
  position:absolute;
  bottom:34rpx;
  right:0;
  height:40rpx;
  width:40rpx;
  border:2rpx solid #ff2742;
  border-radius:50%;
  z-index:1;
  font-size:20rpx;
  color:#fff;
  text-align:center;
  line-height:40rpx;
  background:#ff2742;
}
/*   单行  */
.specialellipsis{
  overflow:hidden;
  text-overflow:ellipsis;
  white-space:nowrap;  
}
.hpimg{
  width: 30rpx;
  height: 30rpx;
  display: inline-block;
  vertical-align: top;
}