            <view class='scractrecacteve' wx:for='{{actrecactlist}}' wx:key='key' catchtap='actexempfun' data-gid="{{item.id}}" wx:if='{{item.status!=3 && index<=3}}' style="margin-bottom:10rpx;width:320rpx;height:257rpx;{{index%2==1?'margin-right:0;':''}}">
                <image class='actrecimg' mode="aspectFit" src='{{item.cover}}' style="width:320rpx;height:171rpx;"></image>
                <view class='bctitle multilineellipsis' style='font-size:22rpx;padding:0;width:100%;height:76rpx;'>
                  <block wx:if='{{item.status==1||item.status==0}}'>报名时间 {{item.start_time}}</block>
                  <block wx:if='{{item.status==3}}'>已开奖</block>
                  <block wx:elif='{{item.is_limit==1}}'>人满开奖 {{item.truth_users}}/{{item.limit_users}}</block>
                  <block wx:elif='{{item.status==2}}'>{{item.stop_time}} 自动开奖</block>
                  {{item.goods_name}}
                </view>
              </view>

          </view>
          <view class="clickButton" catchtap="acetlistfun">查看更多免单商品</view>
      </block>
  </view>
</view>


