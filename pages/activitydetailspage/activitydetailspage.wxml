                <text wx:if='{{rLUserLotto.ratioName == "高"}}' style="color:#F93E45;">高</text>
                <text wx:elif='{{rLUserLotto.ratioName == "中"}}' style="color:#CD8336;">中</text>
                <text wx:elif='{{rLUserLotto.ratioName == "低"}}' style="color:#58995C;">低</text>
