  <view style='width:100%;height:820rpx;background:#fff;position:absolute;bottom:0;border-radius:16rpx 16rpx 0 0;'>
      <block wx:if='{{commoddata.is_receive == 1}}'>
          <view class="winningTips">恭喜！中奖了</view>
