.afoHead{
  width: 100%;
  padding-top: 15rpx;
  height: 100rpx;
  margin: 20rpx auto 40rpx;
  border-radius: 10rpx;
  background: #fff;
  display: flex;
  justify-content: space-around;
}
.bdaiBoxImg{
  width:34rpx;
  height: 34rpx;
  margin:0 auto 10rpx;
  border-radius: 50%;
  border: 2rpx solid #B9B9B9;
  color: #B9B9B9;
  font-size: 24rpx;
  text-align: center;
  line-height: 34rpx;
  position: relative;
}
.bdaiBoxImg.sel{
  border: 2rpx solid #101010;
  color: #101010;
}
.bdaiBoxTxt{
  width: 100%;
  height: 40rpx;
  line-height: 40rpx;
  font-size:22rpx;
  text-align: center;
  color: #B9B9B9;
}
.bdaiBoxTxt.sel{
  color: #101010;
}
.markingLine{
  height: 2rpx;
  width: 100rpx;
  position: absolute;
  left: 60rpx;
  top: 0;
  bottom: 0;
  margin: auto;
  background:#B9B9B9;
}
.speedOfProgress{
  height: 100%;
  background: #101010;
}
.aFoCenter{
  width: 710rpx;
  margin: auto;
  background:#fff;
  border-radius: 15rpx;
}
.aFoCenterEve{
  width: 100%;
  border-bottom:2rpx solid #EEEDED;
}
.cEveTitle{
  font-size: 24rpx;
  padding: 20rpx 0 10rpx 20rpx;
}
.cEveInput{
  padding: 0 0 15rpx 34rpx;
  font-size: 24rpx;
  color: #777777;
}