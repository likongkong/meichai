<!--  html页面转换  -->
<import src="../../wxParse/wxParse.wxml" />

<t_f title='{{c_title}}' arrow='{{c_arrow}}' backcolor='{{c_backcolor}}' is_share='{{is_share}}'></t_f>
<view style='width:100%;height:{{statusBarHeightMc}}px;'></view>

<view style='width:0;height:0;overflow:hidden;position:absolute;top:-1000px;'>
  <canvas canvas-id='snapshot' style='width:300px; height:240px;'></canvas>
</view>

<view style='width:310px;height:410px;overflow:hidden;position:absolute;top:-2000px;'>
  <canvas canvas-id='sharefriend' style='width:310px; height:410px;'></canvas>
</view>

<!-- 保存图片  -->
<view class='tgimgbox' wx:if='{{tgfrShareIftr}}'>
  <view class='tgimgboxgb' catchtap='tgfrShareIftrFun'></view>
  <view class='tgimgbcon' style='width:310px;height:410px;top:-230rpx;'>
    <image class='tgimcimg' src='{{actimgshare}}' style='width:310px;height:410px;'></image>
  </view>
  <view class='tgfrindShare'>
    <view class='tgfrShareLeft' wx:if='{{isShareFun}}'>
      <view class='tgftShLText' style='background:#3f9314;color:#e8f7f0;'>
        <button open-type="share" style='position: absolute;left: 0;top: 0;width: 100%; height: 100%;opacity: 0;padding:0;'  type="default" size="mini"></button>
        分享好友
      </view>
    </view>
    <view class='tgfrShareright'>
      <view class='tgftShLText' style='background:#f18f00;color:#fceeef;' catchtap='sharesavethepicture'>保存海报</view>
    </view>
  </view>
</view>
<view wx:if='{{shareFriendMask}}' class='shareFriendMask'></view>


<view class='boxdetail {{tipback?"tripList_root":""}}' style="{{!headhidden||newcoupon?'position:fixed;top:0;left:0;right:0;bottom:0;':''}}">
  <view class='dlhead' style='height:500rpx;position:relative;'>
    <!-- 下标 -->
    <view class="detailswipernum">
      <view class="detailswinbg"></view>
      <view class="detailswnu">
        <block wx:if='{{isVideoSwiper}}'>
          <block wx:if='{{zunmdata.firstCover}}'>{{detailSwiperindex+1}}/{{movies.length+2}}</block>
          <block wx:else>{{detailSwiperindex+1}}/{{movies.length+1}}</block>
        </block>
        <block wx:else>
          <block wx:if='{{zunmdata.firstCover}}'>{{detailSwiperindex+1}}/{{movies.length+1}}</block>
          <block wx:else>{{detailSwiperindex+1}}/{{movies.length}}</block>
        </block>
      </view>
    </view>

    <!-- 带视频 -->
    <swiper bindchange='swiperchangeindex' bindanimationfinish="changeGoodsSwip" current="{{goodsIndex}}" wx:if='{{isVideoSwiper}}' class="swiper" indicator-color="#fff" indicator-active-color="#ff2742" indicator-dots="{{false}}" circular="true" autoplay="{{isVideoSwiper?false:true}}"
      interval="15000" duration="450" style='height:500rpx;'>
      <swiper-item wx:if='{{isVideoSwiper}}'>
        <view style="width:100%;height:100%;">
          <image class="slide-image" src="{{movies[0].url}}" style="width:100%;height:100%;display:block;" mode='aspectFit'></image>
          <image style='width:70rpx;height:70rpx;position:absolute;top:0;bottom:0;left:0;right:0;margin:auto;z-index:1;' src='{{isVideoSwiper?"https://www.51chaidan.com/images/icon/iconPlayVideo.png":""}}' wx:if='{{!ishowvideo}}' bindtap='previewVideo'> </image>
          <video muted='{{true}}' wx:if='{{ishowvideo}}' id="myVideo" show-fullscreen-btn="{{true}}" enable-progress-gesture="{{false}}" autoplay='{{false}}' src='{{video}}' style='width:100%;height:100%;position:absolute;left:0;top:0;right:0;bottom:0;margin:auto;'></video>
        </view>
      </swiper-item>
      <swiper-item wx:if='{{zunmdata.firstCover}}'>
        <view class="slide-image" style="background:url({{zunmdata.firstCover}});background-repeat: no-repeat;background-size:100% 100%;width:720rpx;height:384rpx;position:absolute;top:0;bottom:0;right:0;left:0;margin:auto;" data-index='{{index}}' bindtap='previewImg'></view>
      </swiper-item>
      <block wx:for="{{movies}}" wx:key="key">
        <swiper-item>
          <view class="slide-image" wx:if='{{item.isVedio==1}}' style='position:relative;width:100%;height:100%;'>
            <video muted='{{true}}' show-fullscreen-btn="{{true}}" enable-progress-gesture="{{false}}" autoplay='{{false}}' src='{{item.url}}' style='width:100%;height:100%;position:absolute;left:0;top:0;right:0;bottom:0;margin:auto;'></video>
          </view>
          <image class="slide-image" wx:else src='{{item.url}}' style="height:100%;width:100%;display:block;" data-index='{{index}}' bindtap='previewImg' mode='aspectFit'></image>
        </swiper-item>
      </block>
    </swiper>
    <!-- 不带视频 -->
    <swiper bindchange='swiperchangeindex' bindanimationfinish="changeGoodsSwip" wx:else class="swiper" indicator-color="#fff" indicator-active-color="#ff2742" indicator-dots="{{false}}" circular="true" autoplay="true" interval="4000" duration="450" style='height:500rpx;'>
      <swiper-item wx:if='{{zunmdata.firstCover}}'>
        <image src="{{zunmdata.firstCover}}" style="width:100%;height:100%;position:absolute;top:0;bottom:0;right:0;left:0;margin:auto;" mode='aspectFit' data-index='{{index}}' bindtap='previewImg' mode='aspectFit'></image>
      </swiper-item>
      <block wx:for="{{movies}}" wx:key="key">
        <swiper-item>
          <view class="slide-image" wx:if='{{item.isVedio==1}}' style='position:relative;width:100%;height:100%;'>
            <video muted='{{true}}' show-fullscreen-btn="{{true}}" enable-progress-gesture="{{false}}" autoplay='{{false}}' src='{{item.url}}' style='width:100%;height:100%;position:absolute;left:0;top:0;right:0;bottom:0;margin:auto;'></video>
          </view>
          <image class="slide-image" wx:else src='{{item.url}}' style="height:100%;width:100%;display:block;" data-index='{{index}}' bindtap='previewImg' mode='aspectFit'></image>
        </swiper-item>
      </block>
    </swiper>
  </view>

  <!-- 定金 -->
  <view class="detailDeposit" wx:if='{{zunmdata.goods_type==3&&zunmdata.status!=3}}'>
      <view class="deDeposleft">
          <view class="deDeposst">定金 ￥{{zunmdata.promote_price}}</view>
          <view class="deDepPresale">预售价 ￥{{zunmdata.gsale}}</view>
      </view>
      <view class="deDeposright">
          <view wx:if='{{zunmdata.finalDepositTime}}' class="specialellipsis" style="width:100%;height:110rpx;text-align:center;line-height:110rpx;font-size:24rpx;color:#EA4F57;">
              {{zunmdata.finalDepositTime}}         
          </view>          
          <block wx:elif='{{zunmdata.status==2}}'>
            <view class="deDepossttime" style="padding: 18rpx 0 10rpx 0;">距离预定结束:</view>
            <view class="deDepossttime"><block wx:if='{{winningovertimedetail.dayStr}}'>{{winningovertimedetail.dayStr}}天</block>  {{winningovertimedetail.hrStr}}:{{winningovertimedetail.minStr}}:{{winningovertimedetail.secStr}}</view>          
          </block>
          <view wx:else style="width:100%;height:110rpx;text-align:center;line-height:110rpx;font-size:24rpx;color:#EA4F57;">
              <block wx:if='{{zunmdata.status==3}}'>预定结束</block>
              <block wx:elif='{{zunmdata.status==1}}'>{{zunmdata.datatimew}}开售</block>  
              <block wx:elif='{{zunmdata.status==0}}'>已售罄</block>         
          </view>

      </view>
  </view>

  <block wx:if='{{is_exhibition==1||zunmdata.status==1}}'>
      <view style="width:100%;height:60rpx;background:#000;" wx:if='{{zunmdata.status!=0&&zunmdata.status!=3}}'>
        <view style="height:60rpx;float:left;line-height:60rpx;padding-left:20rpx;font-size:24rpx;color:#fff;">
          <block wx:if='{{zunmdata.status==2&&winningovertimedetail}}'>距离活动结束<block wx:if='{{winningovertimedetail.dayStr}}'><text style='font-size:30rpx;'>{{winningovertimedetail.dayStr}}</text>天</block><text style='font-size:30rpx;'>{{winningovertimedetail.hrStr}}</text>时<text style='font-size:30rpx;'>{{winningovertimedetail.minStr}}</text>分<text style='font-size:30rpx;'>{{winningovertimedetail.secStr}}</text>秒
          </block>
          <block wx:elif='{{zunmdata.status==1}}'>
             <block wx:if='{{newdataexh}}'>
                 暂未开启
             </block>
             <block wx:else>
             距离抢购开启<block wx:if='{{winningovertimedetail.dayStr}}'><text style='font-size:30rpx;'>{{winningovertimedetail.dayStr}}</text>天</block><text style='font-size:30rpx;'>{{winningovertimedetail.hrStr}}</text>时<text style='font-size:30rpx;'>{{winningovertimedetail.minStr}}</text>分<text style='font-size:30rpx;'>{{winningovertimedetail.secStr}}</text>秒
             </block>
          </block>
          <block wx:elif='{{zunmdata.status==3}}'>已结束</block>
          <block wx:elif='{{zunmdata.status==0}}'>已售罄</block>
        </view>
        <view style="height:40rpx;width:100rpx;background:#f08080;color:#fff;text-align:center;line-height:40rpx;float:right;margin:10rpx 20rpx 0 0;font-size:22rpx;border-radius: 10rpx;" wx:if='{{zunmdata.goodsLimit}}'>{{zunmdata.goodsLimit}}</view>
      </view>
      <block wx:if='{{is_exhibition==1}}'>
          <view style="width:100%;overflow:hidden;background:#fff;">
            <view class='dltitle' style='width:540rpx;font-size:32rpx;padding:20rpx 20rpx 20rpx 20rpx;float:left;'>
              {{zunmdata.gname}}
            </view>
            <view class='dPrice' style="width:170rpx;float:left;padding-top:20rpx;">
              <text style='font-size:30rpx;color:#000;font-weight:bold;color:#ff2742;'><text style='font-size:30rpx;'>￥</text>{{zunmdata.gsale}}</text>
            </view>
          </view>      
      </block>
      <block wx:else>
          <view class='dltitle' style='text-align:center;padding:20rpx 40rpx;font-size:32rpx;'>
            {{zunmdata.gname}}
          </view>
          <view class='dPrice'>
            <!-- 3D图片显示 -->
            <image wx:if='{{zunmdata.isOverAllView}}' src='https://cdn.51chaidan.com//images/sign/panorama.png' catchtap="jumpDSIE" class="detailS3Dimg"></image>
            <text wx:if='{{zunmdata.is_suit==1}}' style='color:#ff2742;font-size:24rpx;margin-right:10rpx;'>{{zunmdata.suitName}}<text style='font-size:30rpx;'>{{zunmdata.unpackUnit}}</text></text>
            <text style='font-size:30rpx;color:#000;font-weight:bold;'><text style='font-size:30rpx;'>￥</text>{{zunmdata.gsale}}</text>
            <text wx:if='{{zunmdata.gsale!=zunmdata.gprice}}' style='color: #959595;font-size: 24rpx;text-decoration:line-through;position: relative;top:3rpx;margin-left: 10rpx;'>￥{{zunmdata.gprice}}</text>
          </view>
          <view class='dltityh' style='text-align:center;margin-bottom:10rpx;padding:12rpx 20rpx;position:relative;min-height:36rpx;'>
            <view class='dlyhnnimeMod' wx:for="{{zunmdata.gtag}}" wx:key="key">{{item}}</view>
          </view>      
      </block>
  
  </block>
  <block wx:else>
      <view class='dltitle' style='text-align:center;padding:20rpx 40rpx;font-size:32rpx;'>
        {{zunmdata.gname}}
      </view>
      <view class='dPrice'>
        <!-- 3D图片显示 -->
        <image wx:if='{{zunmdata.isOverAllView}}' src='https://cdn.51chaidan.com//images/sign/panorama.png' catchtap="jumpDSIE" class="detailS3Dimg"></image>
        <text wx:if='{{zunmdata.is_suit==1}}' style='color:#ff2742;font-size:24rpx;margin-right:10rpx;'>{{zunmdata.suitName}}<text style='font-size:30rpx;'>{{zunmdata.unpackUnit}}</text></text>
        <text style='font-size:30rpx;color:#000;font-weight:bold;'><text style='font-size:30rpx;'>￥</text>{{zunmdata.gsale}}</text>
        <text wx:if='{{zunmdata.gsale!=zunmdata.gprice}}' style='color: #959595;font-size: 24rpx;text-decoration:line-through;position: relative;top:3rpx;margin-left: 10rpx;'>￥{{zunmdata.gprice}}</text>
      </view>
      <view class='dltityh' style='text-align:center;margin-bottom:10rpx;padding:12rpx 20rpx;position:relative;min-height:36rpx;'>
        <view class='dlyhnnimeMod' wx:for="{{zunmdata.gtag}}" wx:key="key">{{item}}</view>
      </view>  
  </block>

  <!-- 定金 -->
  <view class="dDepositconcent" wx:if='{{zunmdata.goods_type==3}}'>
     <view class="dDepositcontop">
         <view class="dDepositcl">尾款</view>
         <view class="dDepositcltxt" style="color:#ff2742;padding:18rpx 0 5rpx 0;">￥{{zunmdata.end_price}}</view>
         <view class="dDepositcltxt specialellipsis">{{zunmdata.finalPaymentTime}}</view>
     </view>
     <view class="dDepositcontop">
         <view class="dDepositcl">流程</view>
         <view class="dDeposibottxt">1.付定金-2.付尾款-3.发货<view class="dDepositxtright" catchtap="depositboxfun">预售规则</view></view>
     </view>
  </view>

  <view class='winningthepbox' wx:if='{{isAwardOrder}}' catchtap='jumporder'>
    <view class='winningthepimgtxt'>
      <image class='winningthepimgshop' src='{{awardOrder.Info.cover}}'></image>
      <view class='winningtheptime specialellipsis'>{{winningovertime}}</view>
    </view>
    <image class='winningthepimg' src='http://www.51chaidan.com/images/FreeSheet/WinningThePrize.png'></image>
  </view>

  <view wx:elif='{{zunmdata.couponValue && zunmdata.couponValue!=0 && welvalue}}' style='width:100%;margin-bottom: 10rpx;background:white;overflow:hidden;'>
    <view style='text-align:right;color:#C7C7C7;font-size:20rpx;margin:10rpx;'>已经有{{zunmdata.countShare}}人分享了该商品</view>
    <view style='height:80rpx;background:#FFDD82;border-radius:10rpx;margin:0 10rpx 20rpx 10rpx;overflow:hidden;'>
      <view style='width:120rpx;text-align:center;height:40rpx;line-height:40rpx;font-size:22rpx;background:#000;color:white;opacity:0.7;border-radius:20rpx;margin:20rpx 10rpx;float:left;'>拆单福利</view>
      <view style='line-height:40rpx;margin:20rpx 0;float:left;font-size:24rpx;text-align:center;'>分享朋友一起拆单各领
        <text style='color:red;font-size:30rpx;'>￥{{zunmdata.couponValue}}</text>立减金</view>
      <view style='width:30rpx;height:30rpx;float:left;padding:25rpx 13rpx;' catchtap='showwel'>
          <view style='width:30rpx;height:30rpx;border-radius:20rpx;background:#000;color:white;opacity:0.7;font-size:20rpx;line-height:30rpx;'>?</view>
      </view>
      <view style='margin:20rpx 10rpx;float:right;display:block;'>
          <view style='color:#9D5316;font-size:24rpx;line-height:40rpx;text-align:center;position:relative;'>
            <button wx:if='{{isShareFun}}' style='width:100rpx;height:40rpx;opacity:0;position:absolute;top:0;left:0;' open-type="share"></button>
            <view wx:else catchtap='displaysharefriend' style='width:100rpx;height:40rpx;opacity:0;position:absolute;top:0;left:0;'></view>
            去分享 >
          </view>
      </view>
    </view>
  </view>

  <view wx:if='{{ishowwelfare}}' style='width:500rpx;height:300rpx;border:1rpx solid #e5e5e5;background:white;position:absolute;left:0;right:0;top:400rpx;margin:0 auto;border-radius:10rpx;z-index:1000rpx;'>
    <view style='font-size:33rpx;margin:20rpx;'>拆单福利</view>
    <view style='font-size:27rpx;margin:0 20rpx;'>购买后分享商品链接给好友，好友下单成功各得
      <text style='color:red;'>{{zunmdata.couponCondition}}-{{zunmdata.couponValue}}元</text>立减金，快去分享吧！</view>
      <view style='width:50rpx;height:50rpx;position:absolute;right:10rpx;top:10rpx;' bindtap='closewelfare'>
          <image style='width:35rpx;height:35rpx;display:block;' src='https://www.51chaidan.com/images/icon/iconClose.png'></image>
      </view>
  </view>


  <block wx:if='{{is_exhibition==1||(is_exhibition!=1&&brandId>0)}}'>
      <view style="width:100%;height:80rpx;">
        <view style="width:50%;height:100%;{{!exhibdetail?'font-size:26rpx;font-weight: bold;':'font-size:24rpx;'}}text-align:center;line-height:80rpx;float:left;position:relative;" data-ind='2' catchtap="exhibdetailfun">
        <view style='width:2rpx;height:50rpx;float:left;background:#BBB9B9;margin-top:15rpx;'></view>
        商品详情
        <view wx:if='{{!exhibdetail}}' style='width:100rpx;height:6rpx;background:#000;position:absolute;left:0;right:0;bottom:0;margin:auto;'></view>
        </view>
        <view style="width:50%;height:100%;{{exhibdetail?'font-size:26rpx;font-weight: bold;':'font-size:24rpx;'}}text-align:center;line-height:80rpx;float:left;position:relative;" data-ind='1' catchtap="exhibdetailfun">
        品牌专区
        <view wx:if='{{exhibdetail}}' style='width:100rpx;height:6rpx;background:#000;position:absolute;left:0;right:0;bottom:0;margin:auto;'></view>
        </view>


      </view>  

  </block>
  <block wx:else>
     <somke colortitle='#000'></somke>
  </block>
  <block>
      <view class='dltittheme'  id='exh2'>
        <image class='dttemeimg' src='../images/rad_left.png'></image>
        <view class='dlttemelef'>
          商品详情
        </view>
      </view>
      <!--  商品详情图片 转换HTML标签  -->
      <view class='detailimg' style='padding:0;{{detaposiiftr&&dryinglist.length!=0?"height:1300rpx;":""}}{{!detaposiiftr&&dryinglist.length!=0?"padding-bottom:100rpx;":""}}'>
        <template is="wxParse" data="{{wxParseData:article.nodes}}" />
        <view class='detaposiclick' wx:if='{{dryinglist.length!=0}}'>
          <image src='https://clw.51chaidan.com//images/goods/GradualChangee.png' class='detaposiclickimg'></image>
          <view class='detaposiclicktxt' catchtap='detaposiiftrfun'>
            <block wx:if='{{detaposiiftr}}'>展开 ↓</block>
            <block wx:else>收起 ↑</block>
          </view>
        </view>
      </view>
      <view class='dltittheme' wx:if='{{dryinglist.length!=0}}'>
        <image class='dttemeimg' src='../images/rad_left.png'></image>
        <view class='dlttemelef'>
          用户晒单
          <block wx:if='{{drydisnum!=0}}'>({{drydisnum}})</block>
        </view>
      </view>
      <view style='width:100%;background:#fff;' wx:if='{{dryinglist.length!=0}}'>
        <view wx:for='{{dryinglist}}' wx:key='key' style='width:100%;'>
          <view class='detdlhead'>
            <image class='detdlheadimg' data-drying_id='{{item.id}}' catchtap='jumpdlfdetail' src='{{item.litpic}}'>
            </image>
            <view class='detdlheadtxt specialellipsis' data-drying_id='{{item.id}}' catchtap='jumpdlfdetail'>{{item.nick}}</view>
            <image class='detdlheadimglast' wx:if='{{item.is_authentication==1}}' src='https://clw.51chaidan.com/images/goods/authentication1.png'></image>
            <image class='detsieimg' wx:if='{{item.is_praise==0}}' data-lid='{{item.id}}' data-is_praise='{{item.is_praise}}' catchtap='ispraisefun' data-ind='{{index}}' src='https://www.51chaidan.com/images/brand/black_heart.png?'>
            </image>
            <image class='detsieimg' wx:if='{{item.is_praise==1}}' data-lid='{{item.id}}' data-is_praise='{{item.is_praise}}' catchtap='ispraisefun' data-ind='{{index}}' src='https://www.51chaidan.com/images/brand/red_heart.png?'>
            </image>
            <view class='detdaltxtnum' data-drying_id='{{item.id}}' catchtap='jumpdlfdetail'>{{item.praise_sum>0?item.praise_sum:''}}</view>
          </view>
          <view wx:if='{{item.introduce}}' style='padding:15rpx 20rpx;color:#000;font-size:24rpx;position: relative;' data-drying_id='{{item.id}}' catchtap='jumpdlfdetail'>{{item.introduce}}
          </view>
          <scroll-view scroll-x="true" class="disanum_tab_comtainer" style='height:160rpx;background:#fff;'>
            <view class='detdlscrollbox' wx:if='{{item.video_img}}' data-drying_id='{{item.id}}' catchtap='jumpdlfdetail'>
              <image class='detdlscrimg' src='{{item.video_img}}'></image>
              <image class='detdlvideclick' src='https://www.51chaidan.com/images/icon/iconPlayVideo.png'></image>
            </view>
            <view class='detdlscrollbox' wx:for='{{item.img_extend}}' wx:key='key' wx:for-item='w' data-drying_id='{{item.id}}' catchtap='jumpdlfdetail'>
              <image class='detdlscrimg' src='{{w.img}}'></image>
            </view>
          </scroll-view>
        </view>
        <view wx:if='{{dryinglist.length>=3}}' style='width:100%;height:80rpx;line-height:80rpx;color:#000;font-size:26rpx;text-align:center;background:#f4f5f9;position:relative;' catchtap='delpostdatamore'>{{loadprompttxt}}</view>
      </view>
      <view class='dltittheme' wx:if='{{allcomlist.length!=0}}'>
        <image class='dttemeimg' src='../images/rad_left.png'></image>
        <view class='dlttemelef'>热门评论</view>
      </view>
      <!--  评论数据  -->
      <view class='pinglunbox' wx:if='{{allcomlist.length!=0}}'>
        <view class='allcom' wx:for='{{allcomlist}}' wx:if='{{index<2}}' wx:key='key' data-cid='{{item.cid}}' data-uid='{{item.uid}}'>
          <view class='allcomhead'>
            <view class='allcomhleft'>
              <image class='achlimg' src='{{item.headphoto}}'></image>
              <view class='allcomname'>
                <view class='allcomnametit specialellipsis'>
                  {{item.nick}}
                  <image wx:if='{{item.gender==0}}' class='allcmmtitn' src='../images/allcom_wman.png'></image>
                  <image wx:if='{{item.gender==1}}' class='allcmmtitn' src='../images/allcom_man.png'></image>
                </view>
                <view class='loutime'>{{item.rank}}楼 {{item.time}}</view>
              </view>
            </view>
            <view class='allcomhright'>
              <view class='allcomripoi'>
