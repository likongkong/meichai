.bgstyle {
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  bottom: 0;
  right: 0;
  background: #000;
  opacity: 0.6;
}

.rotate {
  animation: zhuan 1s linear infinite;
}

@keyframes zhuan {
  0% {
    transform: translate(0, 0);
  }

  25% {
    transform: translate(0, -13px);
  }

  50% {
    transform: translate(0, 0);
  }

  70% {
    transform: translate(0, -5px);
  }

  100% {
    transform: translate(0, 0);
  }
}

.change {
  animation: changeanima 2s linear 1;
}

@keyframes changeanima {
  0% {
    transform: translate(0, 0) scale(1, 1);
    -ms-transform:  translate(0, 0) scale(1, 1);
    -moz-transform: translate(0, 0) scale(1, 1);
    -webkit-transform: translate(0, 0) scale(1, 1);
    -o-transform:translate(0, 0) scale(1, 1);
  }

  60% {
    transform: translate(-300px, 0) scale(0, 0);
    -ms-transform:  translate(-300px, 0) scale(0, 0);
    -moz-transform:translate(-300px, 0) scale(0, 0);
    -webkit-transform: translate(-300px, 0) scale(0, 0);
    -o-transform: translate(-300px, 0) scale(0, 0);
  }

  61% {
    transform: translate(0, -700px) scale(0, 0);
    -ms-transform: translate(0, -700px) scale(0, 0);
    -moz-transform:translate(0, -700px) scale(0, 0);
    -webkit-transform: translate(0, -700px) scale(0, 0);
    -o-transform: translate(0, -700px) scale(0, 0);
  }

  100% {
    transform: translate(0, 0) scale(1, 1);
    -ms-transform:  translate(0, 0) scale(1, 1);
    -moz-transform: translate(0, 0) scale(1, 1);
    -webkit-transform: translate(0, 0) scale(1, 1);
    -o-transform: translate(0, 0) scale(1, 1);
  }
}

.changebox {
  animation: boxanima 1s linear 1;
}

@keyframes boxanima {
  0% {
    transform: translate(400px, 0) scale(1, 1);
  }

  100% {
    transform: translate(0, 0) scale(1, 1);
  }
}

