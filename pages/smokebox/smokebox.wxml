<t_f title='{{c_title}}' arrow='{{c_arrow}}' backcolor='{{c_backcolor}}' is_share='{{is_share}}'></t_f>

<view style='width:100%;height:{{statusBarHeightMc}}px;'></view>

<!-- 全局父view -->
<view class="smokeboxBox" style='background:{{pageBg}};{{iftrdetailpagetwo?"height:80%;position:fixed;top:0;left:0;right:0;bottom:0;overflow:hidden;":""}}'>

	<view wx:if='{{!isallready}}' class="isallreadyCss" style="background:{{pageBg}};"></view>

	<view style='width:{{750-(multipleHide.length*105)}}rpx;height:{{multipleHide.length>0?hideheight+80:""}}rpx;overflow:hidden;'>

		<scroll-view wx:if='{{queueList.length>0}}' scroll-x="true" class="queueListScroll" scroll-with-animation="true">
			<view style='display:inline-block;' wx:for="{{queueList}}" wx:key="key">
				<view class="topScrollBox">
					<image class="topScrollBoxImg" src='{{item.headphoto}}'></image>
					<block wx:if="{{item.isVip}}">
						  <view class='vipBG'><image src='https://cdn.51chaidan.com/images/calendar/VipCase.png' style='position: relative;'></image></view>
					</block>
				</view>
			</view>
		</scroll-view>
 
		<view wx:else class="topScrollBoxEl"></view>

		<view class="smokeBoxSelection">
			<block wx:if='{{employList.length>0}}'>{{employList.length}}人正在选盒</block>
			<block wx:elif='{{activity.aheadUser>0}}'>您前面有{{activity.aheadUser}}人排队</block>
		</view>

		<block wx:if='{{multipleHide.length>0}}'>
				<view class="multipleHideParent" catchtap='iftrdetailpagen'>
					<view wx:for="{{multipleHide}}" wx:key='key' class='smokeBoxHideBox'>
							<swiper autoplay="{{true}}" circular="true" interval="3000" class="smokeBoxHideBoxSwiper">
								<block wx:for="{{item.infoHide}}" wx:key='key'>
									<swiper-item>
										<view class='smokeBoxHideSView'>
											<image mode="heightFix" class="smokeBoxHideBoxImg" bindload='imageLoadhide' src='{{zdyurl + item.img}}'></image>
										</view> 
									</swiper-item>
								</block>
							</swiper>
							<view class="smokeBoxHideOne"></view>
							<view class="smokeBoxHideTwo">
								<view>隐藏概率</view>
								<view>1/{{item.displayTotal}}</view>
							</view>
					</view>
				</view>
	  </block>

    <block wx:elif='{{goods_thumb}}'>
				<view class="smokeBoxCenterBoxParent" catchtap='iftrdetailpagen'>
					<view class="smokeBoxCenterBox">
						<view class="smokeBoxCenterBoxChilbox" style='width:{{hidewidth}}rpx;height:{{hideheight}}rpx;'>
							<image style='width:{{hidewidth}}rpx;display:block;' bindload='imageLoadhide' mode="widthFix" src='{{goods_thumb}}'></image>
						</view>
						<view style='width:{{hidewidth}}rpx;height:10rpx;'></view>
						<view class="smokeBoxCenterNoClass"></view>
						<view class="smokeBoxCenterBotClass">
							<view>商品详情</view>
						</view>
					</view>
				</view>
		</block>


	</view>

<view class="topShareBox" style="top:{{hideheight+100}}rpx;"><image class="topShareBoxImg" src="https://cdn.51chaidan.com/images/spread/blindBoxShare.png"></image><button class="shareButton" open-type='share'></button>分享</view>

  <block wx:if='{{whole_boxGift}}'>
			<view class="wholeBoxGiftBox" style="top:{{hideheight+160}}rpx;" catchtap="collectchip">
				<view style="position:relative;">
					<image class="whole_boxGiftImg" mode="widthFix" src='{{wholeBoxGiftImg}}'></image>
				</view>
				<view class="whole_boxGiftButVie"><text>端盒送</text></view>
			</view>
	</block>

	<block wx:if='{{welfare.length > 0}}'>
		<view catchtap="jumpRedList" data-sourcepage="0" data-ind="{{welfare[0].welfareType == 2?'1':'2'}}" class="smokeBoxWelfareBox" style='top:{{hideheight+310}}rpx;'>
			<block wx:if="{{welfare[0].welfareType == 2}}">
				<view class="tabshopimg specialellipsis" style="top:-8rpx;left:56rpx;" wx:if='{{welfare&&welfare.length!=0&&welfare.length!=1}}'>{{welfare.length}}</view>
				<image src='http://www.51chaidan.com/images/blindBox/doublePackage.png' class="welfareImgDoub"></image>
				<view class="welfareNumBox"><text>幸运值\n</text>{{welfare[0].limitAmount-welfare[0].currentAmount}}点可领</view>
			</block>
			<block wx:elif="{{welfare[0].welfareType == 3}}">
				<view class="tabshopimg specialellipsis" style="top:-8rpx;left:56rpx;" wx:if='{{welfare&&welfare.length!=0&&welfare.length!=1}}'>{{welfare.length}}</view>
				<image src='http://www.51chaidan.com/images/blindBox/doublePackage.png' class="welfareImgDoub"></image>
				<view class="welfareNumBox"><text>抽盒金\n</text>红包</view>
			</block>
		</view>
	</block>


  
	<!--  显示品牌信息  -->
	<block wx:if='{{branddata}}'>
		<view class="brandInformation" catchtap='gobrandDetails' data-mid='{{userbranddata.id}}'>
				<image src='{{branddata.logo}}' class="brandInformationImg"></image>
				<view class="brandInformationTxt" class='specialellipsis'>{{branddata.name}}旗舰店</view>	
		</view>
	</block>


	<view class="brandInformationName">{{activity.name}}</view>

	<block wx:if='{{(welfareTags && welfareTags.length!=0) || activity.isRecycle}}'>
			<view class="boxBene" style="{{welfareTags.length>=4?'':'text-align: center;'}}">
					<view wx:if="{{activity.vip_special_offer == 1}}" class="vipSpecialOffer" catchtap="jumpVipPage">VIP返抽盒金x{{activity.vip_discount}}</view>
					<view wx:if="{{activity.isRecycle}}" class="vipSpecialOfferRecy" catchtap="toggleRecyclerule">{{activity.recycleRatio}}折回收</view>
					<view class="boxBenefits" wx:for='{{welfareTags}}' wx:key='key' catchtap="boxBenefitsFun" data-name='{{item}}'>{{item}}</view>
			</view>
	</block>

	

	<view style='width:100%;position:relative;'>

		<block wx:if='{{isallready && idlist.length>0}}'>
			<view class='arrowParentStyle' style='left:23rpx;' catchtap='chancebox'>
				<image wx:if='{{idlist.length>0}}' src='https://www.51chaidan.com/images/icon/iconLeftArrow.png' class='arrowImgStyle'></image>
			</view>
		</block>

    <block wx:if='{{isallready && idlist.length>0}}'>
				<view class='arrowParentStyle' style='right:23rpx;' catchtap='chancebox'>
					<image wx:if='{{idlist.length>0}}' src='https://www.51chaidan.com/images/icon/iconRightArrow.png' class='arrowImgStyle'></image>
				</view>
		</block>


		<view class='{{ischangebox?"changebox":""}}'>
			<image class="smokeBoxCTopCover" style='width:{{imgwidth+4}}rpx;' catchtap='iftrdetailpagen' mode="widthFix" src='{{activity.cover}}'></image>
  
			<view id='boxindex' class='parentBoxStyle' style="position:relative;width:{{imgwidth}}rpx;">
				<view wx:for='{{roleList}}' wx:key="key">
					<block wx:if='{{!item.isSoldOut}}'>
							<view data-n='{{activity.radioImgBox}}' style='width:{{boxwidth}}px;height:{{boxwidth * activity.radioImgBox}}px;' catchtap='{{activity.canOperate?"selectbox":""}}' class="roleListBoxIsSoldOut">
								<image style='width:100%;height:100%;' src='{{imgTopArr&&imgTopArr.length!=0?imgTopArr[index%(imgTopArr.length)].boxCover:activity.imgTop}}'></image>
								<block wx:if='{{item.headphoto!=""}}'>
									  <image style="width:{{boxwidth * activity.radioImgBox * 0.5}}px;height:{{boxwidth * activity.radioImgBox * 0.5}}px;" src="{{item.headphoto}}" class="roleListBoxIsImg"></image>
								</block>
								<block wx:if='{{!activity.isInQueue}}'>
									  <view class="smokeBoxisInQueue" catchtap="{{activity.status == 1?'blindBoxsubscrfun':'lineup'}}"></view>
								</block>
							</view>
					</block>
					<block wx:if='{{item.isSoldOut}}'>
							<view class="smokeBoxisInQuOut" style='width:{{boxwidth}}px;height:{{boxwidth * activity.radioImgBox}}px;' catchtap='{{activity.isOpenNewBox?"againOpenBox":""}}'>
								<image class="smokeBoxisInQuOutImg" mode="widthFix" src='{{activity.imgSoldOut}}'></image>
							</view>
					</block>


				</view>

				<view wx:if='{{activity.isOpenNewBox}}' class="againOpenBox" catchtap="againOpenBox">
					再开一盒
				</view>
			</view>

		</view>

	</view>

	<view wx:if='{{isallready && ishowhalf}}' class="smokeBoxishowhalf">
		<view class="smokeBoxishowhalfOne">

			<view class="smokeBoxishowhalfThree">
				<view style="width:150rpx;overflow:hidden;" catchtap='iftrdetailpagen'>
					<image class="smokeBoxishowhalfImg" src="https://www.51chaidan.com/images/turntable/Magnifier.png"></image>
					<view class="smokeBoxishowhalfTxt">商品预览</view>
				</view>
			</view>
			<view wx:if="{{suplusTry>0}}" catchtap='showrule' class="smokeBoxishowhalfRule">抽盒规则</view>

			<view wx:if='{{suplusTry>0}}' class='tryStyle'>
				<view style="width:150rpx;" data-isretry='0' catchtap='havetry'>试一试({{suplusTry}}次)</view>
			</view>

		</view>

		<block wx:if='{{activity.status == 1}}'>
				<view class="liveBroadcastStatus">
					<view class="liveBroadcastStatusOne" style="border-radius: 80rpx;overflow: hidden;"> 
						<view class='queuebtnstyle2' style="width:100%;" catchtap="blindBoxsubscrfun" class="liveBroadcastStatusThree">
							<view class="liveBroadcastStatusTwo">{{activity.isLiveShow?activity.start_time+'直播开售':'开售提醒'}}<view class="coupposi" wx:if='{{isSubscribeCoupon}}'>订阅送优惠券</view></view>
							<view style="text-align:center;font-size:28rpx">
								<!-- <block wx:if='{{newdataexh}}'>暂未开售</block>
								<block wx:else>{{activity.start_time}}</block> -->
								点击订阅
							</view>
						</view>
					</view>
				</view>
		</block>

		<block wx:elif='{{activity.status == 2}}'>
				<view class="changeABox">

					<view class="changeABoxOne">
						<view wx:if='{{idlist.length>0}}' class='queuebtnstyle2' style="background:white;float:left;" catchtap='chancebox'>换一盒</view>
						<view wx:else class='queuebtnstyle2' style="background:#B0B0B0;color:white;float:left;">换一盒</view>

					</view>
					<view class="changeABoxTwo">

						<view wx:if='{{whole_boxGift}}' class='queuebtnstyle2 changeABoxTwoOne' style=""  catchtap="collectchip">端盒送</view>

					
						<view wx:elif='{{isOpenWholeBox && activity.status == 2}}' class='queuebtnstyle2 changeABoxTwoOne' catchtap="{{(isDirectOpenWholeBox&&!isDeduct) || (isDirectOpenWholeBox&&isDeduct&&activity.userBlindBoxAmount==0) ? 'wholebox' : 'collectchip'}}">
								<block wx:if='{{activity.wholeBoxTitle}}'>{{activity.wholeBoxTitle}}</block>
								<block wx:elif='{{tipCheapForWholeBox>0||activity.wholeBoxSpecialTitle}}'>{{activity.wholeBoxSpecialTitle||'端盒立减'}}</block>
								<block wx:else>抽整盒</block>
						</view>

						<view wx:elif='{{activity.status==1}}' class='queuebtnstyle2' style="background:{{isAboveQuota?'#ccc':'#e94f57'}};color:white;float:left;" catchtap='{{isAboveQuota?"":"lineup"}}'>

								<block wx:if='{{limitBuy && limitBuy != 0}}'>
									<view class="isAboveQuotaImmediately">{{isAboveQuota?"今日已抽完":"立即排队"}}</view>
									<view class="isAboveQuotaImmediatelyNum">({{limitBuy}}个/人/天)</view>						
								</block>
								<block wx:else>立即排队</block>							

						</view>

						<view wx:elif='{{activity.aheadUser== 0 && activity.isInQueue && activity.canOperate }}' class='queuebtnstyle2' style="background:{{isAboveQuota?'#ccc':'#e94f57'}};color:white;float:left;" catchtap='{{isAboveQuota?"":"selectbox"}}'>

								<block wx:if='{{limitBuy && limitBuy != 0}}'>
									<view class="isAboveQuotaImmediately">{{isAboveQuota?"今日已抽完":"直接选盒"}}</view>
									<view class="isAboveQuotaImmediatelyNum">({{limitBuy}}个/人/天)</view>						
								</block>
								<block wx:else>直接选盒</block>						

						</view>

						<view wx:elif='{{activity.isInQueue}}' class='queuebtnstyle2' style="background:{{isAboveQuota?'#ccc':'#e94f57'}};color:white;float:left;">

							<block wx:if='{{limitBuy && limitBuy != 0}}'>
								<view class="isAboveQuotaImmediately">{{isAboveQuota?"今日已抽完":"排队中"}}</view>
								<view class="isAboveQuotaImmediatelyNum">({{limitBuy}}个/人/天)</view>						
							</block>
							<block wx:else>排队中</block>



						</view>

						<view wx:else class='queuebtnstyle2' style="background:{{isAboveQuota?'#ccc':'#e94f57'}};color:white;float:left;" catchtap='{{isAboveQuota?"":"lineup"}}'>
							<block wx:if='{{limitBuy && limitBuy != 0}}'>
								<view class="isAboveQuotaImmediately">{{isAboveQuota?"今日已抽完":"立即排队"}}</view>
								<view class="isAboveQuotaImmediatelyNum">({{limitBuy}}个/人/天)</view>						
							</block>
							<block wx:else>立即排队</block>
						</view>

					</view>
				</view>

		</block>

    <block wx:elif='{{activity.status == 3}}'>
				<view class="activityStatusThree">
					<view style="width:70%;height:80rpx;position:relative;margin:0 auto;">
						<block wx:if='{{activity.supplyStockTip}}'>
								<view class='queuebtnstyle2' catchtap="blindBoxsubscrfun" style="background:#eeae19;color:white;float:left;line-height:80rpx;height:80rpx;">补货提醒</view>				
						</block>
						<block wx:else>
								<view class='queuebtnstyle2' style="background:#ccc;color:white;float:left;line-height:80rpx;height:80rpx;">已结束</view>	
						</block>
					</view>
				</view>
		</block>

    <block wx:elif='{{activity.status == 4}}'>
			<view class="activityStatusThree">
					<view class="soldOutTips">商品已售罄</view>
					<view style="width:70%;height:80rpx;position:relative;margin:0 auto;">
							<view class='queuebtnstyle2' catchtap="{{activity.isSubscribe?'':'blindBoxsubscrfun'}}" style="{{activity.isSubscribe?'background:#E6E3E3;color:#484848;':'background:#eeae19;color:white;'}}line-height:80rpx;height:80rpx;width:100%;font-size:28rpx;">
									<block wx:if='{{activity.isSubscribe}}'>已设置到货提醒</block>
									<block wx:else>我想要</block>
							</view>	
					</view>
			</view>
		</block>

    <block wx:if='{{toyCabinetList && toyCabinetList.length != 0}}'>
				<view>
						<view class="singleDisplay">单款展示</view>
						<scroll-view scroll-x="true" class="singleDisplay_scroll">

								<view class="sD_s_eve" wx:for='{{toyCabinetList}}' wx:key='key' catchtap='jumpshopbut' data-name='{{item.role_name}}' data-minprice='{{item.minPrice}}' data-maxprice='{{item.maxPrice}}' data-goods_id='{{item.goods_id}}'>
										<image class="sD_s_eve_img" style="height:auto;" mode='widthFix' src="{{item.goodsImg}}"></image>
										<view class="sD_s_eve_tit specialellipsis" catchtap='jumpshopbut' data-name='{{item.role_name}}' data-minprice='{{item.minPrice}}' data-maxprice='{{item.maxPrice}}' data-goods_id='{{item.goods_id}}'>{{item.role_name}}</view>
										<view class="sD_s_eve_tit specialellipsis">
											<block wx:if='{{item.minPrice==0&&item.maxPrice==0}}'></block>
											<block wx:else>闲置:<block wx:if='{{item.minPrice!=item.maxPrice}}'>{{item.minPrice}}~{{item.maxPrice}}</block><block wx:else>{{item.maxPrice}}</block>元</block>
										</view>

								</view>

						</scroll-view>			 
				
				</view>
		</block>




		<somke wx:if='{{is_exhibition == 0&&brandId<=0}}' colortitle='#000'></somke>

		<block wx:elif='{{is_exhibition==1||(is_exhibition!=1&&brandId>0)}}'>
				<view class="exhibitionBox">
					<view class="exhibitionBoxOne">
						<view class="exhibitionBoxThree" style="{{exhibdetail?'font-size:26rpx;font-weight: bold;':'font-size:24rpx;'}}" data-ind='1' catchtap="exhibdetailfun">
							品牌专区
							<view wx:if='{{exhibdetail}}' class="exhibitionBoxFive"></view>
						</view>
						<view class="exhibitionBoxThree" style="{{!exhibdetail?'font-size:26rpx;font-weight: bold;':'font-size:24rpx;'}}" data-ind='2' catchtap="exhibdetailfun">
							<view class="exhibitionBoxDetail"></view>
							商品详情
							<view wx:if='{{!exhibdetail}}' class="exhibitionBoxFive"></view>
						</view>
					</view>

					<!-- 品牌专区 -->
					<view id='exh1'>
						<!-- 展会 -->
						<view class="BrandZoneBox">
							<view class="BrandZoneBoxOne">
								<image class="exhibdeimg" mode='aspectFit' src="{{userbranddata.logo}}" catchtap='gobrandDetails' data-mid='{{userbranddata.id}}'></image>
								<view class="exhibdetxt">
									<view class="exhibdxtxttitle specialellipsis" catchtap='gobrandDetails' data-mid='{{userbranddata.id}}'>{{userbranddata.brandName}}</view>
									<view class="exhibdxtxtadd" style="background:#31b3fb;" catchtap="addfrindcommonifun" data-url='{{userbranddata.picPublic}}' data-name='{{userbranddata.brandName}}'>关注公众号</view>
									<view class="exhibdxtxtadd" style="background:#6aca6b;margin-right:15rpx;" catchtap="addfrindcommonifun" data-url='{{userbranddata.picFriend}}' data-name='{{userbranddata.brandName}}'>添加好友</view>
									<view class="addUserBrandDesc" wx:if='{{userbranddata.bradDesc}}' catchtap='gobrandDetails' data-mid='{{userbranddata.id}}'>简介：{{userbranddata.bradDesc}}</view>
								</view>
							</view>

				<!-- 直播 -->
			<block wx:if='{{livebroadcast&&livebroadcast.length!=0}}'>
			
					<swiper class="livebroadcastBox" indicator-dots="{{false}}" circular="true" autoplay="true" interval="3000" duration="450">
						<swiper-item  wx:for='{{livebroadcast}}' wx:key='key' wx:for-item='w'>
								<view class="livebroadcastBoxOne">
									<navigator url="plugin-private://wx2b03c6e691cd7370/pages/live-player-plugin?room_id={{w.room_id}}"  class="livebroadcastBoxThree"></navigator>
									<view style="overflow:hidden;border-radius:10rpx;">
										<image class="livebroadcastBoxThImg" mode='aspectFit' src="{{w.images}}"></image>
										<view class="livebroadcastBoxFive">
											<view class="livebroadcastBoxFiveTxt">{{w.name}}</view>
											<view class="livebroadcastBoxSix">
												<view wx:if='{{w.status==102}}' class="livebroadcastBoxSixB">{{w.start_time}}开播</view>
												<view wx:elif='{{w.status==101}}' class="livebroadcastBoxSixS">直播中</view>
												<view wx:elif='{{w.status==103}}' class="livebroadcastBoxSixS">已结束</view>
												<view wx:else class="livebroadcastBoxSixS">尚未开播</view>
											</view>
										</view>
									</view>
								</view>
						</swiper-item>
					</swiper>

			</block>

							<block wx:if='{{idendataList&&idendataList.length!=0}}'>
									<view style="width:100%;">
											<view class="onLineBoxExtractor" catchtap="jumplist">在线抽盒机</view>

											<view style="width:100%;margin:5rpx 0 5rpx 0;">
												<scroll-view scroll-x="true" style="white-space:nowrap;text-align:left;">
													<view wx:for='{{idendataList}}' wx:key='key' wx:for-item='w' class="itemstyle">
														<view catchtap='bbevebox' data-gid='{{w.goodsId}}' class="onLineBoxExtractorView">
															<image class='disanum_simg' src='{{w.cover}}' style='height:177rpx;width:222rpx;'></image>
															<view class="specialellipsis" wx:if="{{w.tip!=''}}" class="disanum_simgBoxNew">{{w.tip}}</view>
														</view>
														<view class='specialellipsis onLineBoxExtractorOne'>{{w.name}}</view>
														<view style="width:100%;height:40rpx;color:#d64650;">
															<view class="onLineBoxExtractorPrice">
																<text style="font-size:20rpx;">￥</text>{{w.shop_price}}</view>
														</view>
													</view>
												</scroll-view>
											</view>
									</view>          
							</block>
							<view class="exhibboxcon">
								<view style="float:left;margin-left:20rpx;">
									<view class="mexhibboxconeve {{gid == 33115 ? 'cardmm' : ''}}" wx:for='{{exhdata}}' wx:key='key' wx:if='{{index%2==0}}' catchtap="jumpexhdetail" data-brandid='{{item.brandId}}' data-id='{{item.id}}' data-mtype="{{item.type}}" data-gid='{{item.goodsId}}'>
										<block wx:if='{{item.type == 4 || item.type == 11|| item.type == 12 || item.type == 1 || item.type == 5|| item.type == 8|| item.type == 9|| item.type == 10}}'>
											<image class="exhibboxeveimg" mode='widthFix' src="{{item.images}}"></image>
											<view wx:if='{{item.type != 11 && item.type != 12}}' class="mdismtbcctxthead multilineellipsis" style="font-size:26rpx;padding:5rpx 0;text-align:left;color:black;">{{item.name}}</view>
											<view wx:elif='{{item.type == 11 || item.type == 12}}' class="mdismtbcctxthead multilineellipsis" style="font-size:26rpx;padding:5rpx 0;text-align:left;color:black;">{{item.goodsName}}</view>
											<view wx:if='{{item.type == 12}}' class="mdismtbcctxthead multilineellipsis" style="font-size:26rpx;padding:5rpx 0;text-align:left;color:#666;">{{item.content}}</view>
											<view style="width:100%;padding:10rpx 0;overflow:hidden;">
												<view class="smokeBoxFreeActivity" wx:if='{{item.shopPrice && item.shopPrice!="" && item.type != 1}}'>￥{{item.shopPrice}}</view>
												<view wx:elif='{{item.type == 1}}' class="smokeBoxFreeActivity">免单活动</view>
												<view wx:if='{{item.type == 4 || item.type == 1}}'  class="smokeBoxFreeActNext">
													<view wx:if='{{item.saleButtonDisplay}}' style='color:#6aca6b;'>{{item.saleButtonDisplay}}</view>
													<block wx:elif='{{item.status==1}}'>{{item.start_time}}报名</block>
													<view wx:elif='{{item.status==2&&item.isSign}}' style="color:#6aca6b;">已报名</view>
													<block wx:elif='{{item.status==2}}'>
														<block wx:if='{{item.is_limit==1}}'>满{{item.limit_users}}人开奖</block>
														<block wx:else>{{item.stop_time}}开奖</block>										
													</block>
