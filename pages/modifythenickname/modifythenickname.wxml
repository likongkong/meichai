<view class="helpPopup" wx:if="{{ishelpPopup}}">
  <view class="helpPopupMask" catchtap="showhelpPopup"></view>
  <view class="helpPopupBox">
    <view class="helpPopupTitle">
      {{tips.share}}
    </view>
    <view class="helppeople">
      <block wx:for='{{share}}' wx:key='key'>
        <image src="{{item.headphoto}}" mode="widthFix"></image>
      </block>
    </view>
    <button>分享</button>
  </view>
</view>



<view class="punchBottom" catchtap="showhelpPopup">
    <!-- <view class="punchBottomtxt">已打卡</view> -->
    <view class="punchBotImgBox" style="width:{{50+share.length*15>530?'530':50+share.length*15}}rpx;" wx:if='{{share.length!=0}}'>
      <image class="punchBotImg" wx:for='{{share}}' wx:key='key' src="{{item.headphoto}}" style="z-index:{{index+1}};left:{{index*15}}rpx;" wx:if='{{index*15<480}}'></image>
    </view>
    <view class="punchBottomtxt" catchtap="showSharePopup">分享得南瓜</view>
</view>


<view class="helpBtn" wx:if="canAssist">
助力
</view>

