<view class="modals modals-bottom-dialog" hidden="{{ishelpPopup}}"  catchtouchmove="return">
    <view class="modals-cancel" bindtap="hideModal"  catchtouchmove="return"></view>
    <view class="bottom-dialog-body bottom-pos" animation="{{animationData}}">
      <view class="helpPopupTitle">
        <!-- 邀请好友送南瓜，老朋友送1个，新朋友送5个 -->
        <text>邀请好友送</text><image src="http://www.51chaidan.com/images/sign/2020wsj/ws_6.png" mode="widthFix"></image><text>, 老朋友</text><image src="http://www.51chaidan.com/images/sign/2020wsj/ws_6.png" mode="widthFix"></image><text>x1,</text><text> 新朋友</text><image src="http://www.51chaidan.com/images/sign/2020wsj/ws_6.png" mode="widthFix"></image><text>x5</text>
      </view>
      <view class="helpPopupContent">
        <scroll-view scroll-y style="height:460rpx;">
          <view class="helpPopupscrollWrap">
            <block wx:for='{{share}}' wx:key='key'>
              <view class="helpPopupscroll">
                <image class="popupimg" src="{{item.headphoto}}" mode="widthFix"></image>
                <view class="helpPopupBOX">
                  <image class="nanguaicon" src="http://www.51chaidan.com/images/sign/2020wsj/ws_6.png" mode="widthFix"></image>
                  <text>x{{item.score}}</text>
                </view>
              </view>
            </block>
          </view>
        </scroll-view>
      </view>
      <button open-type="share">分享</button>
    </view>
</view>

