<view class="wrap">
  <view class="countdownWrap">
    <image src="http://www.51chaidan.com/images/sign/2020wsj/ws_1.jpg" mode="widthFix"></image>
    <view class="countdownToday">
      <text>
        {{todaycountdown}}
      </text>
    </view>
  </view>
  <view class="contentWrap">
    <view class="treeWrap">
      <image class="tree" src="http://www.51chaidan.com/images/sign/2020wsj/ws_2.png" mode="widthFix"></image>
    </view>
    <view class="rankWrap">
      <view class="rankBoxWrap">
        <view class="dateTimeRank"> 
          <block wx:for="{{datetimeAll}}" wx:key="key">
            <text class="{{dateTime==index?'active':''}}" catchtap="tabBtnFun" data-dateTime="{{index}}">{{item}}</text>
          </block>
        </view>
        <block>
          <view class="rankBoxInWrap" wx:if="{{rank[dateTime].length!=0}}"> 
            <view class="titleIconWrap">
              <image class="titleIcon" src="http://www.51chaidan.com/images/sign/2020wsj/ws_3.png" mode="widthFix"></image>
            </view>
            <view class="selfPumpkinWrap"> 
              <view class="selfPumpkinNum">我的南瓜：<image src="http://www.51chaidan.com/images/sign/2020wsj/ws_6.png" mode="widthFix"></image> {{halloweenScore}} <text catchtap="showSharePopup">如何获得?</text> </view>
              <view class="refresh" catchtap="refresh">每5分钟自动刷新</view>
            </view>
            <view class="threePumpkin">
              <image class="threePumpkinIcon" src="http://www.51chaidan.com/images/sign/2020wsj/ws_4.png" mode="widthFix"></image>
            </view>
            <view class="topThreeWrap">
              <view>
                <image class="topThreeHeadphoto" src="{{rank[dateTime][1].headphoto}}" mode="aspectFill"></image>
                <text>{{rank[dateTime][1].nick}}</text>
                <view class="topThreeAward" catchtap="jumpOtherPage" data-num='1' data-whref='{{rank[dateTime][1].goodsId}}'>
                  <image src="{{rank[dateTime][1].img}}" mode="aspectFill"></image>
                </view>
              </view>
              <view class="first">
                <image class="topThreeHeadphoto" src="{{rank[dateTime][0].headphoto}}" mode="aspectFill"></image>
                <text>{{rank[dateTime][0].nick}}</text>
                <view class="topThreeAward" catchtap="jumpOtherPage" data-num='1' data-whref='{{rank[dateTime][0].goodsId}}'>
                  <image src="{{rank[dateTime][0].img}}" mode="aspectFill"></image>
                </view>
              </view>
              <view>
                <image class="topThreeHeadphoto" src="{{rank[dateTime][2].headphoto}}" mode="aspectFill"></image>
                <text>{{rank[dateTime][2].nick}}</text>
                <view class="topThreeAward" catchtap="jumpOtherPage" data-num='1' data-whref='{{rank[dateTime][2].goodsId}}'>
                  <image src="{{rank[dateTime][2].img}}" mode="aspectFill"></image>
                </view>
              </view>
            </view>
            <view class="smallPrizeWrap">
              <image class="smallPrizeTitleIcon" src="http://www.51chaidan.com/images/sign/2020wsj/ws_5.png?20201029" mode="widthFix"></image>
              <view class="smallPrizeRankWrap">
                <view wx:if="{{rank[dateTime].length < 4}}">
                  <view class="nodata">暂无榜单</view>
                </view>
                <view wx:else>
                  <block wx:for="{{rank[dateTime]}}" wx:key="key">
                    <view class="smallPrizeRank">
                      <view class="place">
                        <text style="text-align: center;">{{item.rank}}</text>
                      </view>
                      <view class="name">
                        <image class="marginR" src="{{item.headphoto}}" mode="aspectFill"></image>
                        <text>{{item.nick}}</text>
                      </view>
                      <view class="pumpkinNum">
                        <image class="marginR" src="http://www.51chaidan.com/images/sign/2020wsj/ws_6.png" mode="aspectFit"></image>
                        <text>x {{item.score}}</text>
                      </view>
                      <view class="award">
                        <view catchtap="jumpOtherPage" data-num='1' data-whref='{{item.goodsId}}'>
                          <image class="marginR" src="{{item.img}}" mode="aspectFill"></image>
                          <text>x {{item.num}}</text>
                        </view>
                      </view>
                    </view>
                  </block>
                </view>
              </view>
            </view>
          </view>
          <view class="rankBoxInWrap" wx:else>
            <view class="nodata">暂无榜单</view>
          </view>
        </block>
      </view>
    </view>
  </view>
  <view class="activityRrulesWrap">
    <image src="http://www.51chaidan.com/images/sign/2020wsj/ws_7.jpg?20201029" mode="widthFix"></image>
    <!-- <scroll-view scroll-y class=""> -->
      <text>
        {{tips.introduce}}
      </text>
    <!-- </scroll-view> -->
  </view>
</view>







