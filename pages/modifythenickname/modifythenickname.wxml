     <view class="sharePopupContent">
        <view catchtap="jumpOtherPage" data-num='988'>
          <text>去抽盒机</text>
          <image src="http://www.51chaidan.com/images/sign/2020wsj/tiaozhuan.png" mode="widthFix"></image>
        </view>
        <view catchtap="jumpOtherPage" data-num='9015'>
          <text>去一番赏</text>
          <image src="http://www.51chaidan.com/images/sign/2020wsj/tiaozhuan.png" mode="widthFix"></image>
        </view>
        <view catchtap="jumpOtherPage" data-num='998'>
          <text>去手办</text>
          <image src="http://www.51chaidan.com/images/sign/2020wsj/tiaozhuan.png" mode="widthFix"></image>
        </view>
     </view>
     <!-- <view class="invitefriend"> 
      <text>邀请朋友</text>
      <image src="http://www.51chaidan.com/images/sign/2020wsj/tiaozhuan.png" mode="widthFix"></image>
     </view> -->
    <button open-type="share" class="sharePopupshare">
      邀请朋友
      <image src="http://www.51chaidan.com/images/sign/2020wsj/tiaozhuan.png" mode="widthFix"></image>
    </button>
  </view>
</view>


