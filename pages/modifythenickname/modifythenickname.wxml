<t_f title='{{c_title}}' arrow='{{c_arrow}}' backcolor='{{c_backcolor}}'></t_f>
<view style='width:100%;height:{{statusBarHeightMc}}px;'></view>



<!-- 单张详情 -->
<block wx:if='{{isBrandDetail==1}}'>
   <view class="calendDtailPad">
      <view class="cDetBhead">
          <image src="https://cdn.51chaidan.com/images/spread/blindBox/a1606396898.png" mode="widthFix" class="cDetBheadImg"></image>
          <view class="cDetBhBrand">
             <image class="headBrandLogo" src="https://cdn.51chaidan.com/images/spread/blindBox/a1606396898.png"></image>
             <view class="headBrandName specialellipsis">美拆</view>
             <view class="headBrandName specialellipsis" style="float:right;max-width:145rpx;margin-right:15rpx;">5000票</view>
          </view>
      </view>   
   </view>
   <view class="calendDtailPad">
      <view class="ticketRanking">
          <view class="ticketRankingNum">1</view>
          <view class="ticketRankingBox">
            <image class="ticketRankingImg" src="https://cdn.51chaidan.com/images/spread/blindBox/a1606396898.png"></image>
            <view class="ticketRankingInd">已投5000票</view>
          </view>
      </view>
      <view class="ticketRanking">
          <view class="ticketRankingNum">2</view>
          <view class="ticketRankingBox">
            <image class="ticketRankingImg" src="https://cdn.51chaidan.com/images/spread/blindBox/a1606396898.png"></image>
            <view class="ticketRankingInd">已投5000票</view>
          </view>
      </view>

   </view> 
   <view class="leafletBottom">
       <view class="leafletBottomEve">分享日历</view>
       <view class="leafletBottomEve" style="margin-left:60rpx;">投票</view>
   </view>

</block>

<!-- 品牌详情 -->
<block wx:elif='{{isBrandDetail==2}}'>
    <view class="detailBrandHead">
        <image class="detailBImg" mode='aspectFit' src="https://cdn.51chaidan.com/images/202011/thumb_img/35310_thumb_G_1606739742263.png"></image>
        <view class="detailBName">
            <view class="detailBNTxt">大美术馆</view>
            <view class="detailBNTick">3.4w票</view>
        </view>
        <view class="detailBRTick">
            <view class="detailBRTNum">3</view>
            <view class="detailBRTrank">排名</view>
        </view>
    </view>

    <view class="calendDtailPad">
        <view class="cDetBhead">
            <image src="https://cdn.51chaidan.com/images/spread/blindBox/a1606396898.png" mode="widthFix" class="cDetBheadImg"></image>
            <view class="cDetBhBrand">

              <view class="headBrandName specialellipsis" style="margin-left:15rpx;">已投5325票</view>
              <view class="brandShareBox"><image class="brandShareImg" mode='aspectFit' src="http://www.51chaidan.com/images/sign/calendar/vote.png"></image>投票</view>
              <view class="brandShareBox"><image class="brandShareImg" mode='aspectFit' src="https://clw.51chaidan.com//images/goods/share3.png"></image>分享</view>

            </view>
        </view>   
    </view>

   <view class="calendDtailPad">
      <view class="ticketRanking">
          <view class="ticketRankingNum">2</view>
          <view class="ticketRankingBox">
            <image class="ticketRankingImg" src="https://cdn.51chaidan.com/images/spread/blindBox/a1606396898.png"></image>
            <view class="ticketRankingInd" style="float:left;margin:0 0 0 15rpx;">已投5000票</view>
            <view class="ticketRightNum">x2</view>
            <image class="ticketRightImg" mode='aspectFit' src="https://cdn.51chaidan.com/images/spread/blindBox/a1606396898.png"></image>
            
          </view>
      </view>
      <view class="ticketRanking">
          <view class="ticketRankingNum">2</view>
          <view class="ticketRankingBox">
            <image class="ticketRankingImg" src="https://cdn.51chaidan.com/images/spread/blindBox/a1606396898.png"></image>
            <view class="ticketRankingInd" style="float:left;margin:0 0 0 15rpx;">已投5000票</view>
            <view class="ticketRightNum">x2</view>
            <image class="ticketRightImg" mode='aspectFit' src="https://cdn.51chaidan.com/images/spread/blindBox/a1606396898.png"></image>
            
          </view>
      </view>

   </view> 


    <view class="leafletBottom">
        <view class="leafletBottomEve">我参与的</view>
    </view>


</block>

















































