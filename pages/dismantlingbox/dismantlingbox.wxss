/*   单行  */
.specialellipsis{
    overflow:hidden;
    text-overflow:ellipsis;
    white-space:nowrap;  
}
/*  双行  */
.multilineellipsis{
  display: -webkit-box;
  word-break: break-all;
  text-overflow: ellipsis;
  overflow: hidden;
  -webkit-box-orient: vertical;
  -webkit-line-clamp:2;   
}
.swiper {
  height: 213rpx;
  width: 100%;
}
.slide-image{
